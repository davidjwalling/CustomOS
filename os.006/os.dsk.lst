  1941 0000024D 7208                                            jb      irq0.10                                         ;no, skip ahead
  1942 0000024F FE0570040000                                    inc     byte [wbClockDays]                              ;increment clock days
  1943 00000255 31C0                                            xor     eax,eax                                         ;reset clock ticks
  1944 00000257 A36C040000              irq0.10                 mov     dword [wdClockTicks],eax                        ;save clock ticks
