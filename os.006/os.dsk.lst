  1364 00000092 B120                                            mov     cl,32                                           ;shift count
  1365 00000094 D2E8                                            shr     al,cl                                           ;try a 32-bit shift
  1366 00000096 08C0                                            or      al,al                                           ;did the shift happen?
  1367 00000098 7415                                            jz      .10                                             ;yes, cpu is 808x, v20, etc.
  1368                                                          cpu     186
  1369 0000009A 54                                              push    sp                                              ;save stack pointer
  1370 0000009B 59                                              pop     cx                                              ;...into cx
  1371 0000009C 39E1                                            cmp     cx,sp                                           ;did sp decrement before push?
  1372 0000009E 750F                                            jne     .10                                             ;yes, cpu is 80186
  1373                                                          cpu     286
