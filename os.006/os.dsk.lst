  1375 000000A1 0F0106[C200]                                    sgdt    [cbLoaderGDT]                                   ;store gdt reg in work area
  1376 000000A6 8A0E[C700]                                      mov     cl,[cbLoaderGDTHiByte]                          ;CL = hi-order byte
  1377 000000AA FEC1                                            inc     cl                                              ;was hi-byte of GDTR 0xff?
  1378 000000AC 7401                                            jz      .10                                             ;yes, cpu is 80286
  1379 000000AE 40                                              inc     ax                                              ;AX = 3
  1380 000000AF C3                      .10                     ret                                                     ;return
