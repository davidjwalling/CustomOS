  2352                                  ;	Routine:	PutConsoleHexByte
  2353                                  ;
  2354                                  ;	Description:	Write two ASCII characters to the console screen representing the value of a byte.
  2355                                  ;
  2356                                  ;	In:		al	byte value
  2357                                  ;			ch	row
  2358                                  ;			cl	column
  2359                                  ;			ds	OS data selector
  2360                                  ;			es	CGA selector
  2361                                  ;
  2362                                  ;-----------------------------------------------------------------------------------------------------------------------
  2363                                  PutConsoleHexByte	push	ebx						;save volatile regs
  2364                                  			mov	bl,al						;save byte value
  2365                                  			shr	al,4						;hi-order nybble
  2366                                  			or	al,030h						;apply ASCII zone
  2367                                  			cmp	al,03ah						;numeric?
  2368                                  			jb	.10						;yes, skip ahead
  2369                                  			add	al,7						;add ASCII offset for alpha
  2370                                  .10			call	SetConsoleChar					;display ASCII character
  2371                                  			inc	cl						;increment column
  2372                                  			mov	al,bl						;byte value
  2373                                  			and	al,0fh						;lo-order nybble
  2374                                  			or	al,30h						;apply ASCII zone
  2375                                  			cmp	al,03ah						;numeric?
  2376                                  			jb	.20						;yes, skip ahead
  2377                                  			add	al,7						;add ASCII offset for alpha
  2378                                  .20			call	SetConsoleChar					;display ASCII character
  2379                                  			pop	ebx						;restore volatile regs
  2380                                  			ret							;return to caller
