  1929                                  ;
  1930                                  ;	Exit from hardware interrupt
  1931                                  ;
  1932                                  ;-----------------------------------------------------------------------------------------------------------------------
  1933                                  hwwint			call	PutSecondaryEndOfInt				;send EOI to secondary PIC
  1934                                  			jmp	hwint90						;skip ahead
