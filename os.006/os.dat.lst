  2564                                  ;-----------------------------------------------------------------------------------------------------------------------
  2565                                  ;
  2566                                  ;	Routine:	PlaceCursor
  2567                                  ;
  2568                                  ;	Description:	Position the cursor on the console.
  2569                                  ;
  2570                                  ;	In:		DS	OS data selector
  2571                                  ;
  2572                                  ;-----------------------------------------------------------------------------------------------------------------------
  2573                                  PlaceCursor		push	ecx						;save volatile regs
  2574                                  			mov	al,[wbConsoleRow]				;al = row
  2575                                  			mov	ah,ECONCOLS					;ah = cols/row
  2576                                  			mul	ah						;row offset
  2577                                  			add	al,[wbConsoleColumn]				;add column
  2578                                  			adc	ah,0						;add overflow
  2579                                  			mov	ecx,eax						;screen offset
  2580                                  			mov	dl,ECRTPORTLO					;crt controller port lo
  2581                                  			mov	dh,ECRTPORTHI					;crt controller port hi
  2582                                  			mov	al,ECRTCURLOCHI					;crt cursor loc reg hi
  2583                                  			out	dx,al						;select register
  2584                                  			inc	edx						;data port
  2585                                  			mov	al,ch						;hi-order cursor loc
  2586                                  			out	dx,al						;store hi-order loc
  2587                                  			dec	edx						;register select port
  2588                                  			mov	al,ECRTCURLOCLO					;crt cursor loc reg lo
  2589                                  			out	dx,al						;select register
  2590                                  			inc	edx						;data port
  2591                                  			mov	al,cl						;lo-order cursor loc
  2592                                  			out	dx,al						;store lo-order loc
  2593                                  			pop	ecx						;restore volatile regs
  2594                                  			ret							;return to caller
  2595                                  ;-----------------------------------------------------------------------------------------------------------------------
  2596                                  ;
  2597                                  ;	Routine:	PutPrimaryEndOfInt
  2598                                  ;
  2599                                  ;	Description:	Send non-specific end-of-interrupt signal to the primary PIC.
  2600                                  ;
  2601                                  ;-----------------------------------------------------------------------------------------------------------------------
  2602                                  PutPrimaryEndOfInt	sti							;enable maskable interrupts
  2603                                  			mov	al,EPICEOI					;non-specific end-of-interrupt
  2604                                  			out	EPICPORTPRI,al					;send EOI to primary PIC
  2605                                  			ret							;return to caller
  2606                                  ;-----------------------------------------------------------------------------------------------------------------------
  2607                                  ;
  2608                                  ;	Routine:	PutSecondaryEndOfInt
  2609                                  ;
  2610                                  ;	Description:	Send non-specific end-of-interrupt signal to the secondary PIC.
  2611                                  ;
  2612                                  ;-----------------------------------------------------------------------------------------------------------------------
  2613                                  PutSecondaryEndOfInt	sti							;enable maskable interrupts
  2614                                  			mov	al,EPICEOI					;non-specific end-of-interrupt
  2615                                  			out	EPICPORTSEC,al					;send EOI to secondary PIC
  2616                                  			ret							;return to caller
  2617                                  ;-----------------------------------------------------------------------------------------------------------------------
  2618                                  ;
  2619                                  ;	Routine:	SetKeyboardLamps
  2620                                  ;
  2621                                  ;	Description:	Send the set/reset mode indicators command.
  2622                                  ;
  2623                                  ;	In:		bh	00000CNS (C:Caps Lock,N:Num Lock,S:Scroll Lock)
  2624                                  ;
  2625                                  ;-----------------------------------------------------------------------------------------------------------------------
  2626                                  SetKeyboardLamps	call	WaitForKeyInBuffer				;wait for input buffer ready
  2627                                  			mov	al,EKEYCMDLAMPS					;set/reset lamps command
  2628                                  			out	EKEYPORTDATA,al					;send command to 8042
  2629                                  			call	WaitForKeyOutBuffer				;wait for 8042 result
  2630                                  			in	al,EKEYPORTDATA					;read 8042 'ACK' (0fah)
  2631                                  			call	WaitForKeyInBuffer				;wait for input buffer ready
  2632                                  			mov	al,bh						;set/reset lamps value
  2633                                  			out	EKEYPORTDATA,al					;send lamps value
  2634                                  			call	WaitForKeyOutBuffer				;wait for 8042 result
  2635                                  			in	al,EKEYPORTDATA					;read 8042 'ACK' (0fah)
  2636                                  			ret							;return to caller
  2637                                  ;-----------------------------------------------------------------------------------------------------------------------
  2638                                  ;
  2639                                  ;	Routine:	WaitForKeyInBuffer
  2640                                  ;
  2641                                  ;	Description:	Wait for keyboard input buffer to be ready for input.
  2642                                  ;
  2643                                  ;	Out:		ZF	1 = Input buffer ready
  2644                                  ;				0 = Input buffer not ready after timeout
  2645                                  ;
  2646                                  ;-----------------------------------------------------------------------------------------------------------------------
  2647                                  WaitForKeyInBuffer	push	ecx						;save volatile regs
  2648                                  			mov	ecx,EKEYWAITLOOP				;keyboard controller timeout
  2649                                  .10			in	al,EKEYPORTSTAT					;keyboard status byte
  2650                                  			test	al,EKEYBITIN					;is input buffer still full?
  2651                                  			loopnz	.10						;yes, repeat till timeout
  2652                                  			pop	ecx						;restore volatile regs
  2653                                  			ret							;return to caller
  2654                                  ;-----------------------------------------------------------------------------------------------------------------------
  2655                                  ;
  2656                                  ;	Routine:	WaitForKeyOutBuffer
  2657                                  ;
  2658                                  ;	Description:	Wait for keyboard output buffer to have data to read.
  2659                                  ;
  2660                                  ;	Out:		ZF	0 = Output buffer has data from controller
  2661                                  ;				1 = Output buffer empty after timeout
  2662                                  ;
  2663                                  ;-----------------------------------------------------------------------------------------------------------------------
  2664                                  WaitForKeyOutBuffer	push	ecx						;save volatile regs
  2665                                  			mov	ecx,EKEYWAITLOOP				;keyboard controller timeout
  2666                                  .10			in	al,EKEYPORTSTAT					;keyboard status byte
  2667                                  			test	al,EKEYBITOUT					;output buffer status bit
  2668                                  			loopz	.10						;loop until output buffer bit
  2669                                  			pop	ecx						;restore volatile regs
  2670                                  			ret							;return to caller
  2671                                  ;-----------------------------------------------------------------------------------------------------------------------
  2672                                  ;
  2673                                  ;	End of the Kernel Function Library
  2674                                  ;
  2675                                  ;-----------------------------------------------------------------------------------------------------------------------
  2676                                  			times	8190-($-$$) db 0h				;zero fill to end of section
  2677                                  			db	055h,0AAh					;end of segment
  2678                                  ;-----------------------------------------------------------------------------------------------------------------------
  2679                                  ;
  2680                                  ;	Console Task
  2681                                  ;
  2682                                  ;	The only task defined in the kernel is the console task. This task consists of code, data, stack, and task state
  2683                                  ;	segments and a local descriptor table. The console task accepts and echos user keyboard input to the console
  2684                                  ;	screen and responds to user commands.
  2685                                  ;
  2686                                  ;-----------------------------------------------------------------------------------------------------------------------
  2687                                  ;-----------------------------------------------------------------------------------------------------------------------
  2688                                  ;
  2689                                  ;	Console Stack								@disk: 007400	@mem:  004000
  2690                                  ;
  2691                                  ;	This is the stack for the console task. It supports 448 nested calls.
  2692                                  ;
  2693                                  ;-----------------------------------------------------------------------------------------------------------------------
  2694                                  section			constack						;console task stack
  2695                                  			times	1792-($-$$) db 0h				;zero fill to end of section
  2696                                  ;-----------------------------------------------------------------------------------------------------------------------
  2697                                  ;
  2698                                  ;	Console Local Descriptor Table						@disk: 007B00	@mem:  004700
  2699                                  ;
  2700                                  ;	This is the LDT for the console task. It defines the stack, code, data and queue segments as well as data
  2701                                  ;	aliases for the TSS LDT. Data aliases allow inspection and altering of the TSS and LDT. This LDT can hold up to
  2702                                  ;	16 descriptors. Six are initially defined.
  2703                                  ;
  2704                                  ;-----------------------------------------------------------------------------------------------------------------------
  2705                                  section			conldt							;console local descriptors
  2706                                  			dq	004093004780007Fh				;04 TSS alias
  2707                                  			dq	004093004700007Fh				;0C LDT alias
  2708                                  			dq	00409300400006FFh				;14 stack
  2709                                  			dq	00C093000000FFFFh				;1C data
  2710                                  			dq	00409B0050000FFFh				;24 code
  2711                                  			dq	00409300480007FFh				;2C message queue
  2712                                  			times	128-($-$$) db 0h				;zero fill to end of section
  2713                                  ;-----------------------------------------------------------------------------------------------------------------------
  2714                                  ;
  2715                                  ;	Console Task State Segment						@disk: 007B80	@mem:  004780
  2716                                  ;
  2717                                  ;	This is the TSS for the console task. All rings share the same stack. DS and ES are set to the console data
  2718                                  ;	segment. CS to console code.
  2719                                  ;
  2720                                  ;-----------------------------------------------------------------------------------------------------------------------
  2721                                  section			contss							;console task state segment
  2722                                  			dd	0						;00 back-link tss
  2723                                  			dd	0700h						;04 esp ring 0
  2724                                  			dd	0014h						;08 ss ring 0
  2725                                  			dd	0700h						;0C esp ring 1
  2726                                  			dd	0014h						;10 es ring 1
  2727                                  			dd	0700h						;14 esp ring 2
  2728                                  			dd	0014h						;18 ss ring 2
  2729                                  			dd	0						;1C cr ring 3
  2730                                  			dd	0						;20 eip
  2731                                  			dd	0200h						;24 eflags
  2732                                  			dd	0						;28 eax
  2733                                  			dd	0						;2C ecx
  2734                                  			dd	0						;30 edx
  2735                                  			dd	0						;34 ebx
  2736                                  			dd	0700h						;38 esp ring 3
  2737                                  			dd	0						;3C ebp
  2738                                  			dd	0						;40 esi
  2739                                  			dd	0						;44 edi
  2740                                  			dd	001Ch						;48 es
  2741                                  			dd	0024h						;4C cs
  2742                                  			dd	0014h						;50 ss ring 3
  2743                                  			dd	001Ch						;54 ds
  2744                                  			dd	0						;58 fs
  2745                                  			dd	0						;5c gs
  2746                                  			dd	0050h						;60 ldt selector in gdt
  2747                                  			times	128-($-$$) db 0h				;zero fill to end of section
  2748                                  ;-----------------------------------------------------------------------------------------------------------------------
  2749                                  ;
  2750                                  ;	Console Message Queue							@disk: 007C00	@mem: 004800
  2751                                  ;
  2752                                  ;	The console message queue is 2048 bytes of memory organized as a queue of 510 double words (4 bytes each) and
  2753                                  ;	two double word values that act as indices. The queue is a FIFO that is fed by the keyboard hardware interrupt
  2754                                  ;	handler and consumed by a service routine called from a task. Each queue entry defines an input (keystroke)
  2755                                  ;	event.
  2756                                  ;
  2757                                  ;-----------------------------------------------------------------------------------------------------------------------
  2758                                  section			conmque							;console message queue
  2759                                  			dd	8						;head pointer
  2760                                  			dd	8						;tail pointer
  2761                                  			times	510 dd 0					;queue elements
  2762                                  ;-----------------------------------------------------------------------------------------------------------------------
  2763                                  ;
  2764                                  ;	Console Code								@disk: 008400	@mem: 005000
  2765                                  ;
  2766                                  ;	This is the code for the console task. The task is defined in the GDT in two descriptors, the Local Descriptor
  2767                                  ;	Table (LDT) at 0050h and the Task State Segment (TSS) at 0058h. Jumping to or calling a TSS selector causes a
  2768                                  ;	task switch, giving control to the code for the task at the CS:IP defined in the TSS for the current ring level.
  2769                                  ;	The initial CS:IP in the Console TSS is 24h:0, where 24h is a selector in the LDT. This selector points to the
  2770                                  ;	concode section, loaded into memory 5000h by the Loader. The console task is dedicated to accepting user key-
  2771                                  ;	board input, echoing to the console screen and responding to user commands.
  2772                                  ;
  2773                                  ;	When control reaches this section, our addressability is set up according to the following diagram.
  2774                                  ;
  2775                                  ;	DS,ES --------> 000000	+-----------------------------------------------+ DS,ES:0000
  2776                                  ;				|  Real Mode Interrupt Vectors			|
  2777                                  ;			000400	+-----------------------------------------------+ DS,ES:0400
  2778                                  ;				|  Reserved BIOS Memory Area			|
  2779                                  ;			000800	+-----------------------------------------------+ DS,ES:0800
  2780                                  ;				|  Shared Kernel Memory Area			|
  2781                                  ;			001000	+-----------------------------------------------+		<-- GDTR
  2782                                  ;				|  Global Descriptor Table (GDT)		|
  2783                                  ;			001800	+-----------------------------------------------+		<-- IDTR
  2784                                  ;				|  Interrupt Descriptor Table (IDT)		|
  2785                                  ;			002000	+-----------------------------------------------+
  2786                                  ;				|  Interrupt Handlers				|
  2787                                  ;				|  Kernel Function Library			|
  2788                                  ;	SS ----------->	004000	+===============================================+ SS:0000
  2789                                  ;				|  Console Task Stack Area			|
  2790                                  ;	SS:SP --------> 004700	+-----------------------------------------------+ SS:0700	<-- LDTR = GDT.SEL 0050h
  2791                                  ;				|  Console Task Local Descriptor Table (LDT)	|
  2792                                  ;			004780	+-----------------------------------------------+		<-- TR  = GDT.SEL 0058h
  2793                                  ;				|  Console Task Task State Segment (TSS)	|
  2794                                  ;			004800	+-----------------------------------------------+
  2795                                  ;				|  Console Task Message Queue			|
  2796                                  ;	CS,CS:IP ----->	005000	+-----------------------------------------------+ CS:0000
  2797                                  ;				|  Console Task Code				|
  2798                                  ;				|  Console Task Constants			|
  2799                                  ;			006000	+===============================================+
  2800                                  ;
  2801                                  ;-----------------------------------------------------------------------------------------------------------------------
  2802                                  section			concode	vstart=5000h					;labels relative to 5000h
  2803                                  ConCode			call	ConInitializeData				;initialize console variables
  2804                                  			clearConsoleScreen					;clear the console screen
  2805                                  			putConsoleString czTitle				;display startup message
  2806                                  .10			putConsoleString czPrompt				;display input prompt
  2807                                  			placeCursor						;set CRT cursor location
  2808                                  			getConsoleString wzConsoleInBuffer,79,1,13		;accept keyboard input
  2809                                  			putConsoleString czNewLine				;newline
  2810                                  			putConsoleString wzConsoleInBuffer			;print entered command
  2811                                  			putConsoleString czNewLine				;new line
  2812                                  			putConsoleString czUnknownCommand			;display error message
  2813                                  			jmp	.10						;next command
  2814                                  ;-----------------------------------------------------------------------------------------------------------------------
  2815                                  ;
  2816                                  ;	Routine:	ConInitializeData
  2817                                  ;
  2818                                  ;	Description:	Initialize console variables used by the console task.
  2819                                  ;
  2820                                  ;-----------------------------------------------------------------------------------------------------------------------
  2821                                  ConInitializeData	push	ecx						;save volatile regs
  2822                                  			push	edi						;
  2823                                  			push	es						;
  2824                                  			push	ESELDAT						;load OS data selector ...
  2825                                  			pop	es						;... into extra segment register
  2826                                  			mov	edi,ECONDATA					;OS console data address
  2827                                  			xor	al,al						;initialization value
  2828                                  			mov	ecx,ECONDATALEN					;size of OS console data
  2829                                  			cld							;forward strings
  2830                                  			rep	stosb						;initialize data
  2831                                  			pop	es						;restore volatile regs
  2832                                  			pop	edi						;
  2833                                  			pop	ecx						;
  2834                                  			ret							;return to caller
  2835                                  ;-----------------------------------------------------------------------------------------------------------------------
  2836                                  ;
  2837                                  ;	Constants
  2838                                  ;
  2839                                  ;-----------------------------------------------------------------------------------------------------------------------
  2840                                  czTitle			db	"Custom Operating System 1.0",13,10,0		;version string
  2841                                  czPrompt		db	":",0						;prompt string
  2842                                  czUnknownCommand	db	"Unknown command",13,10,0			;unknown command response string
