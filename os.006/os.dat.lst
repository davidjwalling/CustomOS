  1626                                  irq0.15 		mov	dh,EFDCPORTHI					;FDC controller port hi
  1627                                  			mov	dl,EFDCPORTLOSTAT				;FDC main status register
  1628                                  			in	al,dx						;FDC main status byte
  1629                                  			test	al,EFDCSTATBUSY					;test FDC main status for busy
  1630                                  			jnz	irq0.15						;wait while busy
  1631                                  
  1632                                  			mov	al,EFDCMOTOROFF					;motor-off / enable/ DMA setting
  1633                                  			mov	byte [wbFDCControl],al				;save motor-off setting
  1634                                  			mov	dh,EFDCPORTHI					;fdc port hi
  1635                                  			mov	dl,EFDCPORTLOOUT				;fdc digital output register
  1636                                  			out	dx,al						;turn motor off
