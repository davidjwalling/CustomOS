  2021                                  ;	Routine:	PutConsoleString
  2022                                  ;
  2023                                  ;	Description:	Write a sequence of ASCII characters to the console until null and update the console position
  2024                                  ;			as needed.
  2025                                  ;
  2026                                  ;	In:		EDX	source address
  2027                                  ;			DS	OS data selector
  2028                                  ;
  2029                                  ;-----------------------------------------------------------------------------------------------------------------------
  2030                                  PutConsoleString	push	esi						;save volatile regs
  2031                                  			mov	esi,edx						;source address
  2032                                  			cld							;forward strings
  2033                                  .10			lodsb							;ASCII character
  2034                                  			or	al,al						;end of string?
  2035                                  			jz	.40						;yes, skip ahead
  2036                                  			cmp	al,EASCIIRETURN					;carriage return?
  2037                                  			jne	.20						;no, skip ahead
  2038                                  			call	FirstConsoleColumn				;move to start of row
  2039                                  			jmp	.10						;next character
  2040                                  .20			cmp	al,EASCIILINEFEED				;line feed?
  2041                                  			jne	.30						;no, skip ahead
  2042                                  			call	NextConsoleRow					;move to next row
  2043                                  			jmp	.10						;next character
  2044                                  .30			call	PutConsoleChar					;output character to console
  2045                                  			call	NextConsoleColumn				;advance to next column
  2046                                  			jmp	.10						;next character
  2047                                  .40			pop	esi						;restore volatile regs
  2048                                  			ret							;return to caller
  2049                                  ;-----------------------------------------------------------------------------------------------------------------------
  2050                                  ;
  2051                                  ;	Routine:	GetConsoleString
  2052                                  ;
  2053                                  ;	Description:	Accept keyboard input into a buffer.
  2054                                  ;
  2055                                  ;	Input:		ds:edx	target buffer address
  2056                                  ;			ecx	size of input buffer
  2057                                  ;			bh	echo to terminal
  2058                                  ;			bl	terminating character
  2059                                  ;
  2060                                  ;-----------------------------------------------------------------------------------------------------------------------
  2061                                  GetConsoleString	push	ecx						;save volatile regs
  2062                                  			push	esi						;
  2063                                  			push	edi						;
  2064                                  			push	es						;
  2065                                  			push	ds						;load data segment selector ...
  2066                                  			pop	es						;... into extra segment register
  2067                                  			mov	edi,edx						;edi = target buffer
  2068                                  			mov	esi,edx						;esi = target buffer
  2069                                  .10			jecxz	.50						;exit if max-length is zero
  2070                                  .20			call	GetConsoleChar					;al = next input char
  2071                                  			cmp	al,bl						;is this the terminator?
  2072                                  			je	.50						;yes, exit
  2073                                  			cmp	al,EASCIIBACKSPACE				;is this a backspace?
  2074                                  			jne	.30						;no, skip ahead
  2075                                  			cmp	esi,edi						;at start of buffer?
  2076                                  			je	.20						;yes, get next character
  2077                                  			dec	edi						;backup target pointer
  2078                                  			mov	byte [edi],0					;zero previous character
  2079                                  			inc	ecx						;increment remaining chars
  2080                                  			test	bh,1						;echo to console?
  2081                                  			jz	.20						;no, get next character
  2082                                  			call	PreviousConsoleColumn				;backup console position
  2083                                  			mov	al,EASCIISPACE					;ASCII space
  2084                                  			call	PutConsoleChar					;write space to console
  2085                                  			call	PlaceCursor					;position the cursor
  2086                                  			jmp	.20						;get next character
  2087                                  .30			cmp	al,EASCIISPACE					;printable? (lower bounds)
  2088                                  			jb	.20						;no, get another character
  2089                                  			cmp	al,EASCIITILDE					;printable? (upper bounds)
  2090                                  			ja	.20						;no, get another character
  2091                                  			stosb							;store character in buffer
  2092                                  			test	bh,1						;echo to console?
  2093                                  			jz	.40						;no, skip ahead
  2094                                  			call	PutConsoleChar					;write character to console
  2095                                  			call	NextConsoleColumn				;advance console position
  2096                                  			call	PlaceCursor					;position the cursor
  2097                                  .40			dec	ecx						;decrement remaining chars
  2098                                  			jmp	.10						;next
  2099                                  .50			xor	al,al						;null
  2100                                  			stosb							;terminate buffer
  2101                                  			pop	es						;restore volatile regs
  2102                                  			pop	edi						;
  2103                                  			pop	esi						;
  2104                                  			pop	ecx						;
  2105                                  			ret							;return to caller
