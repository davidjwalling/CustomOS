  2391                                  ;	Routine:	PutMessage
  2392                                  ;
  2393                                  ;	Description:	Add a message to the message queue.
  2394                                  ;
  2395                                  ;	Input:		ecx	hi-order data word
  2396                                  ;			edx	lo-order data word
  2397                                  ;
  2398                                  ;	Output:		CY	0 = success
  2399                                  ;				1 = fail: queue is full
  2400                                  ;
  2401                                  ;-----------------------------------------------------------------------------------------------------------------------
  2402                                  PutMessage		push	ds						;save volatile regs
  2403                                  			push	ESELMQ						;load task message queue selector ...
  2404                                  			pop	ds						;... into data segment register
  2405                                  			mov	eax,[MQTail]					;tail ptr
  2406                                  			cmp	dword [eax],0					;is queue full?
  2407                                  			stc							;assume failure
  2408                                  			jne	.20						;yes, cannot store
  2409                                  			mov	[eax],edx					;store lo-order data
  2410                                  			mov	[eax+4],ecx					;store hi-order data
  2411                                  			add	eax,8						;next queue element adr
  2412                                  			and	eax,03fch					;at end of queue?
  2413                                  			jnz	.10						;no, skip ahead
  2414                                  			mov	al,8						;reset to top of queue
  2415                                  .10			mov	[MQTail],eax					;save new tail ptr
  2416                                  			clc							;indicate success
  2417                                  .20			pop	ds						;restore volatile regs
  2418                                  			ret							;return to caller
  2419                                  ;-----------------------------------------------------------------------------------------------------------------------
  2420                                  ;
  2421                                  ;	Routine:	GetMessage
  2422                                  ;
  2423                                  ;	Description:	Read and remove a message from the message queue.
  2424                                  ;
  2425                                  ;	Output:		eax	lo-order message data
  2426                                  ;			edx	hi-order message data
  2427                                  ;
  2428                                  ;			CY	0 = message read
  2429                                  ;				1 = no message to read
  2430                                  ;
  2431                                  ;-----------------------------------------------------------------------------------------------------------------------
  2432                                  GetMessage		push	ebx						;save volatile regs
  2433                                  			push	ecx						;
  2434                                  			push	ds						;
  2435                                  			push	ESELMQ						;load message queue selector ...
  2436                                  			pop	ds						;... into data segment register
  2437                                  			mov	ebx,[MQHead]					;head ptr
  2438                                  			mov	eax,[ebx]					;lo-order 32 bits
  2439                                  			mov	edx,[ebx+4]					;hi-order 32 bits
  2440                                  			or	eax,edx						;is queue empty?
  2441                                  			stc							;assume queue is emtpy
  2442                                  			jz	.20						;yes, skip ahead
  2443                                  			xor	ecx,ecx						;store zero
  2444                                  			mov	[ebx],ecx					;... in lo-order dword
  2445                                  			mov	[ebx+4],ecx					;... in hi-order dword
  2446                                  			add	ebx,8						;next queue element
  2447                                  			and	ebx,03fch					;at end of queue?
  2448                                  			jnz	.10						;no, skip ahead
  2449                                  			mov	bl,8						;reset to 1st entry
  2450                                  .10			mov	[MQHead],ebx					;save new head ptr
  2451                                  			clc							;indicate message read
  2452                                  .20			pop	ds						;restore volatile regs
  2453                                  			pop	ecx						;
  2454                                  			pop	ebx						;
  2455                                  			ret							;return to caller
  2456                                  ;-----------------------------------------------------------------------------------------------------------------------
  2457                                  ;
  2458                                  ;	Memory-Mapped Video Routines
  2459                                  ;
  2460                                  ;	These routines read and/or write directly to CGA video memory (B800:0)
  2461                                  ;
  2462                                  ;	ClearConsoleScreen
  2463                                  ;	ScrollConsoleRow
  2464                                  ;	SetConsoleChar
  2465                                  ;
