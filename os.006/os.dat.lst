  2468                                  ;
  2469                                  ;	Routine:	ClearConsoleScreen
  2470                                  ;
  2471                                  ;	Description:	Clear the console (CGA) screen.
  2472                                  ;
  2473                                  ;-----------------------------------------------------------------------------------------------------------------------
  2474                                  ClearConsoleScreen	push	ecx						;save volatile regs
  2475                                  			push	edi						;
  2476                                  			push	ds						;
  2477                                  			push	es						;
  2478                                  			push	ESELDAT						;load OS Data selector ...
  2479                                  			pop	ds						;... into DS register
  2480                                  			push	ESELCGA						;load CGA selector ...
  2481                                  			pop	es						;... into ES register
  2482                                  			mov	eax,ECONCLEARDWORD				;initializtion value
  2483                                  			mov	ecx,ECONROWDWORDS*(ECONROWS)			;double-words to clear
  2484                                  			xor	edi,edi						;target offset
  2485                                  			cld							;forward strings
  2486                                  			rep	stosd						;reset screen body
  2487                                  			mov	eax,ECONOIADWORD				;OIA attribute and space
  2488                                  			mov	ecx,ECONROWDWORDS				;double-words per row
  2489                                  			rep	stosd						;reset OIA line
  2490                                  			xor	al,al						;zero register
  2491                                  			mov	[wbConsoleRow],al				;reset console row
  2492                                  			mov	[wbConsoleColumn],al				;reset console column
  2493                                  			call	PlaceCursor					;place cursor at current position
  2494                                  			pop	es						;restore volatile regs
  2495                                  			pop	ds						;
  2496                                  			pop	edi						;
  2497                                  			pop	ecx						;
  2498                                  			ret							;return to caller
  2499                                  ;-----------------------------------------------------------------------------------------------------------------------
  2500                                  ;
  2501                                  ;	Routine:	ScrollConsoleRow
  2502                                  ;
  2503                                  ;	Description:	Scroll console (text) screen up one row.
  2504                                  ;
  2505                                  ;-----------------------------------------------------------------------------------------------------------------------
  2506                                  ScrollConsoleRow	push	ecx						;save volatile regs
  2507                                  			push	esi						;
  2508                                  			push	edi						;
  2509                                  			push	ds						;
  2510                                  			push	es						;
  2511                                  			push	ESELCGA						;load CGA video selector ...
  2512                                  			pop	ds						;... into DS
  2513                                  			push	ESELCGA						;load CGA video selector ...
  2514                                  			pop	es						;... into ES
  2515                                  			mov	ecx,ECONROWDWORDS*(ECONROWS-1)			;double-words to move
  2516                                  			mov	esi,ECONROWBYTES				;esi = source (line 2)
  2517                                  			xor	edi,edi						;edi = target (line 1)
  2518                                  			cld							;forward strings
  2519                                  			rep	movsd						;move 24 lines up
  2520                                  			mov	eax,ECONCLEARDWORD				;attribute and ASCII space
  2521                                  			mov	ecx,ECONROWDWORDS				;double-words per row
  2522                                  			rep	stosd						;clear bottom row
  2523                                  			pop	es						;restore volatile regs
  2524                                  			pop	ds						;
  2525                                  			pop	edi						;
  2526                                  			pop	esi						;
  2527                                  			pop	ecx						;
  2528                                  			ret							;return to caller
  2529                                  ;-----------------------------------------------------------------------------------------------------------------------
  2530                                  ;
  2531                                  ;	Routine:	SetConsoleChar
  2532                                  ;
  2533                                  ;	Description:	Output an ASCII character at the given row and column.
  2534                                  ;
  2535                                  ;	In:		AL	ASCII character
  2536                                  ;			CL	column
  2537                                  ;			CH	row
  2538                                  ;			ES	CGA selector
  2539                                  ;
  2540                                  ;-----------------------------------------------------------------------------------------------------------------------
  2541                                  SetConsoleChar		mov	dl,al						;ASCII character
  2542                                  			movzx	eax,ch						;row
  2543                                  			mov	ah,ECONCOLS					;cols/row
  2544                                  			mul	ah						;row * cols/row
  2545                                  			add	al,cl						;add column
  2546                                  			adc	ah,0						;handle carry
  2547                                  			shl	eax,1						;screen offset
  2548                                  			mov	[es:eax],dl					;store character
  2549                                  			ret							;return to caller
