  2168                                  			inc	al						;increment column
  2169                                  			mov	[wbConsoleColumn],al				;save column
  2170                                  			cmp	al,ECONCOLS					;end of row?
  2171                                  			jb	.10						;no, skip ahead
  2172                                  			call	FirstConsoleColumn				;reset column to start of row
  2173                                  			call	NextConsoleRow					;line feed to next row
  2174                                  .10			ret							;return to caller
