  1607                                  			push	edx						;
  1608                                  			push	ds						;
  1609                                  			push	ESELDAT						;load OS data selector ...
  1610                                  			pop	ds						;... into data segment register
  1611                                  
  1612                                  			mov	eax,[wfClockTicks]				;eax = clock ticks
  1613                                  			inc	eax						;increment clock ticks
  1614                                  			cmp	eax,EPITDAYTICKS				;clock ticks per day?
  1615                                  			jb	irq0.10						;no, skip ahead
  1616                                  			inc	byte [wbClockDays]				;increment clock days
  1617                                  			xor	eax,eax						;reset clock ticks
  1618                                  irq0.10			mov	dword [wfClockTicks],eax			;save clock ticks
  1619                                  
  1620                                  			cmp	byte [wbFDCMotor],0				;floppy motor timeout?
  1621                                  			je	irq0.20						;yes, skip ahead
  1622                                  			dec	byte [wbFDCMotor]				;decrement motor timeout
  1623                                  			jnz	irq0.20						;skip ahead if non-zero
  1624                                  
