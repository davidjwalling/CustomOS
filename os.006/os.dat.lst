  2108                                  ;	Routine:	GetConsoleChar
  2109                                  ;
  2110                                  ;	Description:	Wait for EMSGKEYCHAR message and return character code.
  2111                                  ;
  2112                                  ;	Output:		al	ASCII character code
  2113                                  ;			ah	keyboard scan code
  2114                                  ;
  2115                                  ;-----------------------------------------------------------------------------------------------------------------------
  2116                                  GetConsoleChar.10	call	Yield						;pass control or halt
  2117                                  GetConsoleChar		call	GetMessage					;get the next message
  2118                                  			or	eax,eax						;do we have a message?
  2119                                  			jz	GetConsoleChar.10				;no, skip ahead
  2120                                  			push	eax						;save key codes
  2121                                  			and	eax,0FFFF0000h					;mask for message type
  2122                                  			cmp	eax,EMSGKEYCHAR					;key-char message?
  2123                                  			pop	eax						;restore key codes
  2124                                  			jne	GetConsoleChar					;no, try again
  2125                                  			and	eax,0000ffffh					;mask for key codes
  2126                                  			ret							;return to caller
