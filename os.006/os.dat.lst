  2147                                  			or	al,al						;start of row?
  2148                                  			jnz	.10						;no, skip ahead
  2149                                  			mov	ah,[wbConsoleRow]				;current row
  2150                                  			or	ah,ah						;top of screen?
  2151                                  			jz	.20						;yes, exit with no change
  2152                                  			dec	ah						;decrement row
  2153                                  			mov	[wbConsoleRow],ah				;save row
  2154                                  			mov	al,ECONCOLS					;set maximum column
  2155                                  .10			dec	al						;decrement column
  2156                                  			mov	[wbConsoleColumn],al				;save column
  2157                                  .20			ret							;return to caller
