  2903                                  ;
  2904                                  ;       Routine:        AllocateMemory
  2905                                  ;
  2906                                  ;       Description:    This routine allocates a memory block for the given task.
  2907                                  ;
  2908                                  ;       In:             ECX     bytes of memory to allocate
  2909                                  ;
  2910                                  ;       Out:            EAX     !0      address of user portion of newly allocated memory block
  2911                                  ;                               0       unable to allocate memory
  2912                                  ;
  2913                                  ;-----------------------------------------------------------------------------------------------------------------------
  2914 00000977 53                      AllocateMemory          push    ebx                                             ;save non-volatile regs
  2915 00000978 51                                              push    ecx                                             ;
  2916 00000979 56                                              push    esi                                             ;
  2917 0000097A 1E                                              push    ds                                              ;
  2918                                  ;
  2919                                  ;       Address kernel memory structures
  2920                                  ;
  2921 0000097B 6A18                                            push    EGDTOSDATA                                      ;load OS data GDT selector ...
  2922 0000097D 1F                                              pop     ds                                              ;... into data segment reg
  2923 0000097E BE31090000                                      mov     esi,wsConsoleMemRoot                            ;memory root structure address
  2924                                  ;
  2925                                  ;       Set requested size to minimum block size if requested size is too small.
  2926                                  ;
  2927 00000983 81F900010000                                    cmp     ecx,EMEMMINSIZE                                 ;is requested size too small?
  2928 00000989 7305                                            jae     .10                                             ;no, branch
  2929 0000098B B900010000                                      mov     ecx,EMEMMINSIZE                                 ;set requested size to minimum
  2930 00000990 83C120                  .10                     add     ecx,EMEMBLOCKLEN                                ;add header block length
  2931                                  ;
  2932                                  ;       Find the first free memory block large enough to satisfy the request.
  2933                                  ;
  2934 00000993 8B4608                                          mov     eax,[esi+MEMROOT.firstfree]                     ;first free block ptr
  2935 00000996 85C0                    .20                     test    eax,eax                                         ;end of free block chain?
  2936 00000998 0F84F2000000                                    jz      .220                                            ;yes, branch
  2937 0000099E 3B4804                                          cmp     ecx,[eax+MEMBLOCK.bytes]                        ;free block big enough?
  2938 000009A1 7605                                            jbe     .30                                             ;yes, branch
  2939 000009A3 8B4018                                          mov     eax,[eax+MEMBLOCK.nextblock]                    ;next free block addr
  2940 000009A6 EBEE                                            jmp     .20                                             ;continue
  2941                                  ;-----------------------------------------------------------------------------------------------------------------------
  2942                                  ;
  2943                                  ;       Address the previous and next free memory blocks.
  2944                                  ;
  2945 000009A8 8B581C                  .30                     mov     ebx,[eax+MEMBLOCK.previousblock]                ;previous free block addr
  2946 000009AB 8B5018                                          mov     edx,[eax+MEMBLOCK.nextblock]                    ;next free block addr
  2947                                  ;
  2948                                  ;       Remove the free memory block from the forward free memory block chain.
  2949                                  ;
  2950 000009AE 85DB                                            test    ebx,ebx                                         ;any previous free memory block?
  2951 000009B0 7405                                            jz      .40                                             ;no, branch
  2952 000009B2 895318                                          mov     [ebx+MEMBLOCK.nextblock],edx                    ;remove free block from forwrad chain
  2953 000009B5 EB03                                            jmp     .50                                             ;continue
  2954 000009B7 895608                  .40                     mov     [esi+MEMROOT.firstfree],edx                     ;next free is now also the first free
  2955                                  ;
  2956                                  ;       Remove the free memory block from the reverse free memory block chain.
  2957                                  ;
  2958 000009BA 85D2                    .50                     test    edx,edx                                         ;any next free memory block?
  2959 000009BC 7405                                            jz      .60                                             ;no, branch
  2960 000009BE 895A1C                                          mov     [edx+MEMBLOCK.previousblock],ebx                ;remove free block from reverse chain
  2961 000009C1 EB03                                            jmp     .70                                             ;continue
  2962 000009C3 895E0C                  .60                     mov     [esi+MEMROOT.lastfree],ebx                      ;previous free is now also the last free
  2963                                  ;-----------------------------------------------------------------------------------------------------------------------
  2964                                  ;
  2965                                  ;       Determine if the free memory block can be split.
  2966                                  ;
  2967 000009C6 8B5804                  .70                     mov     ebx,[eax+MEMBLOCK.bytes]                        ;size of free memory block
  2968 000009C9 29CB                                            sub     ebx,ecx                                         ;subtract requested memory size
  2969 000009CB 81FB00010000                                    cmp     ebx,EMEMMINSIZE                                 ;remaining block can stand alone?
  2970 000009D1 7263                                            jb      .150                                            ;no, branch
  2971                                  ;
  2972                                  ;       We know that our block can be split to create a new free memory block. We update the size of our free memory
  2973                                  ;       block to the requested memory size. We update the next contiguous block pointer to point just past the end
  2974                                  ;       of the requested memory size.
  2975                                  ;
  2976 000009D3 894804                                          mov     [eax+MEMBLOCK.bytes],ecx                        ;shorten memory block size
  2977 000009D6 89C2                                            mov     edx,eax                                         ;memory block address
  2978 000009D8 01CA                                            add     edx,ecx                                         ;address new new next contig block
  2979 000009DA 8B4810                                          mov     ecx,[eax+MEMBLOCK.nextcontig]                   ;next contig block address
  2980 000009DD 895010                                          mov     [eax+MEMBLOCK.nextcontig],edx                   ;update next contig block address
  2981                                  ;
  2982                                  ;       If there is a next contiguous block, we update that memory block's previous contig pointer to point to the new
  2983                                  ;       free block we are splitting off. If there is no next contiguous block, we update the last contig block pointer.
  2984                                  ;
  2985 000009E0 E305                                            jecxz   .80                                             ;no next contig, branch
  2986 000009E2 895114                                          mov     [ecx+MEMBLOCK.previouscontig],edx               ;update previous contig pointer
  2987 000009E5 EB03                                            jmp     .90                                             ;continue
  2988 000009E7 895604                  .80                     mov     [esi+MEMROOT.lastcontig],edx                    ;update last contig pointer
  2989                                  ;
  2990                                  ;       Now that the contig block pointers have been updated, we initialize the new free block members.
  2991                                  ;
  2992 000009EA 895A04                  .90                     mov     [edx+MEMBLOCK.bytes],ebx                        ;set the block size
  2993 000009ED 894A10                                          mov     [edx+MEMBLOCK.nextcontig],ecx                   ;set the next contig block addr
  2994 000009F0 894214                                          mov     [edx+MEMBLOCK.previouscontig],eax               ;set the previous contig block addr
  2995 000009F3 BB46524545                                      mov     ebx,EMEMFREECODE                                ;free memory signature
  2996 000009F8 891A                                            mov     [edx+MEMBLOCK.signature],ebx                    ;set the block signature
  2997 000009FA 31DB                                            xor     ebx,ebx                                         ;zero register
  2998 000009FC 895A0C                                          mov     [edx+MEMBLOCK.reserved],ebx                     ;set reserved
  2999 000009FF 895A08                                          mov     [edx+MEMBLOCK.owner],ebx                        ;set the owner
  3000                                  ;
  3001                                  ;       Find the proper location in the free block chain for the new free block
  3002                                  ;
  3003 00000A02 8B5A04                                          mov     ebx,[edx+MEMBLOCK.bytes]                        ;free block size
  3004 00000A05 8B4E08                                          mov     ecx,[esi+MEMROOT.firstfree]                     ;first free block addr
  3005 00000A08 E30A                    .100                    jecxz   .110                                            ;branch if at end of chain
  3006 00000A0A 3B5904                                          cmp     ebx,[ecx+MEMBLOCK.bytes]                        ;new block smaller or equal?
  3007 00000A0D 7605                                            jbe     .110                                            ;yes, branch
  3008 00000A0F 8B4918                                          mov     ecx,[ecx+MEMBLOCK.nextblock]                    ;next free block addr
  3009 00000A12 EBF4                                            jmp     .100                                            ;continue
  3010                                  ;
  3011                                  ;       Having found the proper location for our new free block, we store the address of the following free block, or
  3012                                  ;       zero if our new free block is larger than any other, as our next free block. Then, we take the address of our
  3013                                  ;       next block's previous block or the global last-free block as our new previous block and update the previous
  3014                                  ;       block of hte next block, if there is one.
  3015                                  ;
  3016 00000A14 894A18                  .110                    mov     [edx+MEMBLOCK.nextblock],ecx                    ;set the new free block's next ptr
  3017 00000A17 8B5E0C                                          mov     ebx,[esi+MEMROOT.lastfree]                      ;last free block addr
  3018 00000A1A E308                                            jecxz   .120                                            ;branch if no next block
  3019 00000A1C 8B591C                                          mov     ebx,[ecx+MEMBLOCK.previousblock]                ;next block's previous block
  3020 00000A1F 89511C                                          mov     [ecx+MEMBLOCK.previousblock],edx                ;set the next block's previous block
  3021 00000A22 EB03                                            jmp     .130                                            ;continue
  3022 00000A24 89560C                  .120                    mov     [esi+MEMROOT.lastfree],edx                      ;set the new last free block
  3023                                  ;
  3024                                  ;       Store our previous block pointer. If we have a previous free block, update that block's next block pointer to
  3025                                  ;       point to the new block. Since the new block may now be the first or last user block, we update the first and/or
  3026                                  ;       last user block pointers if necessary.
  3027                                  ;
  3028 00000A27 895A1C                  .130                    mov     [edx+MEMBLOCK.previousblock],ebx                ;set the previous block pointer
  3029 00000A2A 85DB                                            test    ebx,ebx                                         ;is there a previous block?
  3030 00000A2C 7405                                            jz      .140                                            ;no, branch
  3031 00000A2E 895318                                          mov     [ebx+MEMBLOCK.nextblock],edx                    ;set the previous block's next ptr
  3032 00000A31 EB03                                            jmp     .150                                            ;continue
  3033 00000A33 895608                  .140                    mov     [esi+MEMROOT.firstfree],edx                     ;set the new first free ptr
  3034                                  ;
  3035                                  ;       Update the newly allocated block's owner and signature.
  3036                                  ;
  3037 00000A36 BA55534552              .150                    mov     edx,EMEMUSERCODE                                ;user memory signature
  3038 00000A3B 8910                                            mov     [eax+MEMBLOCK.signature],edx                    ;set the block signature
  3039 00000A3D 31D2                                            xor     edx,edx                                         ;zero register
  3040 00000A3F 660F00CA                                        str     dx                                              ;load the task state register
  3041 00000A43 895008                                          mov     [eax+MEMBLOCK.owner],edx                        ;set the block owner
  3042                                  ;
  3043                                  ;       Remove the allocated block from the free block chain and insert it into the user block chain.
  3044                                  ;
  3045 00000A46 8B4E10                                          mov     ecx,[esi+MEMROOT.firsttask]                     ;first task block
  3046 00000A49 E323                    .160                    jecxz   .180                                            ;branch if at end of chain
  3047 00000A4B 3B5108                                          cmp     edx,[ecx+MEMBLOCK.owner]                        ;does this block belong to the task?
  3048 00000A4E 721E                                            jb      .180                                            ;branch if block belongs to next task
  3049 00000A50 7405                                            je      .170                                            ;branch if block belongs to this task
  3050 00000A52 8B4918                                          mov     ecx,[ecx+MEMBLOCK.nextblock]                    ;next task block
  3051 00000A55 EBF2                                            jmp     .160                                            ;continue
  3052                                  ;
  3053                                  ;       We have found the start of our task's user block chain or the start of the next task's user block chain. If we
  3054                                  ;       have found the next task's chain, then we have no other user memory for this task and we can simply add the
  3055                                  ;       block here. If we are at the start of our task's user block chain, then we need to further seek for the proper
  3056                                  ;       place to insert the block.
  3057                                  ;
  3058 00000A57 8B5004                  .170                    mov     edx,[eax+MEMBLOCK.bytes]                        ;size of block in bytes
  3059 00000A5A 3B5104                                          cmp     edx,[ecx+MEMBLOCK.bytes]                        ;less or equal to chain block?
  3060 00000A5D 760F                                            jbe     .180                                            ;yes, branch
  3061 00000A5F 8B4918                                          mov     ecx,[ecx+MEMBLOCK.nextblock]                    ;next chain block address
  3062 00000A62 85C9                                            test    ecx,ecx                                         ;end of chain?
  3063 00000A64 7408                                            jz      .180                                            ;yes, branch
  3064 00000A66 8B5008                                          mov     edx,[eax+MEMBLOCK.owner]                        ;owning task
  3065 00000A69 3B5108                                          cmp     edx,[ecx+MEMBLOCK.owner]                        ;same task?
  3066 00000A6C 74E9                                            je      .170                                            ;yes, continue search
  3067                                  ;
  3068                                  ;       We have found the proper place in our task's user-block chain to insert our new user block. It may also be the
  3069                                  ;       end of the user-block chain. To insert our new user block, first we update the next-block pointer. Then, we load
  3070                                  ;       the next-block's previous-block pointer or the global last-user block pointer if we have no next-block. If we
  3071                                  ;       do have a previous-block, we update that block's next-block pointer.
  3072                                  ;
  3073 00000A6E 894818                  .180                    mov     [eax+MEMBLOCK.nextblock],ecx                    ;set the next task block
  3074 00000A71 8B5E14                                          mov     ebx,[esi+MEMROOT.lasttask]                      ;last task block
  3075 00000A74 E308                                            jecxz   .190                                            ;branch if no next-task block
  3076 00000A76 8B591C                                          mov     ebx,[ecx+MEMBLOCK.previousblock]                ;next-task's previous-task block
  3077 00000A79 89411C                                          mov     [ecx+MEMBLOCK.previousblock],eax                ;update next-task block's previous-task
  3078 00000A7C EB03                                            jmp     .200                                            ;continue
  3079 00000A7E 894614                  .190                    mov     [esi+MEMROOT.lasttask],eax                      ;new block is the last user-block
  3080                                  ;
  3081                                  ;       Now wes tore our previous-block pointer and, if we have a previous-free block, we update that block's next-
  3082                                  ;       block pointer to point to our block. Since our block may now be the first or last user-block, we update the
  3083                                  ;       global first and/or last user-block pointers if necessary.
  3084                                  ;
  3085 00000A81 89581C                  .200                    mov     [eax+MEMBLOCK.previousblock],ebx                ;set the previous task block
  3086 00000A84 85DB                                            test    ebx,ebx                                         ;do we have a previous task block?
  3087 00000A86 7405                                            jz      .210                                            ;no, branch
  3088 00000A88 894318                                          mov     [ebx+MEMBLOCK.nextblock],eax                    ;set previous-block's next-task block
  3089 00000A8B EB03                                            jmp     .220                                            ;continue
  3090 00000A8D 894610                  .210                    mov     [esi+MEMROOT.firsttask],eax                     ;new block is the first user-block
  3091                                  ;
  3092                                  ;       Restore registers and return to caller.
  3093                                  ;
  3094 00000A90 1F                      .220                    pop     ds                                              ;restore non-volatie regs
  3095 00000A91 5E                                              pop     esi                                             ;
  3096 00000A92 59                                              pop     ecx                                             ;
  3097 00000A93 5B                                              pop     ebx                                             ;
  3098 00000A94 C3                                              ret                                                     ;return
