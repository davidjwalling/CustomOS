  3100                                  ;
  3101                                  ;       Routine:        FreeMemory
  3102                                  ;
  3103                                  ;       Description:    This routine frees a memory block for the given task. The address provided in EDX points to the
  3104                                  ;                       memory block header. The memory block must be USER memory, not a FREE memory block. If the block
  3105                                  ;                       is adjacent to a contiguous FREE memory block, then the blocks are merged. The residual FREE
  3106                                  ;                       memory is repositioned in the FREE memory block chain according to size. The user portion of the
  3107                                  ;                       block, following the block header, is reset (wiped) with the memory wipe value.
  3108                                  ;
  3109                                  ;       In:             EDX     memory block to free, relative to EGDTOSDATA
  3110                                  ;
  3111                                  ;       Out:            EAX     -1      invalid memory block
  3112                                  ;                               0       memory block freed
  3113                                  ;
  3114                                  ;-----------------------------------------------------------------------------------------------------------------------
  3115 00000A95 53                      FreeMemory              push    ebx                                             ;save non-volatile regs
  3116 00000A96 51                                              push    ecx                                             ;
  3117 00000A97 56                                              push    esi                                             ;
  3118 00000A98 57                                              push    edi                                             ;
  3119 00000A99 1E                                              push    ds                                              ;
  3120 00000A9A 06                                              push    es                                              ;
  3121                                  ;
  3122                                  ;       Address the root memory structure
  3123                                  ;
  3124 00000A9B 6A18                                            push    EGDTOSDATA                                      ;load OS data selector ...
  3125 00000A9D 07                                              pop     es                                              ;... into extra segment reg
  3126 00000A9E 6A18                                            push    EGDTOSDATA                                      ;load OS data selector ...
  3127 00000AA0 1F                                              pop     ds                                              ;... into data segment reg
  3128 00000AA1 BE31090000                                      mov     esi,wsConsoleMemRoot                            ;memory root structure
  3129 00000AA6 89D7                                            mov     edi,edx                                         ;memory block address
  3130                                  ;
  3131                                  ;       If the block is FREE, return success. Otherwise, if it is not USER, return with error.
  3132                                  ;
  3133 00000AA8 31C0                                            xor     eax,eax                                         ;indicate success
  3134 00000AAA 813F46524545                                    cmp     dword [edi+MEMBLOCK.signature],EMEMFREECODE     ;is the block FREE?
  3135 00000AB0 0F8415010000                                    je      .240                                            ;yes, branch
  3136 00000AB6 48                                              dec     eax                                             ;indicate failure
  3137 00000AB7 813F55534552                                    cmp     dword [edi+MEMBLOCK.signature],EMEMUSERCODE     ;is the block USER?
