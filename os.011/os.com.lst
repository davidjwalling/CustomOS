   993                                  ;
   994                                  ;       Constants
   995                                  ;
   996                                  ;-----------------------------------------------------------------------------------------------------------------------
   997                                                          align   2
   998                                  cwEntryLen              dw      32                                              ;length of directory entry
   999                                  cbKernelProgram         db      "OS      COM"                                   ;kernel program name
  1000                                  czLoadMsg               db      "Loading OS",13,10,0                            ;loading message
  1001                                  czErrorMsg              db      "Disk error "                                   ;error message
  1002                                  wzErrorCode             db      020h,020h,0                                     ;error code and null terminator
  1003                                  czNoKernel              db      "OS missing",0                                  ;missing kernel message
