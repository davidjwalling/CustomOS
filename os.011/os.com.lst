  1001                                  ;
  1002                                  ;       Work Areas
  1003                                  ;
  1004                                  ;-----------------------------------------------------------------------------------------------------------------------
  1005                                                          align   2
  1006                                  wwDirBuffer             dw      0                                               ;directory i/o buffer address
  1007                                  wwEntriesLeft           dw      0                                               ;directory entries to search
  1008                                  wwOverhead              dw      0                                               ;overhead sectors
  1009                                  wwSectorEntries         dw      0                                               ;directory entries per sector
  1010                                  wwLogicalSector         dw      0                                               ;current logical sector
  1011                                  wwReadCountCommand      equ     $                                               ;read count and command
  1012                                  wbReadCount             db      0                                               ;sectors to read
  1013                                  cbReadCommand           db      EBIOSFNREADSECTOR                               ;BIOS read disk fn code
  1014                                  wwDriveHead             equ     $                                               ;drive, head (word)
  1015                                  wbDrive                 db      0                                               ;drive
  1016                                  wbHead                  db      0                                               ;head
  1017                                  wwSectorTrack           equ     $                                               ;sector, track (word)
  1018                                                          db      0                                               ;sector
  1019                                  wbTrack                 db      0                                               ;track
  1020                                                          times   510-($-$$) db 0h                                ;zero fill to end of sector
  1021                                                          db      055h,0AAh                                       ;end of sector signature
