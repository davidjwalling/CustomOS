  3372 00000BD3 57                                              push    edi                                             ;
  3373 00000BD4 06                                              push    es                                              ;
  3374 00000BD5 1E                                              push    ds                                              ;copy DS
  3375 00000BD6 07                                              pop     es                                              ;... to ES
  3376 00000BD7 89D6                                            mov     esi,edx                                         ;first source address
  3377 00000BD9 89DF                                            mov     edi,ebx                                         ;second source address
  3378 00000BDB FC                                              cld                                                     ;forward strings
  3379 00000BDC F3A6                                            rep     cmpsb                                           ;compare bytes
  3380 00000BDE B000                                            mov     al,0                                            ;default result
  3381 00000BE0 7406                                            jz      .10                                             ;branch if arrays equal
  3382 00000BE2 B001                                            mov     al,1                                            ;positive result
  3383 00000BE4 7302                                            jnc     .10                                             ;branch if target > source
  3384 00000BE6 B0FF                                            mov     al,-1                                           ;negative result
  3385 00000BE8 0FBEC8                  .10                     movsx   ecx,al                                          ;extend sign
  3386 00000BEB 07                                              pop     es                                              ;restore non-volatile regs
  3387 00000BEC 5F                                              pop     edi                                             ;
  3388 00000BED 5E                                              pop     esi                                             ;
  3389 00000BEE C3                                              ret                                                     ;return
