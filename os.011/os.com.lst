  1005                                  ;
  1006                                  ;       Work Areas
  1007                                  ;
  1008                                  ;-----------------------------------------------------------------------------------------------------------------------
  1009                                                          align   2
  1010                                  wwDirBuffer             dw      0                                               ;directory i/o buffer address
  1011                                  wwEntriesLeft           dw      0                                               ;directory entries to search
  1012                                  wwOverhead              dw      0                                               ;overhead sectors
  1013                                  wwSectorEntries         dw      0                                               ;directory entries per sector
  1014                                  wwLogicalSector         dw      0                                               ;current logical sector
  1015                                  wwReadCountCommand      equ     $                                               ;read count and command
  1016                                  wbReadCount             db      0                                               ;sectors to read
  1017                                  cbReadCommand           db      EBIOSFNREADSECTOR                               ;BIOS read disk fn code
  1018                                  wwDriveHead             equ     $                                               ;drive, head (word)
  1019                                  wbDrive                 db      0                                               ;drive
  1020                                  wbHead                  db      0                                               ;head
  1021                                  wwSectorTrack           equ     $                                               ;sector, track (word)
  1022                                                          db      0                                               ;sector
  1023                                  wbTrack                 db      0                                               ;track
  1024                                                          times   510-($-$$) db 0h                                ;zero fill to end of sector
  1025                                                          db      055h,0AAh                                       ;end of sector signature
