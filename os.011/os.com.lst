  4056 00000FC0 891D00000000            .10                     mov     [MQHead],ebx                                    ;save new head ptr
  4057 00000FC6 F8                                              clc                                                     ;indicate message read
  4058 00000FC7 1F                      .20                     pop     ds                                              ;restore non-volatile regs
  4059 00000FC8 59                                              pop     ecx                                             ;
  4060 00000FC9 5B                                              pop     ebx                                             ;
  4061 00000FCA C3                                              ret                                                     ;return
