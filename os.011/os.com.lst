   990                                  ;       Constants
   991                                  ;
   992                                  ;-----------------------------------------------------------------------------------------------------------------------
   993                                                          align   2
   994                                  cwEntryLen              dw      32                                              ;length of directory entry
   995                                  cbKernelProgram         db      "OS      COM"                                   ;kernel program name
   996                                  czLoadMsg               db      "Loading OS",13,10,0                            ;loading message
   997                                  czErrorMsg              db      "Disk error "                                   ;error message
   998                                  wzErrorCode             db      020h,020h,0                                     ;error code and null terminator
   999                                  czNoKernel              db      "OS missing",0                                  ;missing kernel message
