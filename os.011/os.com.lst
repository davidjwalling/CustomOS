  1266                                                          db      020h                                            ;attribute (archive bit set)
  1267                                                          times   10 db 0                                         ;unused
  1268                                                          dw      0h                                              ;time
  1269                                                          db      01000001b                                       ;mmm = 10 MOD 8 = 2; ddddd = 1
  1270                                                          db      01001001b                                       ;yyyyyyy = 2016-1980 = 36 = 24h; m/8 = 1
