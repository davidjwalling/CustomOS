  3388                                  ;
  3389                                  ;       Routine:        PutMessage
  3390                                  ;
  3391                                  ;       Description:    This routine adda a message to the message queue.
  3392                                  ;
  3393                                  ;       In:             ECX     hi-order data word
  3394                                  ;                       EDX     lo-order data word
  3395                                  ;
  3396                                  ;       Out:            CY      0 = success
  3397                                  ;                               1 = fail: queue is full
  3398                                  ;
  3399                                  ;-----------------------------------------------------------------------------------------------------------------------
  3400 00000C71 1E                      PutMessage              push    ds                                              ;save non-volatile regs
  3401 00000C72 6A2C                                            push    ELDTMQ                                          ;load task message queue selector ...
  3402 00000C74 1F                                              pop     ds                                              ;... into data segment register
  3403 00000C75 A104000000                                      mov     eax,[MQTail]                                    ;tail ptr
  3404 00000C7A 833800                                          cmp     dword [eax],0                                   ;is queue full?
  3405 00000C7D F9                                              stc                                                     ;assume failure
  3406 00000C7E 7517                                            jne     .20                                             ;yes, cannot store
  3407 00000C80 8910                                            mov     [eax],edx                                       ;store lo-order data
  3408 00000C82 894804                                          mov     [eax+4],ecx                                     ;store hi-order data
  3409 00000C85 83C008                                          add     eax,8                                           ;next queue element adr
  3410 00000C88 25FC030000                                      and     eax,03FCh                                       ;at end of queue?
  3411 00000C8D 7502                                            jnz     .10                                             ;no, skip ahead
  3412 00000C8F B008                                            mov     al,8                                            ;reset to top of queue
  3413 00000C91 A304000000              .10                     mov     [MQTail],eax                                    ;save new tail ptr
  3414 00000C96 F8                                              clc                                                     ;indicate success
  3415 00000C97 1F                      .20                     pop     ds                                              ;restore non-volatile regs
  3416 00000C98 C3                                              ret                                                     ;return
