  4233 00000018 FFFF00000093CF00        			dq	00CF93000000FFFFh				;1C data
  4234 00000020 FF0F0050009B4000        			dq	00409B0050000FFFh				;24 code
  4235 00000028 FF07004800934000        			dq	00409300480007FFh				;2C message queue
