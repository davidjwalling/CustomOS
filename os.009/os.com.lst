  3301 00000C03 AC                      .10                     lodsb                                                   ;source byte
  3302 00000C04 84C0                                            test    al,al                                           ;end of string?
  3303 00000C06 7410                                            jz      .30                                             ;yes, branch
  3304 00000C08 3C39                                            cmp     al,'9'                                          ;hexadecimal?
  3305 00000C0A 7602                                            jna     .20                                             ;no, skip ahead
  3306 00000C0C 2C37                                            sub     al,037h                                         ;'A' = 41h, less 37h = 0Ah
  3307 00000C0E 83E00F                  .20                     and     eax,00Fh                                        ;remove ascii zone
  3308 00000C11 C1E204                                          shl     edx,4                                           ;previous total x 16
  3309 00000C14 01C2                                            add     edx,eax                                         ;add prior value x 16
  3310 00000C16 EBEB                                            jmp     .10                                             ;next
  3311 00000C18 89D0                    .30                     mov     eax,edx                                         ;result
  3312 00000C1A 5E                                              pop     esi                                             ;restore non-volatile regs
  3313 00000C1B C3                                              ret                                                     ;return
