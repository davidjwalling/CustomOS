  3364 00000C3D 51                                              push    ecx                                             ;
  3365 00000C3E 1E                                              push    ds                                              ;
  3366 00000C3F 6A2C                                            push    ELDTMQ                                          ;load message queue selector ...
  3367 00000C41 1F                                              pop     ds                                              ;... into data segment register
  3368 00000C42 8B1D00000000                                    mov     ebx,[MQHead]                                    ;head ptr
  3369 00000C48 8B03                                            mov     eax,[ebx]                                       ;lo-order 32 bits
  3370 00000C4A 8B5304                                          mov     edx,[ebx+4]                                     ;hi-order 32 bits
  3371 00000C4D 09D0                                            or      eax,edx                                         ;is queue empty?
  3372 00000C4F F9                                              stc                                                     ;assume queue is emtpy
  3373 00000C50 741B                                            jz      .20                                             ;yes, skip ahead
  3374 00000C52 31C9                                            xor     ecx,ecx                                         ;store zero
  3375 00000C54 890B                                            mov     [ebx],ecx                                       ;... in lo-order dword
  3376 00000C56 894B04                                          mov     [ebx+4],ecx                                     ;... in hi-order dword
  3377 00000C59 83C308                                          add     ebx,8                                           ;next queue element
  3378 00000C5C 81E3FC030000                                    and     ebx,03FCh                                       ;at end of queue?
  3379 00000C62 7502                                            jnz     .10                                             ;no, skip ahead
  3380 00000C64 B308                                            mov     bl,8                                            ;reset to 1st entry
  3381 00000C66 891D00000000            .10                     mov     [MQHead],ebx                                    ;save new head ptr
  3382 00000C6C F8                                              clc                                                     ;indicate message read
  3383 00000C6D 1F                      .20                     pop     ds                                              ;restore non-volatile regs
  3384 00000C6E 59                                              pop     ecx                                             ;
  3385 00000C6F 5B                                              pop     ebx                                             ;
  3386 00000C70 C3                                              ret                                                     ;return
