  2864 0000099E 89C8                    			mov	eax,ecx						;EAX = YYYY
  2865 000009A0 31C9                    			xor	ecx,ecx						;assume not leap year
  2866 000009A2 A803                    			test	al,00000011b					;multiple of four?
  2867 000009A4 750D                    			jnz	.no						;no, branch
  2868 000009A6 B264                    			mov	dl,100						;divisor
  2869 000009A8 F6F2                    			div	dl						;divide by 100
  2870 000009AA 84E4                    			test	ah,ah						;multiple of 100?
  2871 000009AC 7504                    			jnz	.yes						;yes, branch
  2872 000009AE A803                    			test	al,00000011b					;multiple of 400?
  2873 000009B0 7501                    			jnz	.no						;no, branch
  2874 000009B2 41                      .yes			inc	ecx						;indicate leap
  2875 000009B3 C3                      .no			ret							;
