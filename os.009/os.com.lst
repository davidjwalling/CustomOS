  3315                                  ;
  3316                                  ;       Routine:        UnsignedToHexadecimal
  3317                                  ;
  3318                                  ;       Description:    This routine creates an ASCIIZ string representing the hexadecimal value of binary input
  3319                                  ;
  3320                                  ;       Input:          DS:EDX  output buffer address
  3321                                  ;                       ECX     32-bit binary
  3322                                  ;
  3323                                  ;-----------------------------------------------------------------------------------------------------------------------
  3324 00000C1C 57                      UnsignedToHexadecimal   push    edi                                             ;store non-volatile regs
  3325 00000C1D 89D7                                            mov     edi,edx                                         ;output buffer address
  3326 00000C1F 89CA                                            mov     edx,ecx                                         ;32-bit unsigned
  3327 00000C21 31C9                                            xor     ecx,ecx                                         ;zero register
  3328 00000C23 B108                                            mov     cl,8                                            ;nybble count
  3329 00000C25 C1C204                  .10                     rol     edx,4                                           ;next hi-order nybble in bits 0-3
  3330 00000C28 88D0                                            mov     al,dl                                           ;????bbbb
  3331 00000C2A 240F                                            and     al,00Fh                                         ;mask out bits 4-7
  3332 00000C2C 0C30                                            or      al,030h                                         ;mask in ascii zone
  3333 00000C2E 3C3A                                            cmp     al,03Ah                                         ;A through F?
  3334 00000C30 7202                                            jb      .20                                             ;no, skip ahead
  3335 00000C32 0407                                            add     al,7                                            ;41h through 46h
  3336 00000C34 AA                      .20                     stosb                                                   ;store hexnum
  3337 00000C35 E2EE                                            loop    .10                                             ;next nybble
  3338 00000C37 30C0                                            xor     al,al                                           ;zero reg
  3339 00000C39 AA                                              stosb                                                   ;null terminate
  3340 00000C3A 5F                                              pop     edi                                             ;restore non-volatile regs
  3341 00000C3B C3                                              ret                                                     ;return
