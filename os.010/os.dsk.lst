  1405                                  ;
  1406                                  ;       Routine:        GetCPUType
  1407                                  ;
  1408                                  ;       Description:    The loader needs only to determine that the cpu is at least an 80386 or an equivalent. Note that
  1409                                  ;                       the CPUID instruction was not introduced until the SL-enhanced 80486 and Pentium processors, so
  1410                                  ;                       to distinguish whether we have at least an 80386, other means must be used.
  1411                                  ;
  1412                                  ;       Out:            AX      0 = 808x, v20, etc.
  1413                                  ;                               1 = 80186
  1414                                  ;                               2 = 80286
  1415                                  ;                               3 = 80386
  1416                                  ;
  1417                                  ;-----------------------------------------------------------------------------------------------------------------------
  1418 00000090 B001                    GetCPUType              mov     al,1                                            ;AL = 1
  1419 00000092 B120                                            mov     cl,32                                           ;shift count
  1420 00000094 D2E8                                            shr     al,cl                                           ;try a 32-bit shift
  1421 00000096 08C0                                            or      al,al                                           ;did the shift happen?
  1422 00000098 7415                                            jz      .10                                             ;yes, cpu is 808x, v20, etc.
  1423                                                          cpu     186
  1424 0000009A 54                                              push    sp                                              ;save stack pointer
  1425 0000009B 59                                              pop     cx                                              ;...into cx
  1426 0000009C 39E1                                            cmp     cx,sp                                           ;did sp decrement before push?
  1427 0000009E 750F                                            jne     .10                                             ;yes, cpu is 80186
  1428                                                          cpu     286
  1429 000000A0 40                                              inc     ax                                              ;AX = 2
  1430 000000A1 0F0106[C200]                                    sgdt    [cbLoaderGDT]                                   ;store gdt reg in work area
  1431 000000A6 8A0E[C700]                                      mov     cl,[cbLoaderGDTHiByte]                          ;CL = hi-order byte
  1432 000000AA FEC1                                            inc     cl                                              ;was hi-byte of GDTR 0xff?
  1433 000000AC 7401                                            jz      .10                                             ;yes, cpu is 80286
  1434 000000AE 40                                              inc     ax                                              ;AX = 3
  1435 000000AF C3                      .10                     ret                                                     ;return
  1436                                  ;-----------------------------------------------------------------------------------------------------------------------
  1437                                  ;
  1438                                  ;       Routine:        PutTTYString
  1439                                  ;
  1440                                  ;       Description:    This routine sends a NUL-terminated string of characters to the TTY output device. We use the
  1441                                  ;                       TTY output function of the BIOS video interrupt, passing the address of the string in DS:SI
  1442                                  ;                       and the BIOS teletype function code in AH. After a return from the BIOS interrupt, we repeat
  1443                                  ;                       for the next string character until a NUL is found. Note that we clear the direction flag (DF)
  1444                                  ;                       with CLD before the first LODSB. The direction flag is not guaranteed to be preseved between
  1445                                  ;                       calls within the OS. However, the "int" instruction does store the EFLAGS register on the
  1446                                  ;                       stack and restores it on return. Therefore, clearing the direction flag before subsequent calls
  1447                                  ;                       to LODSB is not needed.
  1448                                  ;
  1449                                  ;       In:             DS:SI   address of string
  1450                                  ;
  1451                                  ;       Out:            DF      0
  1452                                  ;                       ZF      1
  1453                                  ;                       AL      0
  1454                                  ;
  1455                                  ;-----------------------------------------------------------------------------------------------------------------------
  1456 000000B0 FC                      PutTTYString            cld                                                     ;forward strings
  1457 000000B1 AC                      .10                     lodsb                                                   ;load next byte at DS:SI in AL
  1458 000000B2 84C0                                            test    al,al                                           ;end of string?
  1459 000000B4 7406                                            jz      .20                                             ;... yes, exit our loop
  1460 000000B6 B40E                                            mov     ah,EBIOSFNTTYOUTPUT                             ;BIOS teletype function
  1461 000000B8 CD10                                            int     EBIOSINTVIDEO                                   ;call BIOS display interrupt
  1462 000000BA EBF5                                            jmp     .10                                             ;repeat until done
  1463 000000BC C3                      .20                     ret                                                     ;return
