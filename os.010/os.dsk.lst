  1202 00000006 056000078000                                    db      005h,060h,000h, 007h,080h,000h
  1203 0000000C 09A0000BC000                                    db      009h,0A0h,000h, 00Bh,0C0h,000h
  1204 00000012 0DE0000F0001                                    db      00Dh,0E0h,000h, 00Fh,000h,001h
  1205 00000018 112001134001                                    db      011h,020h,001h, 013h,040h,001h
  1206 0000001E 156001178001                                    db      015h,060h,001h, 017h,080h,001h
  1207 00000024 19A0011BC001                                    db      019h,0A0h,001h, 01Bh,0C0h,001h
  1208 0000002A 1DE0011F0002                                    db      01Dh,0E0h,001h, 01Fh,000h,002h
  1209 00000030 212002234002                                    db      021h,020h,002h, 023h,040h,002h
  1210 00000036 256002278002                                    db      025h,060h,002h, 027h,080h,002h
  1211 0000003C 29A0022BF0FF                                    db      029h,0A0h,002h, 02Bh,0F0h,0FFh
