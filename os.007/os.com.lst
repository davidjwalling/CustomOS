  2431 0000065F 57                                              push    edi                                             ;
  2432 00000660 06                                              push    es                                              ;
  2433 00000661 1E                                              push    ds                                              ;copy DS
  2434 00000662 07                                              pop     es                                              ;... to ES
  2435 00000663 89D6                                            mov     esi,edx                                         ;first source address
  2436 00000665 89DF                                            mov     edi,ebx                                         ;second source address
  2437 00000667 FC                                              cld                                                     ;forward strings
  2438 00000668 F3A6                                            rep     cmpsb                                           ;compare bytes
  2439 0000066A B000                                            mov     al,0                                            ;default result
  2440 0000066C 7406                                            jz      .10                                             ;branch if arrays equal
  2441 0000066E B001                                            mov     al,1                                            ;positive result
  2442 00000670 7302                                            jnc     .10                                             ;branch if target > source
  2443 00000672 B0FF                                            mov     al,-1                                           ;negative result
  2444 00000674 0FBEC8                  .10                     movsx   ecx,al                                          ;extend sign
  2445 00000677 07                                              pop     es                                              ;restore non-volatile regs
  2446 00000678 5F                                              pop     edi                                             ;
  2447 00000679 5E                                              pop     esi                                             ;
  2448 0000067A C3                                              ret                                                     ;return
