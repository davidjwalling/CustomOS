  1352                                  ;
  1353                                  ;       Routine:        GetCPUType
  1354                                  ;
  1355                                  ;       Description:    The loader needs only to determine that the cpu is at least an 80386 or an equivalent. Note that
  1356                                  ;                       the CPUID instruction was not introduced until the SL-enhanced 80486 and Pentium processors, so
  1357                                  ;                       to distinguish whether we have at least an 80386, other means must be used.
  1358                                  ;
  1359                                  ;       Out:            AX      0 = 808x, v20, etc.
  1360                                  ;                               1 = 80186
  1361                                  ;                               2 = 80286
  1362                                  ;                               3 = 80386
  1363                                  ;
  1364                                  ;-----------------------------------------------------------------------------------------------------------------------
  1365 00000090 B001                    GetCPUType              mov     al,1                                            ;AL = 1
  1366 00000092 B120                                            mov     cl,32                                           ;shift count
  1367 00000094 D2E8                                            shr     al,cl                                           ;try a 32-bit shift
  1368 00000096 08C0                                            or      al,al                                           ;did the shift happen?
  1369 00000098 7415                                            jz      .10                                             ;yes, cpu is 808x, v20, etc.
  1370                                                          cpu     186
  1371 0000009A 54                                              push    sp                                              ;save stack pointer
  1372 0000009B 59                                              pop     cx                                              ;...into cx
  1373 0000009C 39E1                                            cmp     cx,sp                                           ;did sp decrement before push?
  1374 0000009E 750F                                            jne     .10                                             ;yes, cpu is 80186
  1375                                                          cpu     286
  1376 000000A0 40                                              inc     ax                                              ;AX = 2
  1377 000000A1 0F0106[C200]                                    sgdt    [cbLoaderGDT]                                   ;store gdt reg in work area
  1378 000000A6 8A0E[C700]                                      mov     cl,[cbLoaderGDTHiByte]                          ;CL = hi-order byte
  1379 000000AA FEC1                                            inc     cl                                              ;was hi-byte of GDTR 0xff?
  1380 000000AC 7401                                            jz      .10                                             ;yes, cpu is 80286
  1381 000000AE 40                                              inc     ax                                              ;AX = 3
  1382 000000AF C3                      .10                     ret                                                     ;return
