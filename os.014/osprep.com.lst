  4088                                  ;
  4089                                  ;       Routine:        GetConsoleChar
  4090                                  ;
  4091                                  ;       Description:    This routine waits for EMSGKEYCHAR message and return character code.
  4092                                  ;
  4093                                  ;       Out:            AL      ASCII character code
  4094                                  ;                       AH      keyboard scan code
  4095                                  ;
  4096                                  ;-----------------------------------------------------------------------------------------------------------------------
  4097                                  GetConsoleChar.10       call    Yield                                           ;pass control or halt
  4098                                  GetConsoleChar          call    GetMessage                                      ;get the next message
  4099                                                          or      eax,eax                                         ;do we have a message?
  4100                                                          jz      GetConsoleChar.10                               ;no, skip ahead
  4101                                                          push    eax                                             ;save key codes
  4102                                                          and     eax,0FFFF0000h                                  ;mask for message type
  4103                                                          cmp     eax,EMSGKEYCHAR                                 ;key-char message?
  4104                                                          pop     eax                                             ;restore key codes
  4105                                                          jne     GetConsoleChar                                  ;no, try again
  4106                                                          and     eax,0000ffffh                                   ;mask for key codes
  4107                                                          ret                                                     ;return
  4108                                  ;-----------------------------------------------------------------------------------------------------------------------
  4109                                  ;
  4110                                  ;       Routine:        GetConsoleString
  4111                                  ;
  4112                                  ;       Description:    This routine accepts keyboard input into a buffer.
  4113                                  ;
  4114                                  ;       In:             DS:EDX  target buffer address
  4115                                  ;                       ECX     maximum number of characters to accept
  4116                                  ;                       BH      echo to terminal
  4117                                  ;                       BL      terminating character
  4118                                  ;
  4119                                  ;-----------------------------------------------------------------------------------------------------------------------
  4120                                  GetConsoleString        push    ecx                                             ;save non-volatile regs
  4121                                                          push    esi                                             ;
  4122                                                          push    edi                                             ;
  4123                                                          push    es                                              ;
  4124                                                          push    ds                                              ;load data segment selector ...
  4125                                                          pop     es                                              ;... into extra segment register
  4126                                                          mov     edi,edx                                         ;edi = target buffer
  4127                                                          push    ecx                                             ;save maximum characters
  4128                                                          xor     al,al                                           ;zero register
  4129                                                          cld                                                     ;forward strings
  4130                                                          rep     stosb                                           ;zero fill buffer
  4131                                                          pop     ecx                                             ;maximum characters
  4132                                                          mov     edi,edx                                         ;edi = target buffer
  4133                                                          mov     esi,edx                                         ;esi = target buffer
  4134                                  .10                     jecxz   .50                                             ;exit if max-length is zero
  4135                                  .20                     call    GetConsoleChar                                  ;al = next input char
  4136                                                          cmp     al,bl                                           ;is this the terminator?
  4137                                                          je      .50                                             ;yes, exit
  4138                                                          cmp     al,EASCIIBACKSPACE                              ;is this a backspace?
  4139                                                          jne     .30                                             ;no, skip ahead
  4140                                                          cmp     esi,edi                                         ;at start of buffer?
  4141                                                          je      .20                                             ;yes, get next character
  4142                                                          dec     edi                                             ;backup target pointer
  4143                                                          mov     byte [edi],0                                    ;zero previous character
  4144                                                          inc     ecx                                             ;increment remaining chars
  4145                                                          test    bh,1                                            ;echo to console?
  4146                                                          jz      .20                                             ;no, get next character
  4147                                                          call    PreviousConsoleColumn                           ;backup console position
  4148                                                          mov     al,EASCIISPACE                                  ;ASCII space
  4149                                                          call    PutConsoleChar                                  ;write space to console
  4150                                                          call    PlaceCursor                                     ;position the cursor
  4151                                                          jmp     .20                                             ;get next character
  4152                                  .30                     cmp     al,EASCIISPACE                                  ;printable? (lower bounds)
  4153                                                          jb      .20                                             ;no, get another character
  4154                                                          cmp     al,EASCIITILDE                                  ;printable? (upper bounds)
  4155                                                          ja      .20                                             ;no, get another character
  4156                                                          stosb                                                   ;store character in buffer
  4157                                                          test    bh,1                                            ;echo to console?
  4158                                                          jz      .40                                             ;no, skip ahead
  4159                                                          call    PutConsoleChar                                  ;write character to console
  4160                                                          call    NextConsoleColumn                               ;advance console position
  4161                                                          call    PlaceCursor                                     ;position the cursor
  4162                                  .40                     dec     ecx                                             ;decrement remaining chars
  4163                                                          jmp     .10                                             ;next
  4164                                  .50                     xor     al,al                                           ;null
  4165                                                          stosb                                                   ;terminate buffer
  4166                                                          pop     es                                              ;restore non-volatile regs
  4167                                                          pop     edi                                             ;
  4168                                                          pop     esi                                             ;
  4169                                                          pop     ecx                                             ;
  4170                                                          ret                                                     ;return
