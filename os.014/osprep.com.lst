  4363                                  ;
  4364                                  ;       Routine:        PutConsoleOIAShift
  4365                                  ;
  4366                                  ;       Description:    This routine updates the shift/ctrl/alt/lock indicators in the operator information area (OIA).
  4367                                  ;
  4368                                  ;       In:             BL      shift flags
  4369                                  ;                       BH      lock flags
  4370                                  ;                       DS      OS data selector
  4371                                  ;
  4372                                  ;-----------------------------------------------------------------------------------------------------------------------
  4373                                  PutConsoleOIAShift      push    ecx                                             ;save non-volatile regs
  4374                                                          push    es                                              ;
  4375                                                          push    EGDTCGA                                         ;load CGA selector ...
  4376                                                          pop     es                                              ;... into ES register
  4377                                                          mov     ch,ECONOIAROW                                   ;OIA row
  4378                                                          mov     al,EASCIISPACE                                  ;space is default character
  4379                                                          test    bl,EKEYFSHIFTLEFT                               ;left-shift indicated?
  4380                                                          jz      .10                                             ;no, skip ahead
  4381                                                          mov     al,'S'                                          ;yes, indicate with 'S'
  4382                                  .10                     mov     cl,14                                           ;indicator column
  4383                                                          call    SetConsoleChar                                  ;display ASCII character
  4384                                                          mov     al,EASCIISPACE                                  ;ASCII space
  4385                                                          test    bl,EKEYFSHIFTRIGHT                              ;right-shift indicated?
  4386                                                          jz      .20                                             ;no, skip ahead
  4387                                                          mov     al,'S'                                          ;yes, indicate with 'S'
  4388                                  .20                     mov     cl,64                                           ;indicator column
  4389                                                          call    SetConsoleChar                                  ;display ASCII character
  4390                                                          mov     al,EASCIISPACE                                  ;ASCII space
  4391                                                          test    bl,EKEYFCTRLLEFT                                ;left-ctrl indicated?
  4392                                                          jz      .30                                             ;no, skip ahead
  4393                                                          mov     al,'C'                                          ;yes, indicate with 'C'
  4394                                  .30                     mov     cl,15                                           ;indicator column
  4395                                                          call    SetConsoleChar                                  ;display ASCII character
  4396                                                          mov     al,EASCIISPACE                                  ;ASCII space
  4397                                                          test    bl,EKEYFCTRLRIGHT                               ;right-ctrl indicated?
  4398                                                          jz      .40                                             ;no, skip ahead
  4399                                                          mov     al,'C'                                          ;yes, indicate with 'C'
  4400                                  .40                     mov     cl,63                                           ;indicator column
  4401                                                          call    SetConsoleChar                                  ;display ASCII character
  4402                                                          mov     al,EASCIISPACE                                  ;ASCII space
  4403                                                          test    bl,EKEYFALTLEFT                                 ;left-alt indicated?
  4404                                                          jz      .50                                             ;no, skip ahead
  4405                                                          mov     al,'A'                                          ;yes, indicate with 'A'
  4406                                  .50                     mov     cl,16                                           ;indicator column
  4407                                                          call    SetConsoleChar                                  ;display ASCII character
  4408                                                          mov     al,EASCIISPACE                                  ;ASCII space
  4409                                                          test    bl,EKEYFALTRIGHT                                ;right-alt indicated?
  4410                                                          jz      .60                                             ;no, skip ahead
  4411                                                          mov     al,'A'                                          ;yes, indicate with 'A'
  4412                                  .60                     mov     cl,62                                           ;indicator column
  4413                                                          call    SetConsoleChar                                  ;display ASCII character
  4414                                                          mov     al,EASCIISPACE                                  ;ASCII space
  4415                                                          test    bh,EKEYFLOCKCAPS                                ;caps-lock indicated?
  4416                                                          jz      .70                                             ;no, skip ahead
  4417                                                          mov     al,'C'                                          ;yes, indicate with 'C'
  4418                                  .70                     mov     cl,78                                           ;indicator column
  4419                                                          call    SetConsoleChar                                  ;display ASCII character
  4420                                                          mov     al,EASCIISPACE                                  ;ASCII space
  4421                                                          test    bh,EKEYFLOCKNUM                                 ;num-lock indicated?
  4422                                                          jz      .80                                             ;no, skip ahead
  4423                                                          mov     al,'N'                                          ;yes, indicate with 'N'
  4424                                  .80                     mov     cl,77                                           ;indicator column
  4425                                                          call    SetConsoleChar                                  ;display ASCII character
  4426                                                          mov     al,EASCIISPACE                                  ;ASCII space
  4427                                                          test    bh,EKEYFLOCKSCROLL                              ;scroll-lock indicated?
  4428                                                          jz      .90                                             ;no, skip ahead
  4429                                                          mov     al,'S'                                          ;yes, indicate with 'S'
  4430                                  .90                     mov     cl,76                                           ;indicator column
  4431                                                          call    SetConsoleChar                                  ;display ASCII character
  4432                                                          pop     es                                              ;restore non-volatile regs
  4433                                                          pop     ecx                                             ;
  4434                                                          ret                                                     ;return
