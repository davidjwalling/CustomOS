  5408                                  ;
  5409                                  ;       Routine:        ConInitializeNetwork
  5410                                  ;
  5411                                  ;       Description:    This routine initializes console network variables.
  5412                                  ;
  5413                                  ;-----------------------------------------------------------------------------------------------------------------------
  5414                                  ConInitializeNetwork    push    ebx                                             ;save non-volatile regs
  5415                                                          push    ecx                                             ;
  5416                                                          push    esi                                             ;
  5417                                                          push    edi                                             ;
  5418                                  ;
  5419                                  ;       Initialize ETHER structure.
  5420                                  ;
  5421                                                          mov     ebx,wsConsoleEther                              ;ETHER structure address
  5422                                                          call    ConInitEtherContext                             ;initialize ETHER struct
  5423                                                          mov     esi,ebx                                         ;ETHER structure address
  5424                                  ;
  5425                                  ;       Initialize variables.
  5426                                  ;       Construct PCI selector.
  5427                                  ;       Read PCI configuration data.
  5428                                  ;
  5429                                                          mov     ebx,wsConsolePCI                                ;PCI structure address
  5430                                                          call    ConInitPCIContext                               ;initialize PCI struct
