  4437                                  ;       Routine:        PutConsoleString
  4438                                  ;
  4439                                  ;       Description:    This routine writes a sequence of ASCII characters to the console until null and updates the
  4440                                  ;                       console position as needed.
  4441                                  ;
  4442                                  ;       In:             EDX     source address
  4443                                  ;                       DS      OS data selector
  4444                                  ;
  4445                                  ;-----------------------------------------------------------------------------------------------------------------------
  4446                                  PutConsoleString        push    esi                                             ;save non-volatile regs
  4447                                                          mov     esi,edx                                         ;source address
  4448                                                          cld                                                     ;forward strings
  4449                                  .10                     lodsb                                                   ;ASCII character
  4450                                                          or      al,al                                           ;end of string?
  4451                                                          jz      .40                                             ;yes, skip ahead
  4452                                                          cmp     al,EASCIIRETURN                                 ;carriage return?
  4453                                                          jne     .20                                             ;no, skip ahead
  4454                                                          call    FirstConsoleColumn                              ;move to start of row
  4455                                                          jmp     .10                                             ;next character
  4456                                  .20                     cmp     al,EASCIILINEFEED                               ;line feed?
  4457                                                          jne     .30                                             ;no, skip ahead
  4458                                                          call    NextConsoleRow                                  ;move to next row
  4459                                                          jmp     .10                                             ;next character
  4460                                  .30                     call    PutConsoleChar                                  ;output character to console
  4461                                                          call    NextConsoleColumn                               ;advance to next column
  4462                                                          jmp     .10                                             ;next character
  4463                                  .40                     pop     esi                                             ;restore non-volatile regs
  4464                                                          ret                                                     ;return
