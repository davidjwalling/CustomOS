  5594                                  ;       Routine:        ConBuildPCISelector
  5595                                  ;
  5596                                  ;       Description:    This routine sets the selector member of a PCI based on the bus, device and function.
  5597                                  ;
  5598                                  ;       In:             DS:EBX  PCI structure address
  5599                                  ;
  5600                                  ;       Out:            EAX     bus, device, function, register selector
  5601                                  ;
  5602                                  ;-----------------------------------------------------------------------------------------------------------------------
  5603                                  ConBuildPCISelector     mov     ah,[ebx+PCI.bus]                                ;AH = bbbb bbbb
  5604                                                          mov     dl,[ebx+PCI.device]                             ;DL = ???d dddd
  5605                                                          shl     dl,3                                            ;DL = dddd d000
  5606                                                          mov     al,[ebx+PCI.function]                           ;AL = ???? ?fff
  5607                                                          and     al,007h                                         ;AL = 0000 0fff
  5608                                                          or      al,dl                                           ;AL = dddd dfff
  5609                                                          movzx   eax,ax                                          ;0000 0000 0000 0000 bbbb bbbb dddd dfff
  5610                                                          shl     eax,8                                           ;0000 0000 bbbb bbbb dddd dfff 0000 0000
  5611                                                          mov     al,[ebx+PCI.register]                           ;0000 0000 bbbb bbbb dddd dfff rrrr rrrr
  5612                                                          or      eax,080000000h                                  ;1000 0000 bbbb bbbb dddd dfff rrrr rrrr
  5613                                                          mov     [ebx+PCI.selector],eax                          ;set selector
  5614                                                          ret                                                     ;return
