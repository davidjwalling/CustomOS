  4489                                  ;       Routine:        ByteToHex
  4490                                  ;
  4491                                  ;       Description:    This routine creates an ASCIIZ string representing the hexadecimal value of 8-bit binary input.
  4492                                  ;
  4493                                  ;       Input:          DS:ESI  source address of byte
  4494                                  ;                       ES:EDI  target address of ASCIIZ string
  4495                                  ;
  4496                                  ;-----------------------------------------------------------------------------------------------------------------------
  4497                                  ByteToHex               lodsb                                                   ;input byte
  4498                                                          push    eax                                             ;save input byte
  4499                                                          shr     al,4                                            ;hi-order nybble
  4500                                                          call    .10                                             ;make ASCII and store
  4501                                                          pop     eax                                             ;input byte
  4502                                                          and     al,00Fh                                         ;lo-order nybble
  4503                                  .10                     or      al,030h                                         ;ASCII numeral zone
  4504                                                          cmp     al,03Ah                                         ;'A' through 'F'?
  4505                                                          jb      .20                                             ;no, branch
  4506                                                          add     al,7                                            ;ajdust for 'A' through 'F'
  4507                                  .20                     stosb                                                   ;store to output buffer
  4508                                                          ret                                                     ;return
