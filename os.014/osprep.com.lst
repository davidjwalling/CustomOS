  2404                                                          xor     ecx,ecx                                         ;zero register
  2405                                                          mov     cl,dl                                           ;width, excluding border
  2406                                                          rep     stosw                                           ;display horizontal border
  2407                                                          mov     al,EASCIIBORDSGLUPRRGT                          ;upper-right single border
