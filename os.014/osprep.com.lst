  5018                                  ;
  5019                                  ;       Routine:        ReadRealTimeClock
  5020                                  ;
  5021                                  ;       Description:    This routine gets current date time from the real-time clock.
  5022                                  ;
  5023                                  ;       In:             DS:EBX  DATETIME structure
  5024                                  ;
  5025                                  ;-----------------------------------------------------------------------------------------------------------------------
  5026                                  ReadRealTimeClock       push    esi                                             ;save non-volatile regs
  5027                                                          push    edi                                             ;
  5028                                                          push    es                                              ;
  5029                                                          push    ds                                              ;store data selector ...
  5030                                                          pop     es                                              ;... in es register
  5031                                                          mov     edi,ebx                                         ;date-time structure
  5032                                                          mov     al,ERTCSECONDREG                                ;second register
  5033                                                          out     ERTCREGPORT,al                                  ;select second register
  5034                                                          in      al,ERTCDATAPORT                                 ;read second register
  5035                                                          cld                                                     ;forward strings
  5036                                                          stosb                                                   ;store second value
  5037                                                          mov     al,ERTCMINUTEREG                                ;minute register
  5038                                                          out     ERTCREGPORT,al                                  ;select minute register
  5039                                                          in      al,ERTCDATAPORT                                 ;read minute register
  5040                                                          stosb                                                   ;store minute value
  5041                                                          mov     al,ERTCHOURREG                                  ;hour register
  5042                                                          out     ERTCREGPORT,al                                  ;select hour register
  5043                                                          in      al,ERTCDATAPORT                                 ;read hour register
  5044                                                          stosb                                                   ;store hour value
  5045                                                          mov     al,ERTCWEEKDAYREG                               ;weekday register
  5046                                                          out     ERTCREGPORT,al                                  ;select weekday register
  5047                                                          in      al,ERTCDATAPORT                                 ;read weekday register
  5048                                                          stosb                                                   ;store weekday value
  5049                                                          mov     al,ERTCDAYREG                                   ;day register
  5050                                                          out     ERTCREGPORT,al                                  ;select day register
  5051                                                          in      al,ERTCDATAPORT                                 ;read day register
  5052                                                          stosb                                                   ;store day value
  5053                                                          mov     al,ERTCMONTHREG                                 ;month register
  5054                                                          out     ERTCREGPORT,al                                  ;select month register
  5055                                                          in      al,ERTCDATAPORT                                 ;read month register
  5056                                                          stosb                                                   ;store month value
  5057                                                          mov     al,ERTCYEARREG                                  ;year register
  5058                                                          out     ERTCREGPORT,al                                  ;select year register
  5059                                                          in      al,ERTCDATAPORT                                 ;read year register
  5060                                                          stosb                                                   ;store year value
  5061                                                          mov     al,ERTCCENTURYREG                               ;century register
  5062                                                          out     ERTCREGPORT,al                                  ;select century register
  5063                                                          in      al,ERTCDATAPORT                                 ;read century register
  5064                                                          stosb                                                   ;store century value
  5065                                                          mov     al,ERTCSTATUSREG                                ;status register
  5066                                                          out     ERTCREGPORT,al                                  ;select status register
  5067                                                          in      al,ERTCDATAPORT                                 ;read status register
  5068                                                          test    al,ERTCBINARYVALS                               ;test if values are binary
  5069                                                          jnz     .20                                             ;skip ahead if binary values
  5070                                                          mov     esi,ebx                                         ;date-time structure address
  5071                                                          mov     edi,ebx                                         ;date-time structure address
  5072                                                          mov     ecx,8                                           ;loop counter
  5073                                  .10                     lodsb                                                   ;BCD value
  5074                                                          mov     ah,al                                           ;BCD value
  5075                                                          and     al,00001111b                                    ;low-order decimal zone
  5076                                                          and     ah,11110000b                                    ;hi-order decimal zone
  5077                                                          shr     ah,1                                            ;hi-order decimal * 8
  5078                                                          add     al,ah                                           ;low-order + hi-order * 8
  5079                                                          shr     ah,2                                            ;hi-order decimal * 2
  5080                                                          add     al,ah                                           ;low-order + hi-order * 10
  5081                                                          stosb                                                   ;replace BCD with binary
  5082                                                          loop    .10                                             ;next value
  5083                                  .20                     pop     es                                              ;restore non-volatile regs
  5084                                                          pop     edi                                             ;
  5085                                                          pop     esi                                             ;
  5086                                                          ret                                                     ;return
