  4231                                  ;
  4232                                  ;       Routine:        PutConsoleChar
  4233                                  ;
  4234                                  ;       Description:    This routine writes one ASCII character to the console screen.
  4235                                  ;
  4236                                  ;       In:             AL      ASCII character
  4237                                  ;                       DS      OS data selector
  4238                                  ;
  4239                                  ;-----------------------------------------------------------------------------------------------------------------------
  4240                                  PutConsoleChar          push    ecx                                             ;save non-volatile regs
  4241                                                          push    es                                              ;
  4242                                                          push    EGDTCGA                                         ;load CGA selector ...
  4243                                                          pop     es                                              ;... into extra segment reg
  4244                                                          mov     cl,[wbConsoleColumn]                            ;column
  4245                                                          mov     ch,[wbConsoleRow]                               ;row
  4246                                                          call    SetConsoleChar                                  ;put character at row, column
  4247                                                          pop     es                                              ;restore non-volatile regs
  4248                                                          pop     ecx                                             ;
  4249                                                          ret                                                     ;return
