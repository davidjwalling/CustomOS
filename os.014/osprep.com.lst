  4031                                  ;       Routine:        UpperCaseString
  4032                                  ;
  4033                                  ;       Description:    This routine places all characters in the given string to upper case.
  4034                                  ;
  4035                                  ;       In:             DS:EDX  string address
  4036                                  ;
  4037                                  ;       Out:            EDX     string address
  4038                                  ;
  4039                                  ;-----------------------------------------------------------------------------------------------------------------------
  4040                                  UpperCaseString         push    esi                                             ;save non-volatile regs
  4041                                                          mov     esi,edx                                         ;string address
  4042                                                          cld                                                     ;forward strings
  4043                                  .10                     lodsb                                                   ;string character
  4044                                                          test    al,al                                           ;null?
  4045                                                          jz      .20                                             ;yes, skip ahead
  4046                                                          cmp     al,EASCIILOWERA                                 ;lower-case? (lower bounds)
  4047                                                          jb      .10                                             ;no, continue
  4048                                                          cmp     al,EASCIILOWERZ                                 ;lower-case? (upper bounds)
  4049                                                          ja      .10                                             ;no, continue
  4050                                                          and     al,EASCIICASEMASK                               ;mask for upper case
  4051                                                          mov     [esi-1],al                                      ;upper character
  4052                                                          jmp     .10                                             ;continue
  4053                                  .20                     pop     esi                                             ;restore non-volatile regs
  4054                                                          ret                                                     ;return
