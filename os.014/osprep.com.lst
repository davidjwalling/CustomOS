  4604                                  ;
  4605                                  ;       Routine:        UnsignedToDecimalString
  4606                                  ;
  4607                                  ;       Description:    This routine creates an ASCIIZ string representing the decimal value of 32-bit binary input.
  4608                                  ;
  4609                                  ;       Input:          BH      flags           bit 0: 1 = trim leading zeros
  4610                                  ;                                               bit 1: 1 = include comma grouping delimiters
  4611                                  ;                                               bit 4: 1 = non-zero digit found (internal)
  4612                                  ;                       ECX     32-bit binary
  4613                                  ;                       DS:EDX  output buffer address
  4614                                  ;
  4615                                  ;-----------------------------------------------------------------------------------------------------------------------
  4616                                  UnsignedToDecimalString push    ebx                                             ;save non-volatile regs
  4617                                                          push    ecx                                             ;
  4618                                                          push    edi                                             ;
  4619                                                          push    es                                              ;
  4620                                                          push    ds                                              ;load data selector
  4621                                                          pop     es                                              ;... into extra segment reg
  4622                                                          mov     edi,edx                                         ;output buffer address
  4623                                                          and     bh,00001111b                                    ;zero internal flags
  4624                                                          mov     edx,ecx                                         ;binary
  4625                                                          mov     ecx,1000000000                                  ;10^9 divisor
  4626                                                          call    .30                                             ;divide and store
  4627                                                          mov     ecx,100000000                                   ;10^8 divisor
  4628                                                          call    .10                                             ;divide and store
  4629                                                          mov     ecx,10000000                                    ;10^7 divisor
  4630                                                          call    .30                                             ;divide and store
  4631                                                          mov     ecx,1000000                                     ;10^6 divisor
  4632                                                          call    .30                                             ;divide and store
  4633                                                          mov     ecx,100000                                      ;10^5 divisor
  4634                                                          call    .10                                             ;divide and store
  4635                                                          mov     ecx,10000                                       ;10^4 divisor
  4636                                                          call    .30                                             ;divide and store
  4637                                                          mov     ecx,1000                                        ;10^3 divisor
  4638                                                          call    .30                                             ;divide and store
  4639                                                          mov     ecx,100                                         ;10^2 divisor
  4640                                                          call    .10                                             ;divide and store
  4641                                                          mov     ecx,10                                          ;10^2 divisor
  4642                                                          call    .30                                             ;divide and store
  4643                                                          mov     eax,edx                                         ;10^1 remainder
  4644                                                          call    .40                                             ;store
  4645                                                          xor     al,al                                           ;null terminator
  4646                                                          stosb                                                   ;store in output buffer
  4647                                                          pop     es                                              ;restore non-volatile regs
  4648                                                          pop     edi                                             ;
  4649                                                          pop     ecx                                             ;
  4650                                                          pop     ebx                                             ;
  4651                                                          ret                                                     ;return
  4652                                  .10                     test    bh,00000010b                                    ;comma group delims?
  4653                                                          jz      .30                                             ;no, branch
  4654                                                          test    bh,00000001b                                    ;trim leading zeros?
  4655                                                          jz      .20                                             ;no, store delim
  4656                                                          test    bh,00010000b                                    ;non-zero found?
  4657                                                          jz      .30                                             ;no, branch
  4658                                  .20                     mov     al,','                                          ;delimiter
  4659                                                          stosb                                                   ;store delimiter
  4660                                  .30                     mov     eax,edx                                         ;lo-orer dividend
  4661                                                          xor     edx,edx                                         ;zero hi-order
  4662                                                          div     ecx                                             ;divide by power of 10
  4663                                                          test    al,al                                           ;zero?
  4664                                                          jz      .50                                             ;yes, branch
  4665                                                          or      bh,00010000b                                    ;non-zero found
  4666                                  .40                     or      al,030h                                         ;ASCII zone
  4667                                                          stosb                                                   ;store digit
  4668                                                          ret                                                     ;return
  4669                                  .50                     test    bh,00000001b                                    ;trim leading zeros?
  4670                                                          jz      .40                                             ;no, store and continue
  4671                                                          test    bh,00010000b                                    ;non-zero found?
  4672                                                          jnz     .40                                             ;yes, store and continue
  4673                                                          ret                                                     ;return
