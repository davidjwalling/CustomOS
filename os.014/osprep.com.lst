  4711                                  ;
  4712                                  ;       Routine:        GetMessage
  4713                                  ;
  4714                                  ;       Description:    This routine reads and removes a message from the message queue.
  4715                                  ;
  4716                                  ;       Out:            EAX     lo-order message data
  4717                                  ;                       EDX     hi-order message data
  4718                                  ;
  4719                                  ;                       CY      0 = message read
  4720                                  ;                               1 = no message to read
  4721                                  ;
  4722                                  ;-----------------------------------------------------------------------------------------------------------------------
  4723                                  GetMessage              push    ebx                                             ;save non-volatile regs
  4724                                                          push    ecx                                             ;
  4725                                                          push    ds                                              ;
  4726                                                          push    ELDTMQ                                          ;load message queue selector ...
  4727                                                          pop     ds                                              ;... into data segment register
  4728                                                          mov     ebx,[MQHead]                                    ;head ptr
  4729                                                          mov     eax,[ebx]                                       ;lo-order 32 bits
  4730                                                          mov     edx,[ebx+4]                                     ;hi-order 32 bits
  4731                                                          or      eax,edx                                         ;is queue empty?
  4732                                                          stc                                                     ;assume queue is emtpy
  4733                                                          jz      .20                                             ;yes, skip ahead
  4734                                                          xor     ecx,ecx                                         ;store zero
  4735                                                          mov     [ebx],ecx                                       ;... in lo-order dword
  4736                                                          mov     [ebx+4],ecx                                     ;... in hi-order dword
  4737                                                          add     ebx,8                                           ;next queue element
  4738                                                          and     ebx,03FCh                                       ;at end of queue?
  4739                                                          jnz     .10                                             ;no, skip ahead
  4740                                                          mov     bl,8                                            ;reset to 1st entry
  4741                                  .10                     mov     [MQHead],ebx                                    ;save new head ptr
  4742                                                          clc                                                     ;indicate message read
  4743                                  .20                     pop     ds                                              ;restore non-volatile regs
  4744                                                          pop     ecx                                             ;
  4745                                                          pop     ebx                                             ;
  4746                                                          ret                                                     ;return
  4747                                  ;-----------------------------------------------------------------------------------------------------------------------
  4748                                  ;
  4749                                  ;       Routine:        PutMessage
  4750                                  ;
  4751                                  ;       Description:    This routine adda a message to the message queue.
  4752                                  ;
  4753                                  ;       In:             ECX     hi-order data word
  4754                                  ;                       EDX     lo-order data word
  4755                                  ;
  4756                                  ;       Out:            CY      0 = success
  4757                                  ;                               1 = fail: queue is full
  4758                                  ;
  4759                                  ;-----------------------------------------------------------------------------------------------------------------------
  4760                                  PutMessage              push    ds                                              ;save non-volatile regs
  4761                                                          push    ELDTMQ                                          ;load task message queue selector ...
  4762                                                          pop     ds                                              ;... into data segment register
  4763                                                          mov     eax,[MQTail]                                    ;tail ptr
  4764                                                          cmp     dword [eax],0                                   ;is queue full?
  4765                                                          stc                                                     ;assume failure
  4766                                                          jne     .20                                             ;yes, cannot store
  4767                                                          mov     [eax],edx                                       ;store lo-order data
  4768                                                          mov     [eax+4],ecx                                     ;store hi-order data
  4769                                                          add     eax,8                                           ;next queue element adr
  4770                                                          and     eax,03FCh                                       ;at end of queue?
  4771                                                          jnz     .10                                             ;no, skip ahead
  4772                                                          mov     al,8                                            ;reset to top of queue
  4773                                  .10                     mov     [MQTail],eax                                    ;save new tail ptr
  4774                                                          clc                                                     ;indicate success
  4775                                  .20                     pop     ds                                              ;restore non-volatile regs
  4776                                                          ret                                                     ;return
