  5468                                                          mov     ecx,eax                                         ;ethernet adapter PCI selector
  5469                                                          mov     edx,czEthernetSelector                          ;message label
  5470                                                          call    ConPutLabeledHexValue                           ;output labeled message to console
  5471                                  
  5472                                                          mov     eax,[esi+ETHER.selector]                        ;ethernet adapter PCI selector
  5473                                                          xor     al,al                                           ;register 0
  5474                                                          call    ConReadPCIRegister                              ;EAX = device id | vendor id
  5475                                                          mov     [esi+ETHER.devicevendor],eax                    ;save device id | vendor id
  5476                                                          mov     ecx,eax                                         ;device id | vendor id
  5477                                                          mov     edx,czEthernetDeviceVendor                      ;message label
  5478                                                          call    ConPutLabeledHexValue                           ;output labeled message to console
  5479                                  
  5480                                                          mov     eax,[esi+ETHER.selector]                        ;ethernet adapter PCI selector
  5481                                                          mov     al,004h                                         ;status and command register
  5482                                                          call    ConReadPCIRegister                              ;EAX = status | command
  5483                                                          mov     [esi+ETHER.statuscommand],eax                   ;save status | command
  5484                                                          mov     ecx,eax                                         ;status | command
  5485                                                          mov     edx,czEthernetStatusCommand                     ;message label
  5486                                                          call    ConPutLabeledHexLine                            ;output labeled message to console
  5487                                  
  5488                                                          mov     eax,[esi+ETHER.selector]                        ;ethernet adapter PCI selector
  5489                                                          mov     al,010h                                         ;mapped memory I/O (BAR0) register
  5490                                                          call    ConReadPCIRegister                              ;EAX = MMIO
  5491                                                          mov     [esi+ETHER.mmio],eax                            ;save MMIO
  5492                                                          mov     ecx,eax                                         ;MMIO
  5493                                                          mov     edx,czEthernetMemoryAddr                        ;message label
  5494                                                          call    ConPutLabeledHexValue                           ;output labeled message to console
  5495                                  
  5496                                                          mov     eax,[esi+ETHER.selector]                        ;ethernet adapter PCI selector
  5497                                                          mov     al,018h                                         ;I/O port (BAR1) register
  5498                                                          call    ConReadPCIRegister                              ;EAX = I/O port
  5499                                                          and     eax,-8                                          ;mask out bits 2:0
  5500                                                          mov     [esi+ETHER.port],eax                            ;save I/O port
  5501                                                          mov     ecx,eax                                         ;I/O port
  5502                                                          mov     edx,czEthernetPort                              ;message label
  5503                                                          call    ConPutLabeledHexValue                           ;output labeled message to console
  5504                                  
  5505                                                          mov     eax,[esi+ETHER.selector]                        ;ethernet device PCI selector
  5506                                                          mov     al,03Ch                                         ;interrupt number register
  5507                                                          call    ConReadPCIRegister                              ;EAX = interrupt number
  5508                                                          mov     [esi+ETHER.irq],al                              ;save interrupt number
  5509                                                          movzx   ecx,al                                          ;interrupt number
  5510                                                          mov     edx,czEthernetIRQ                               ;message label
  5511                                                          call    ConPutLabeledDecLine                            ;output labeled message to console
  5512                                  ;
