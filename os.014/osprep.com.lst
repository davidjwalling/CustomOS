  3172                                  ;       Routine:        PutDateString
  3173                                  ;
  3174                                  ;       Description:    This routine returns an ASCIIZ mm/dd/yyyy string at ds:edx from the date in the DATETIME
  3175                                  ;                       structure at ds:ebx.
  3176                                  ;
  3177                                  ;       In:             DS:EBX  DATETIME address
  3178                                  ;                       DS:EDX  output buffer address
  3179                                  ;
  3180                                  ;-----------------------------------------------------------------------------------------------------------------------
  3181                                  PutDateString           push    ecx                                             ;save non-volatile regs
  3182                                                          push    edi                                             ;
  3183                                                          push    es                                              ;
  3184                                                          push    ds                                              ;store data selector ...
  3185                                                          pop     es                                              ;... in extra segment reg
  3186                                                          mov     edi,edx                                         ;output buffer address
  3187                                                          mov     cl,10                                           ;divisor
  3188                                                          mov     edx,0002F3030h                                  ;ASCIIZ "00/" (reversed)
  3189                                                          movzx   eax,byte [ebx+DATETIME.month]                   ;month
  3190                                                          div     cl                                              ;AH = rem; AL = quotient
  3191                                                          or      eax,edx                                         ;apply ASCII zones and delimiter
  3192                                                          cld                                                     ;forward strings
  3193                                                          stosd                                                   ;store "mm/"nul
  3194                                                          dec     edi                                             ;address of terminator
  3195                                                          movzx   eax,byte [ebx+DATETIME.day]                     ;day
  3196                                                          div     cl                                              ;AH = rem; AL = quotient
  3197                                                          or      eax,edx                                         ;apply ASCII zones and delimiter
  3198                                                          stosd                                                   ;store "dd/"nul
  3199                                                          dec     edi                                             ;address of terminator
  3200                                                          movzx   eax,byte [ebx+DATETIME.century]                 ;century
  3201                                                          div     cl                                              ;AH = rem; AL = quotient
  3202                                                          or      eax,edx                                         ;apply ASCII zones and delimiter
  3203                                                          stosd                                                   ;store "cc/"null
  3204                                                          dec     edi                                             ;address of terminator
  3205                                                          dec     edi                                             ;address of delimiter
  3206                                                          movzx   eax,byte [ebx+DATETIME.year]                    ;year (yy)
  3207                                                          div     cl                                              ;AH = rem; AL = quotient
  3208                                                          or      eax,edx                                         ;apply ASCII zones and delimiter
  3209                                                          stosb                                                   ;store quotient
  3210                                                          mov     al,ah                                           ;remainder
  3211                                                          stosb                                                   ;store remainder
  3212                                                          xor     al,al                                           ;null terminator
  3213                                                          stosb                                                   ;store terminator
  3214                                                          pop     es                                              ;restore non-volatile regs
  3215                                                          pop     edi                                             ;
  3216                                                          pop     ecx                                             ;
  3217                                                          ret                                                     ;return
