  4589                                                          mov     esi,ecx                                         ;source buffer address
  4590                                                          xor     ecx,ecx                                         ;zero ecx
  4591                                                          mov     cl,5                                            ;bytes that precede dashes
  4592                                  .10                     call    ByteToHex                                       ;store hexadecimal ASCII
  4593                                                          mov     al,EASCIIDASH                                   ;delimiter
  4594                                                          stosb                                                   ;store delimiter
  4595                                                          loop    .10                                             ;next
