  5104                                  ;
  5105                                  ;       Routine:        SetKeyboardLamps
  5106                                  ;
  5107                                  ;       Description:    This routine sends the set/reset mode indicators command to the keyboard device.
  5108                                  ;
  5109                                  ;       In:             BH      00000CNS (C:Caps Lock,N:Num Lock,S:Scroll Lock)
  5110                                  ;
  5111                                  ;-----------------------------------------------------------------------------------------------------------------------
  5112                                  SetKeyboardLamps        call    WaitForKeyInBuffer                              ;wait for input buffer ready
  5113                                                          mov     al,EKEYBCMDLAMPS                                ;set/reset lamps command
  5114                                                          out     EKEYBPORTDATA,al                                ;send command to 8042
  5115                                                          call    WaitForKeyOutBuffer                             ;wait for 8042 result
  5116                                                          in      al,EKEYBPORTDATA                                ;read 8042 'ACK' (0fah)
  5117                                                          call    WaitForKeyInBuffer                              ;wait for input buffer ready
  5118                                                          mov     al,bh                                           ;set/reset lamps value
  5119                                                          out     EKEYBPORTDATA,al                                ;send lamps value
  5120                                                          call    WaitForKeyOutBuffer                             ;wait for 8042 result
  5121                                                          in      al,EKEYBPORTDATA                                ;read 8042 'ACK' (0fah)
  5122                                                          ret                                                     ;return
  5123                                  ;-----------------------------------------------------------------------------------------------------------------------
  5124                                  ;
  5125                                  ;       Routine:        WaitForKeyInBuffer
  5126                                  ;
  5127                                  ;       Description:    This routine waits for keyboard input buffer to be ready for input.
  5128                                  ;
  5129                                  ;       Out:            ZF      1 = Input buffer ready
  5130                                  ;                               0 = Input buffer not ready after timeout
  5131                                  ;
  5132                                  ;-----------------------------------------------------------------------------------------------------------------------
  5133                                  WaitForKeyInBuffer      push    ecx                                             ;save non-volatile regs
  5134                                                          mov     ecx,EKEYBWAITLOOP                               ;keyboard controller timeout
  5135                                  .10                     in      al,EKEYBPORTSTAT                                ;keyboard status byte
  5136                                                          test    al,EKEYBBITIN                                   ;is input buffer still full?
  5137                                                          loopnz  .10                                             ;yes, repeat till timeout
  5138                                                          pop     ecx                                             ;restore non-volatile regs
  5139                                                          ret                                                     ;return
  5140                                  ;-----------------------------------------------------------------------------------------------------------------------
  5141                                  ;
  5142                                  ;       Routine:        WaitForKeyOutBuffer
  5143                                  ;
  5144                                  ;       Description:    This routine waits for keyboard output buffer to have data to read.
  5145                                  ;
  5146                                  ;       Out:            ZF      1 = Output buffer has data from controller
  5147                                  ;                               0 = Output buffer empty after timeout
  5148                                  ;
  5149                                  ;-----------------------------------------------------------------------------------------------------------------------
  5150                                  WaitForKeyOutBuffer     push    ecx                                             ;save non-volatile regs
  5151                                                          mov     ecx,EKEYBWAITLOOP                               ;keyboard controller timeout
  5152                                  .10                     in      al,EKEYBPORTSTAT                                ;keyboard status byte
  5153                                                          test    al,EKEYBBITOUT                                  ;output buffer status bit
  5154                                                          loopz   .10                                             ;loop until output buffer bit
  5155                                                          pop     ecx                                             ;restore non-volatile regs
  5156                                                          ret                                                     ;return
