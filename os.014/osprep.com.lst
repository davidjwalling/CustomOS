  4965                                  ;
  4966                                  ;       Routine:        PlaceCursor
  4967                                  ;
  4968                                  ;       Description:    This routine positions the cursor on the console.
  4969                                  ;
  4970                                  ;       In:             DS      OS data selector
  4971                                  ;
  4972                                  ;-----------------------------------------------------------------------------------------------------------------------
  4973                                  PlaceCursor             push    ecx                                             ;save non-volatile regs
  4974                                                          mov     al,[wbConsoleRow]                               ;AL = row
  4975                                                          mov     ah,ECONCOLS                                     ;AH = cols/row
  4976                                                          mul     ah                                              ;row offset
  4977                                                          add     al,[wbConsoleColumn]                            ;add column
  4978                                                          adc     ah,0                                            ;add overflow
  4979                                                          mov     ecx,eax                                         ;screen offset
  4980                                                          mov     dl,ECRTPORTLO                                   ;crt controller port lo
  4981                                                          mov     dh,ECRTPORTHI                                   ;crt controller port hi
  4982                                                          mov     al,ECRTCURLOCHI                                 ;crt cursor loc reg hi
  4983                                                          out     dx,al                                           ;select register
