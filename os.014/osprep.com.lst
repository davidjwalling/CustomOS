  4510                                  ;
  4511                                  ;       Routine:        DecimalToUnsigned
  4512                                  ;
  4513                                  ;       Description:    This routine returns an unsigned integer of the value of the input ASCIIZ decimal string.
  4514                                  ;
  4515                                  ;       Input:          DS:EDX  null-terminated decimal string address
  4516                                  ;
  4517                                  ;       Output:         EAX     unsigned integer value
  4518                                  ;
  4519                                  ;-----------------------------------------------------------------------------------------------------------------------
  4520                                  DecimalToUnsigned       push    esi                                             ;save non-volatile regs
  4521                                                          mov     esi,edx                                         ;source address
  4522                                                          xor     edx,edx                                         ;zero total
  4523                                  .10                     lodsb                                                   ;source byte
  4524                                                          cmp     al,','                                          ;comma?
  4525                                                          je      .10                                             ;yes, ignore
  4526                                                          test    al,al                                           ;end of string?
  4527                                                          jz      .30                                             ;yes, done
  4528                                                          cmp     al,'.'                                          ;decimal point?
  4529                                                          je      .30                                             ;yes, done
  4530                                                          cmp     al,'0'                                          ;numeral?
  4531                                                          jb      .20                                             ;no, invalid string
  4532                                                          cmp     al,'9'                                          ;numeral?
  4533                                                          ja      .20                                             ;no, invalid string
  4534                                                          and     al,00Fh                                         ;mask ASCII zone
  4535                                                          push    eax                                             ;save numeral
  4536                                                          shl     edx,1                                           ;total * 2
  4537                                                          mov     eax,edx                                         ;total * 2
  4538                                                          shl     edx,2                                           ;total * 8
  4539                                                          add     edx,eax                                         ;total * 10
  4540                                                          pop     eax                                             ;restore numeral
  4541                                                          add     edx,eax                                         ;accumulate decimal digit
  4542                                                          xor     eax,eax                                         ;zero register
  4543                                                          jmp     .10                                             ;next
  4544                                  .20                     xor     edx,edx                                         ;zero result on error
  4545                                  .30                     mov     eax,edx                                         ;result
  4546                                                          pop     esi                                             ;restore non-volatile regs
  4547                                                          ret                                                     ;return
  4548                                  ;-----------------------------------------------------------------------------------------------------------------------
  4549                                  ;
  4550                                  ;       Routine:        HexadecimalToUnsigned
  4551                                  ;
  4552                                  ;       Description:    This routine returns an unsigned integer of the value of the input ASCIIZ hexadecimal string.
  4553                                  ;
  4554                                  ;       Input:          DS:EDX  null-terminated hexadecimal string address
  4555                                  ;
  4556                                  ;       Output:         EAX     unsigned integer value
  4557                                  ;
  4558                                  ;-----------------------------------------------------------------------------------------------------------------------
  4559                                  HexadecimalToUnsigned   push    esi                                             ;save non-volatile regs
  4560                                                          mov     esi,edx                                         ;source address
  4561                                                          xor     edx,edx                                         ;zero register
  4562                                  .10                     lodsb                                                   ;source byte
  4563                                                          test    al,al                                           ;end of string?
  4564                                                          jz      .30                                             ;yes, branch
  4565                                                          cmp     al,'9'                                          ;hexadecimal?
  4566                                                          jna     .20                                             ;no, skip ahead
  4567                                                          sub     al,037h                                         ;'A' = 41h, less 37h = 0Ah
  4568                                  .20                     and     eax,00Fh                                        ;remove ascii zone
  4569                                                          shl     edx,4                                           ;previous total x 16
  4570                                                          add     edx,eax                                         ;add prior value x 16
  4571                                                          jmp     .10                                             ;next
  4572                                  .30                     mov     eax,edx                                         ;result
  4573                                                          pop     esi                                             ;restore non-volatile regs
  4574                                                          ret                                                     ;return
