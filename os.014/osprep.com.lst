  4852                                  ;
  4853                                  ;       Routine:        SetConsoleChar
  4854                                  ;
  4855                                  ;       Description:    This routine outputs an ASCII character at the given row and column.
  4856                                  ;
  4857                                  ;       In:             AL      ASCII character
  4858                                  ;                       CL      column
  4859                                  ;                       CH      row
  4860                                  ;                       ES      CGA selector
  4861                                  ;
  4862                                  ;       Out:            EAX     last target address written (ES:)
  4863                                  ;                       CL      column + 1
  4864                                  ;
  4865                                  ;-----------------------------------------------------------------------------------------------------------------------
  4866                                  SetConsoleChar          mov     dl,al                                           ;ASCII character
  4867                                                          movzx   eax,ch                                          ;row
  4868                                                          mov     ah,ECONCOLS                                     ;cols/row
  4869                                                          mul     ah                                              ;row * cols/row
  4870                                                          add     al,cl                                           ;add column
  4871                                                          adc     ah,0                                            ;handle carry
  4872                                                          shl     eax,1                                           ;screen offset
  4873                                                          mov     [es:eax],dl                                     ;store character
  4874                                                          inc     cl                                              ;next column
  4875                                                          ret                                                     ;return
