  5843                                  .10                     lodsb                                                   ;command length
  5844                                                          movzx   ecx,al                                          ;command length
  5845                                                          jecxz   .20                                             ;branch if end of table
  5846                                                          mov     ebx,esi                                         ;table entry address
  5847                                                          add     esi,ecx                                         ;next table entry address
