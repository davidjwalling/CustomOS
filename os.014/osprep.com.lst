  5516                                                          jecxz   .60                                             ;branch if none
  5517                                                          add     ecx,05400h                                      ;MAC address offset
  5518                                                          mov     eax,[ecx]                                       ;MAC address lo-order dword
  5519                                                          mov     [esi+ETHER.mac],eax                             ;save
  5520                                                          mov     ax,[ecx+4]                                      ;MAC address hi-order word
  5521                                                          mov     [esi+ETHER.mac+4],ax                            ;save
  5522                                                          lea     ecx,[esi+ETHER.mac]                             ;address of MAC bytes
  5523                                                          putMACString wzConsoleToken                             ;output MAC ASCIIZ string
  5524                                                          mov     edx,czEthernetMAC                               ;label string
  5525                                                          call    ConPutLabeledLine                               ;output labeled line to console
  5526                                  ;
  5527                                  ;       Restore and return.
  5528                                  ;
  5529                                  .60                     pop     edi                                             ;restore non-volatile regs
  5530                                                          pop     esi                                             ;
  5531                                                          pop     ecx                                             ;
  5532                                                          pop     ebx                                             ;
  5533                                                          ret                                                     ;return
