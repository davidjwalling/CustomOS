  4313                                  ;
  4314                                  ;       Routine:        PutConsoleOIAChar
  4315                                  ;
  4316                                  ;       Description:    This routine updates the Operator Information Area (OIA).
  4317                                  ;
  4318                                  ;       In:             DS      OS data selector
  4319                                  ;
  4320                                  ;-----------------------------------------------------------------------------------------------------------------------
  4321                                  PutConsoleOIAChar       push    ebx                                             ;save non-volatile regs
  4322                                                          push    ecx                                             ;
  4323                                                          push    esi                                             ;
  4324                                                          push    ds                                              ;
  4325                                                          push    es                                              ;
  4326                                                          push    EGDTOSDATA                                      ;load OS data selector ...
  4327                                                          pop     ds                                              ;... into data segment register
  4328                                                          push    EGDTCGA                                         ;load CGA selector ...
  4329                                                          pop     es                                              ;... into extra segment register
  4330                                                          mov     esi,wbConsoleScan0                              ;scan codes address
  4331                                                          mov     bh,ECONOIAROW                                   ;OIA row
  4332                                                          mov     bl,0                                            ;starting column
  4333                                                          mov     ecx,6                                           ;maximum scan codes
  4334                                  .10                     push    ecx                                             ;save remaining count
  4335                                                          mov     ecx,ebx                                         ;row, column
  4336                                                          lodsb                                                   ;read scan code
  4337                                                          or      al,al                                           ;scan code present?
  4338                                                          jz      .20                                             ;no, skip ahead
  4339                                                          call    PutConsoleHexByte                               ;display scan code
  4340                                                          jmp     .30                                             ;continue
  4341                                  .20                     mov     al,' '                                          ;ASCII space
  4342                                                          call    SetConsoleChar                                  ;display space
  4343                                                          mov     al,' '                                          ;ASCII space
  4344                                                          call    SetConsoleChar                                  ;display space
  4345                                  .30                     add     bl,2                                            ;next column (+2)
  4346                                                          pop     ecx                                             ;restore remaining
  4347                                                          loop    .10                                             ;next code
  4348                                                          mov     al,[wbConsoleChar]                              ;console ASCII character
  4349                                                          cmp     al,32                                           ;printable? (lower-bounds)
  4350                                                          jb      .40                                             ;no, skip ahead
  4351                                                          cmp     al,126                                          ;printable? (upper-bounds)
  4352                                                          ja      .40                                             ;no, skip ahead
  4353                                                          mov     ch,bh                                           ;OIA row
  4354                                                          mov     cl,40                                           ;character display column
  4355                                                          call    SetConsoleChar                                  ;display ASCII character
  4356                                  .40                     pop     es                                              ;restore non-volatile regs
  4357                                                          pop     ds                                              ;
  4358                                                          pop     esi                                             ;
  4359                                                          pop     ecx                                             ;
  4360                                                          pop     ebx                                             ;
  4361                                                          ret                                                     ;return
