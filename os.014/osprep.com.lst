  3130                                  ;
  3131                                  ;       Routine:        GetYear
  3132                                  ;
  3133                                  ;       Description:    Return the four-digit year (century * 100 + year of century)
  3134                                  ;
  3135                                  ;       In:             DS:EBX  DATETIME address
  3136                                  ;
  3137                                  ;       Out:            ECX     year
  3138                                  ;
  3139                                  ;-----------------------------------------------------------------------------------------------------------------------
  3140                                  GetYear                 movzx   ecx,byte [ebx+DATETIME.century]                 ;century
  3141                                                          imul    ecx,100                                         ;century * 100
  3142                                                          movzx   eax,byte [ebx+DATETIME.year]                    ;year of century
  3143                                                          add     ecx,eax                                         ;year (YYYY)
  3144                                                          ret                                                     ;return
  3145                                  ;-----------------------------------------------------------------------------------------------------------------------
  3146                                  ;
  3147                                  ;       Routine:        IsLeapYear
  3148                                  ;
  3149                                  ;       Description:    This routine returns an indicator if the current year is a leap year.
  3150                                  ;
  3151                                  ;       In:             DS:EBX  DATETIME ADDRESS
  3152                                  ;
  3153                                  ;       Out:            ECX     0 = not a leap year
  3154                                  ;                               1 = leap year
  3155                                  ;
  3156                                  ;-----------------------------------------------------------------------------------------------------------------------
  3157                                  IsLeapYear              call    GetYear                                         ;ECX = YYYY
  3158                                                          mov     eax,ecx                                         ;EAX = YYYY
  3159                                                          xor     ecx,ecx                                         ;assume not leap year
  3160                                                          test    al,00000011b                                    ;multiple of four?
  3161                                                          jnz     .no                                             ;no, branch
  3162                                                          mov     dl,100                                          ;divisor
  3163                                                          div     dl                                              ;divide by 100
  3164                                                          test    ah,ah                                           ;multiple of 100?
  3165                                                          jnz     .yes                                            ;yes, branch
  3166                                                          test    al,00000011b                                    ;multiple of 400?
  3167                                                          jnz     .no                                             ;no, branch
  3168                                  .yes                    inc     ecx                                             ;indicate leap
  3169                                  .no                     ret                                                     ;return
