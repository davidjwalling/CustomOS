  4822                                  ;
  4823                                  ;       Routine:        ScrollConsoleRow
  4824                                  ;
  4825                                  ;       Description:    This routine scrolls the console (text) screen up one row.
  4826                                  ;
  4827                                  ;-----------------------------------------------------------------------------------------------------------------------
  4828                                  ScrollConsoleRow        push    ecx                                             ;save non-volatile regs
  4829                                                          push    esi                                             ;
  4830                                                          push    edi                                             ;
  4831                                                          push    ds                                              ;
  4832                                                          push    es                                              ;
  4833                                                          push    EGDTCGA                                         ;load CGA video selector ...
  4834                                                          pop     ds                                              ;... into DS
  4835                                                          push    EGDTCGA                                         ;load CGA video selector ...
  4836                                                          pop     es                                              ;... into ES
  4837                                                          mov     ecx,ECONROWDWORDS*(ECONROWS-1)                  ;double-words to move
  4838                                                          mov     esi,ECONROWBYTES                                ;ESI = source (line 2)
  4839                                                          xor     edi,edi                                         ;EDI = target (line 1)
  4840                                                          cld                                                     ;forward strings
  4841                                                          rep     movsd                                           ;move 24 lines up
  4842                                                          mov     eax,ECONCLEARDWORD                              ;attribute and ASCII space
  4843                                                          mov     ecx,ECONROWDWORDS                               ;double-words per row
  4844                                                          rep     stosd                                           ;clear bottom row
  4845                                                          pop     es                                              ;restore non-volatile regs
  4846                                                          pop     ds                                              ;
  4847                                                          pop     edi                                             ;
  4848                                                          pop     esi                                             ;
  4849                                                          pop     ecx                                             ;
  4850                                                          ret                                                     ;return
