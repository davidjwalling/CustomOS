  4791                                  ;       Routine:        ClearConsoleScreen
  4792                                  ;
  4793                                  ;       Description:    This routine clears the console (CGA) screen.
  4794                                  ;
  4795                                  ;-----------------------------------------------------------------------------------------------------------------------
  4796                                  ClearConsoleScreen      push    ecx                                             ;save non-volatile regs
  4797                                                          push    edi                                             ;
  4798                                                          push    ds                                              ;
  4799                                                          push    es                                              ;
  4800                                                          push    EGDTOSDATA                                      ;load OS Data selector ...
  4801                                                          pop     ds                                              ;... into DS register
  4802                                                          push    EGDTCGA                                         ;load CGA selector ...
  4803                                                          pop     es                                              ;... into ES register
  4804                                                          mov     eax,ECONCLEARDWORD                              ;initializtion value
  4805                                                          mov     ecx,ECONROWDWORDS*(ECONROWS)                    ;double-words to clear
  4806                                                          xor     edi,edi                                         ;target offset
  4807                                                          cld                                                     ;forward strings
  4808                                                          rep     stosd                                           ;reset screen body
  4809                                                          mov     eax,ECONOIADWORD                                ;OIA attribute and space
  4810                                                          mov     ecx,ECONROWDWORDS                               ;double-words per row
  4811                                                          rep     stosd                                           ;reset OIA line
  4812                                                          xor     al,al                                           ;zero register
  4813                                                          mov     [wbConsoleRow],al                               ;reset console row
  4814                                                          mov     [wbConsoleColumn],al                            ;reset console column
  4815                                                          call    PlaceCursor                                     ;place cursor at current position
  4816                                                          pop     es                                              ;restore non-volatile regs
  4817                                                          pop     ds                                              ;
  4818                                                          pop     edi                                             ;
  4819                                                          pop     ecx                                             ;
  4820                                                          ret                                                     ;return
