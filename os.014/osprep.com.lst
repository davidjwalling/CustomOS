  3513                                  ;
  3514                                  ;       Memory Helper Routines
  3515                                  ;
  3516                                  ;       AllocateMemory
  3517                                  ;       FreeMemory
  3518                                  ;
  3519                                  ;=======================================================================================================================
  3520                                  ;-----------------------------------------------------------------------------------------------------------------------
  3521                                  ;
  3522                                  ;       Routine:        AllocateMemory
  3523                                  ;
  3524                                  ;       Description:    This routine allocates a memory block for the given task.
  3525                                  ;
  3526                                  ;       In:             ECX     bytes of memory to allocate
  3527                                  ;
  3528                                  ;       Out:            EAX     !0      address of user portion of newly allocated memory block
  3529                                  ;                               0       unable to allocate memory
  3530                                  ;
  3531                                  ;-----------------------------------------------------------------------------------------------------------------------
  3532                                  AllocateMemory          push    ebx                                             ;save non-volatile regs
  3533                                                          push    ecx                                             ;
  3534                                                          push    esi                                             ;
  3535                                                          push    ds                                              ;
  3536                                  ;
  3537                                  ;       Address kernel memory structures
  3538                                  ;
  3539                                                          push    EGDTOSDATA                                      ;load OS data GDT selector ...
  3540                                                          pop     ds                                              ;... into data segment reg
  3541                                                          mov     esi,wsConsoleMemRoot                            ;memory root structure address
  3542                                  ;
  3543                                  ;       Set requested size to minimum block size if requested size is too small.
  3544                                  ;
  3545                                                          cmp     ecx,EMEMMINSIZE                                 ;is requested size too small?
  3546                                                          jae     .10                                             ;no, branch
  3547                                                          mov     ecx,EMEMMINSIZE                                 ;set requested size to minimum
  3548                                  .10                     add     ecx,EMEMBLOCKLEN                                ;add header block length
  3549                                  ;
  3550                                  ;       Find the first free memory block large enough to satisfy the request.
  3551                                  ;
  3552                                                          mov     eax,[esi+MEMROOT.firstfree]                     ;first free block ptr
  3553                                  .20                     test    eax,eax                                         ;end of free block chain?
  3554                                                          jz      .220                                            ;yes, branch
  3555                                                          cmp     ecx,[eax+MEMBLOCK.bytes]                        ;free block big enough?
  3556                                                          jbe     .30                                             ;yes, branch
  3557                                                          mov     eax,[eax+MEMBLOCK.nextblock]                    ;next free block addr
  3558                                                          jmp     .20                                             ;continue
  3559                                  ;-----------------------------------------------------------------------------------------------------------------------
  3560                                  ;
  3561                                  ;       Address the previous and next free memory blocks.
  3562                                  ;
  3563                                  .30                     mov     ebx,[eax+MEMBLOCK.previousblock]                ;previous free block addr
  3564                                                          mov     edx,[eax+MEMBLOCK.nextblock]                    ;next free block addr
  3565                                  ;
  3566                                  ;       Remove the free memory block from the forward free memory block chain.
  3567                                  ;
  3568                                                          test    ebx,ebx                                         ;any previous free memory block?
  3569                                                          jz      .40                                             ;no, branch
  3570                                                          mov     [ebx+MEMBLOCK.nextblock],edx                    ;remove free block from forwrad chain
  3571                                                          jmp     .50                                             ;continue
  3572                                  .40                     mov     [esi+MEMROOT.firstfree],edx                     ;next free is now also the first free
  3573                                  ;
  3574                                  ;       Remove the free memory block from the reverse free memory block chain.
  3575                                  ;
  3576                                  .50                     test    edx,edx                                         ;any next free memory block?
  3577                                                          jz      .60                                             ;no, branch
  3578                                                          mov     [edx+MEMBLOCK.previousblock],ebx                ;remove free block from reverse chain
  3579                                                          jmp     .70                                             ;continue
  3580                                  .60                     mov     [esi+MEMROOT.lastfree],ebx                      ;previous free is now also the last free
  3581                                  ;-----------------------------------------------------------------------------------------------------------------------
  3582                                  ;
  3583                                  ;       Determine if the free memory block can be split.
  3584                                  ;
  3585                                  .70                     mov     ebx,[eax+MEMBLOCK.bytes]                        ;size of free memory block
  3586                                                          sub     ebx,ecx                                         ;subtract requested memory size
  3587                                                          cmp     ebx,EMEMMINSIZE                                 ;remaining block can stand alone?
  3588                                                          jb      .150                                            ;no, branch
  3589                                  ;
  3590                                  ;       We know that our block can be split to create a new free memory block. We update the size of our free memory
  3591                                  ;       block to the requested memory size. We update the next contiguous block pointer to point just past the end
  3592                                  ;       of the requested memory size.
  3593                                  ;
  3594                                                          mov     [eax+MEMBLOCK.bytes],ecx                        ;shorten memory block size
  3595                                                          mov     edx,eax                                         ;memory block address
  3596                                                          add     edx,ecx                                         ;address new new next contig block
  3597                                                          mov     ecx,[eax+MEMBLOCK.nextcontig]                   ;next contig block address
  3598                                                          mov     [eax+MEMBLOCK.nextcontig],edx                   ;update next contig block address
  3599                                  ;
  3600                                  ;       If there is a next contiguous block, we update that memory block's previous contig pointer to point to the new
  3601                                  ;       free block we are splitting off. If there is no next contiguous block, we update the last contig block pointer.
  3602                                  ;
  3603                                                          jecxz   .80                                             ;no next contig, branch
  3604                                                          mov     [ecx+MEMBLOCK.previouscontig],edx               ;update previous contig pointer
  3605                                                          jmp     .90                                             ;continue
  3606                                  .80                     mov     [esi+MEMROOT.lastcontig],edx                    ;update last contig pointer
  3607                                  ;
  3608                                  ;       Now that the contig block pointers have been updated, we initialize the new free block members.
  3609                                  ;
  3610                                  .90                     mov     [edx+MEMBLOCK.bytes],ebx                        ;set the block size
  3611                                                          mov     [edx+MEMBLOCK.nextcontig],ecx                   ;set the next contig block addr
  3612                                                          mov     [edx+MEMBLOCK.previouscontig],eax               ;set the previous contig block addr
  3613                                                          mov     ebx,EMEMFREECODE                                ;free memory signature
  3614                                                          mov     [edx+MEMBLOCK.signature],ebx                    ;set the block signature
  3615                                                          xor     ebx,ebx                                         ;zero register
  3616                                                          mov     [edx+MEMBLOCK.reserved],ebx                     ;set reserved
  3617                                                          mov     [edx+MEMBLOCK.owner],ebx                        ;set the owner
  3618                                  ;
  3619                                  ;       Find the proper location in the free block chain for the new free block
  3620                                  ;
  3621                                                          mov     ebx,[edx+MEMBLOCK.bytes]                        ;free block size
  3622                                                          mov     ecx,[esi+MEMROOT.firstfree]                     ;first free block addr
  3623                                  .100                    jecxz   .110                                            ;branch if at end of chain
  3624                                                          cmp     ebx,[ecx+MEMBLOCK.bytes]                        ;new block smaller or equal?
  3625                                                          jbe     .110                                            ;yes, branch
  3626                                                          mov     ecx,[ecx+MEMBLOCK.nextblock]                    ;next free block addr
  3627                                                          jmp     .100                                            ;continue
  3628                                  ;
  3629                                  ;       Having found the proper location for our new free block, we store the address of the following free block, or
  3630                                  ;       zero if our new free block is larger than any other, as our next free block. Then, we take the address of our
  3631                                  ;       next block's previous block or the global last-free block as our new previous block and update the previous
  3632                                  ;       block of hte next block, if there is one.
  3633                                  ;
  3634                                  .110                    mov     [edx+MEMBLOCK.nextblock],ecx                    ;set the new free block's next ptr
  3635                                                          mov     ebx,[esi+MEMROOT.lastfree]                      ;last free block addr
  3636                                                          jecxz   .120                                            ;branch if no next block
  3637                                                          mov     ebx,[ecx+MEMBLOCK.previousblock]                ;next block's previous block
  3638                                                          mov     [ecx+MEMBLOCK.previousblock],edx                ;set the next block's previous block
  3639                                                          jmp     .130                                            ;continue
  3640                                  .120                    mov     [esi+MEMROOT.lastfree],edx                      ;set the new last free block
  3641                                  ;
  3642                                  ;       Store our previous block pointer. If we have a previous free block, update that block's next block pointer to
  3643                                  ;       point to the new block. Since the new block may now be the first or last user block, we update the first and/or
  3644                                  ;       last user block pointers if necessary.
  3645                                  ;
  3646                                  .130                    mov     [edx+MEMBLOCK.previousblock],ebx                ;set the previous block pointer
  3647                                                          test    ebx,ebx                                         ;is there a previous block?
  3648                                                          jz      .140                                            ;no, branch
  3649                                                          mov     [ebx+MEMBLOCK.nextblock],edx                    ;set the previous block's next ptr
  3650                                                          jmp     .150                                            ;continue
  3651                                  .140                    mov     [esi+MEMROOT.firstfree],edx                     ;set the new first free ptr
  3652                                  ;
  3653                                  ;       Update the newly allocated block's owner and signature.
  3654                                  ;
  3655                                  .150                    mov     edx,EMEMUSERCODE                                ;user memory signature
  3656                                                          mov     [eax+MEMBLOCK.signature],edx                    ;set the block signature
  3657                                                          xor     edx,edx                                         ;zero register
  3658                                                          str     dx                                              ;load the task state register
  3659                                                          mov     [eax+MEMBLOCK.owner],edx                        ;set the block owner
  3660                                  ;
  3661                                  ;       Remove the allocated block from the free block chain and insert it into the user block chain.
  3662                                  ;
  3663                                                          mov     ecx,[esi+MEMROOT.firsttask]                     ;first task block
  3664                                  .160                    jecxz   .180                                            ;branch if at end of chain
  3665                                                          cmp     edx,[ecx+MEMBLOCK.owner]                        ;does this block belong to the task?
  3666                                                          jb      .180                                            ;branch if block belongs to next task
  3667                                                          je      .170                                            ;branch if block belongs to this task
  3668                                                          mov     ecx,[ecx+MEMBLOCK.nextblock]                    ;next task block
  3669                                                          jmp     .160                                            ;continue
  3670                                  ;
  3671                                  ;       We have found the start of our task's user block chain or the start of the next task's user block chain. If we
  3672                                  ;       have found the next task's chain, then we have no other user memory for this task and we can simply add the
  3673                                  ;       block here. If we are at the start of our task's user block chain, then we need to further seek for the proper
  3674                                  ;       place to insert the block.
  3675                                  ;
  3676                                  .170                    mov     edx,[eax+MEMBLOCK.bytes]                        ;size of block in bytes
  3677                                                          cmp     edx,[ecx+MEMBLOCK.bytes]                        ;less or equal to chain block?
  3678                                                          jbe     .180                                            ;yes, branch
  3679                                                          mov     ecx,[ecx+MEMBLOCK.nextblock]                    ;next chain block address
  3680                                                          test    ecx,ecx                                         ;end of chain?
  3681                                                          jz      .180                                            ;yes, branch
  3682                                                          mov     edx,[eax+MEMBLOCK.owner]                        ;owning task
  3683                                                          cmp     edx,[ecx+MEMBLOCK.owner]                        ;same task?
  3684                                                          je      .170                                            ;yes, continue search
  3685                                  ;
  3686                                  ;       We have found the proper place in our task's user-block chain to insert our new user block. It may also be the
  3687                                  ;       end of the user-block chain. To insert our new user block, first we update the next-block pointer. Then, we load
  3688                                  ;       the next-block's previous-block pointer or the global last-user block pointer if we have no next-block. If we
  3689                                  ;       do have a previous-block, we update that block's next-block pointer.
  3690                                  ;
  3691                                  .180                    mov     [eax+MEMBLOCK.nextblock],ecx                    ;set the next task block
  3692                                                          mov     ebx,[esi+MEMROOT.lasttask]                      ;last task block
  3693                                                          jecxz   .190                                            ;branch if no next-task block
  3694                                                          mov     ebx,[ecx+MEMBLOCK.previousblock]                ;next-task's previous-task block
  3695                                                          mov     [ecx+MEMBLOCK.previousblock],eax                ;update next-task block's previous-task
  3696                                                          jmp     .200                                            ;continue
  3697                                  .190                    mov     [esi+MEMROOT.lasttask],eax                      ;new block is the last user-block
  3698                                  ;
  3699                                  ;       Now wes tore our previous-block pointer and, if we have a previous-free block, we update that block's next-
  3700                                  ;       block pointer to point to our block. Since our block may now be the first or last user-block, we update the
  3701                                  ;       global first and/or last user-block pointers if necessary.
  3702                                  ;
  3703                                  .200                    mov     [eax+MEMBLOCK.previousblock],ebx                ;set the previous task block
  3704                                                          test    ebx,ebx                                         ;do we have a previous task block?
  3705                                                          jz      .210                                            ;no, branch
  3706                                                          mov     [ebx+MEMBLOCK.nextblock],eax                    ;set previous-block's next-task block
  3707                                                          jmp     .220                                            ;continue
  3708                                  .210                    mov     [esi+MEMROOT.firsttask],eax                     ;new block is the first user-block
  3709                                  ;
  3710                                  ;       Restore registers and return to caller.
  3711                                  ;
  3712                                  .220                    pop     ds                                              ;restore non-volatie regs
  3713                                                          pop     esi                                             ;
  3714                                                          pop     ecx                                             ;
  3715                                                          pop     ebx                                             ;
  3716                                                          ret                                                     ;return
  3717                                  ;-----------------------------------------------------------------------------------------------------------------------
  3718                                  ;
  3719                                  ;       Routine:        FreeMemory
  3720                                  ;
  3721                                  ;       Description:    This routine frees a memory block for the given task. The address provided in EDX points to the
  3722                                  ;                       memory block header. The memory block must be USER memory, not a FREE memory block. If the block
  3723                                  ;                       is adjacent to a contiguous FREE memory block, then the blocks are merged. The residual FREE
  3724                                  ;                       memory is repositioned in the FREE memory block chain according to size. The user portion of the
  3725                                  ;                       block, following the block header, is reset (wiped) with the memory wipe value.
  3726                                  ;
  3727                                  ;       In:             EDX     memory block to free, relative to EGDTOSDATA
  3728                                  ;
  3729                                  ;       Out:            EAX     -1      invalid memory block
  3730                                  ;                               0       memory block freed
  3731                                  ;
  3732                                  ;-----------------------------------------------------------------------------------------------------------------------
  3733                                  FreeMemory              push    ebx                                             ;save non-volatile regs
  3734                                                          push    ecx                                             ;
  3735                                                          push    esi                                             ;
  3736                                                          push    edi                                             ;
  3737                                                          push    ds                                              ;
  3738                                                          push    es                                              ;
  3739                                  ;
  3740                                  ;       Address the root memory structure
  3741                                  ;
  3742                                                          push    EGDTOSDATA                                      ;load OS data selector ...
  3743                                                          pop     es                                              ;... into extra segment reg
  3744                                                          push    EGDTOSDATA                                      ;load OS data selector ...
  3745                                                          pop     ds                                              ;... into data segment reg
  3746                                                          mov     esi,wsConsoleMemRoot                            ;memory root structure
  3747                                                          mov     edi,edx                                         ;memory block address
  3748                                  ;
  3749                                  ;       If the block is FREE, return success. Otherwise, if it is not USER, return with error.
  3750                                  ;
  3751                                                          xor     eax,eax                                         ;indicate success
  3752                                                          cmp     dword [edi+MEMBLOCK.signature],EMEMFREECODE     ;is the block FREE?
  3753                                                          je      .240                                            ;yes, branch
  3754                                                          dec     eax                                             ;indicate failure
  3755                                                          cmp     dword [edi+MEMBLOCK.signature],EMEMUSERCODE     ;is the block USER?
  3756                                                          jne     .240                                            ;no, branch
  3757                                  ;-----------------------------------------------------------------------------------------------------------------------
  3758                                  ;
  3759                                  ;       Unlink the USER memory block.
  3760                                  ;
  3761                                  ;-----------------------------------------------------------------------------------------------------------------------
  3762                                  ;
  3763                                  ;       Set the block signature. Reset owner.
  3764                                  ;
  3765                                                          mov     dword [edi+MEMBLOCK.signature],EMEMFREECODE     ;set FREE block signature
  3766                                                          xor     eax,eax                                         ;zero register
  3767                                                          mov     [edi+MEMBLOCK.owner],eax                        ;zero block owner
  3768                                  ;
  3769                                  ;       Wipe user area.
  3770                                  ;
  3771                                                          push    edi                                             ;save block address
  3772                                                          mov     ecx,[edi+MEMBLOCK.bytes]                        ;block size
  3773                                                          sub     ecx,EMEMBLOCKLEN                                ;subtract header size
  3774                                                          add     edi,EMEMBLOCKLEN                                ;point to user area
  3775                                                          mov     al,EMEMWIPEBYTE                                 ;memory wipe byte
  3776                                                          rep     stosb                                           ;clear memory
  3777                                                          pop     edi                                             ;restore block address
  3778                                  ;
  3779                                  ;       Address the preceding and following USER memory blocks
  3780                                  ;
  3781                                                          mov     ebx,[edi+MEMBLOCK.previousblock]                ;previous block pointer
  3782                                                          mov     ecx,[edi+MEMBLOCK.nextblock]                    ;next block pointer
  3783                                  ;
  3784                                  ;       If a USER block precedes this block, update that block's next pointer. Otherwise, update the first task
  3785                                  ;       pointer to point to the USER block following this block.
  3786                                  ;
  3787                                                          test    ebx,ebx                                         ;is there a previous block?
  3788                                                          jz      .10                                             ;no, branch
  3789                                                          mov     [ebx+MEMBLOCK.nextblock],ecx                    ;update previous block's next pointer
  3790                                                          jmp     .20                                             ;continue
  3791                                  .10                     mov     [esi+MEMROOT.firsttask],ecx                     ;update first USER pointer
  3792                                  ;
  3793                                  ;       If a USER block follows this block, update that block's previous pointer. Otherwise, update the last task
  3794                                  ;       pointer to point to the USER block preceding this block.
  3795                                  ;
  3796                                  .20                     jecxz   .30                                             ;branch if no next block
  3797                                                          mov     [ecx+MEMBLOCK.previousblock],ebx                ;update next block's previous pointer
  3798                                                          jmp     .40                                             ;continue
  3799                                  .30                     mov     [esi+MEMROOT.lasttask],ebx                      ;update last USER pointer
  3800                                  ;-----------------------------------------------------------------------------------------------------------------------
  3801                                  ;
  3802                                  ;       Merge with a previous contiguous FREE memory block.
  3803                                  ;
  3804                                  ;-----------------------------------------------------------------------------------------------------------------------
  3805                                  ;
  3806                                  ;       Address the preceding and following contiguous memory blocks.
  3807                                  ;
  3808                                  .40                     mov     ebx,[edi+MEMBLOCK.previouscontig]               ;previous contiguous block ptr
  3809                                                          mov     ecx,[edi+MEMBLOCK.nextcontig]                   ;next contiguous block ptr
  3810                                  ;
  3811                                  ;       Verify we have a previous contiguous FREE block.
  3812                                  ;
  3813                                                          test    ebx,ebx                                         ;is there a previous block?
  3814                                                          jz      .100                                            ;no, branch
  3815                                                          cmp     dword [ebx+MEMBLOCK.signature],EMEMFREECODE     ;is the previous block FREE?
  3816                                                          jne     .100                                            ;no, branch
  3817                                  ;
  3818                                  ;       Update adjacent block's contiguous pointers.
  3819                                  ;
  3820                                                          mov     [ebx+MEMBLOCK.nextcontig],ecx                   ;update previous contig's next contig
  3821                                                          jecxz   .50                                             ;branch if no next contiguous block
  3822                                                          mov     [ecx+MEMBLOCK.previouscontig],ebx               ;update next congit's previous contig
  3823                                                          jmp     .60                                             ;continue
  3824                                  .50                     mov     [esi+MEMROOT.lastcontig],ebx                    ;update last contig pointer
  3825                                  ;
  3826                                  ;       Update the size of the merged FREE block.
  3827                                  ;
  3828                                  .60                     mov     eax,[edi+MEMBLOCK.bytes]                        ;current block size
  3829                                                          add     [ebx+MEMBLOCK.bytes],eax                        ;update previous block's size
  3830                                  ;
  3831                                  ;       Having merged our new free block into the previous free block, make the previous free block the current block
  3832                                  ;
  3833                                                          mov     ecx,EMEMBLOCKLEN                                ;block header length
  3834                                                          mov     al,EMEMWIPEBYTE                                 ;memory wipe byte
  3835                                                          rep     stosb                                           ;clear memory header
  3836                                                          mov     edi,ebx                                         ;current block is now previous block
  3837                                  ;-----------------------------------------------------------------------------------------------------------------------
  3838                                  ;
  3839                                  ;       Unlink the previous contiguous FREE memory block
  3840                                  ;
  3841                                  ;-----------------------------------------------------------------------------------------------------------------------
  3842                                  ;
  3843                                  ;       Address the preceding and following USER memory blocks
  3844                                  ;
  3845                                                          mov     ebx,[edi+MEMBLOCK.previousblock]                ;previous block pointer
  3846                                                          mov     ecx,[edi+MEMBLOCK.nextblock]                    ;next block pointer
  3847                                  ;
  3848                                  ;       Update the previous block's next-block pointer if there is a previous block. Otherwise, update the first free
  3849                                  ;       block pointer.
  3850                                  ;
  3851                                                          test    ebx,ebx                                         ;is there a previous block?
  3852                                                          jz      .70                                             ;no, branch
  3853                                                          mov     [ebx+MEMBLOCK.nextblock],ecx                    ;update previous block's next pointer
  3854                                                          jmp     .80                                             ;branch
  3855                                  .70                     mov     [esi+MEMROOT.firstfree],ecx                     ;update first FREE block pointer
  3856                                  ;
  3857                                  ;       Update the next block's previous-block pointer if there is a next block. Otherwise, update the last free block
  3858                                  ;       pointer.
  3859                                  ;
  3860                                  .80                     jecxz   .90                                             ;branch if no next block
  3861                                                          mov     [ecx+MEMBLOCK.previousblock],ebx                ;update next block's previous pointer
  3862                                                          jmp     .100                                            ;continue
  3863                                  .90                     mov     [esi+MEMROOT.lastfree],ebx                      ;update last FREE block pointer
  3864                                  ;-----------------------------------------------------------------------------------------------------------------------
  3865                                  ;
  3866                                  ;       Merge with a following contiguous FREE memory block.
  3867                                  ;
  3868                                  ;-----------------------------------------------------------------------------------------------------------------------
  3869                                  ;
  3870                                  ;       Verify we have a following contiguous FREE block.
  3871                                  ;
  3872                                  .100                    mov     ecx,[edi+MEMBLOCK.nextcontig]                   ;next contiguous block ptr
  3873                                                          jecxz   .170                                            ;branch if no next contiguous block
  3874                                                          cmp     dword [ecx+MEMBLOCK.signature],EMEMFREECODE     ;is the next-contiguous block free?
  3875                                                          jne     .170                                            ;no, branch
  3876                                  ;
  3877                                  ;       Add the size of the following adjacent FREE block to this block's size.
  3878                                  ;
  3879                                                          mov     eax,[ecx+MEMBLOCK.bytes]                        ;next contiguous (free) block size
  3880                                                          add     [edi+MEMBLOCK.bytes],eax                        ;add size to this block's size
  3881                                  ;
  3882                                  ;       Unlink the following contiguous FREE block from the contiguous block chain.
  3883                                  ;
  3884                                                          mov     eax,[ecx+MEMBLOCK.nextcontig]                   ;following block's next-contig ptr
  3885                                                          mov     [edi+MEMBLOCK.nextcontig],eax                   ;update this block's next-contig ptr
  3886                                                          test    eax,eax                                         ;does a block follow the next contig blk
  3887                                                          jz      .110                                            ;no, branch
  3888                                                          mov     [eax+MEMBLOCK.previouscontig],edi               ;update following block's prev contig
  3889                                                          jmp     .120                                            ;continue
  3890                                  .110                    mov     [esi+MEMROOT.lastcontig],edi                    ;update last contig block ptr
  3891                                  ;-----------------------------------------------------------------------------------------------------------------------
  3892                                  ;
  3893                                  ;       Unlink the following contiguous FREE memory block
  3894                                  ;
  3895                                  ;-----------------------------------------------------------------------------------------------------------------------
  3896                                  ;
  3897                                  ;       Unlink the following adjacent FREE block from the FREE block chain.
  3898                                  ;
  3899                                  .120                    push    edi                                             ;save this block
  3900                                                          mov     edi,ecx                                         ;next contiguous block
  3901                                                          push    ecx                                             ;save next contiguous block
  3902                                  ;
  3903                                  ;       Address the preceding and following USER memory blocks
  3904                                  ;
  3905                                                          mov     ebx,[edi+MEMBLOCK.previousblock]                ;next contig's previous block pointer
  3906                                                          mov     ecx,[edi+MEMBLOCK.nextblock]                    ;next contig's next block pointer
  3907                                  ;
  3908                                  ;       Update the previous block's next-block pointer if there is a previous block. Otherwise, update the first free
  3909                                  ;       block pointer.
  3910                                  ;
  3911                                                          test    ebx,ebx                                         ;is there a previous block?
  3912                                                          jz      .130                                            ;no, branch
  3913                                                          mov     [ebx+MEMBLOCK.nextblock],ecx                    ;update next contig's prev blk next-ptr
  3914                                                          jmp     .140                                            ;branch
  3915                                  .130                    mov     [esi+MEMROOT.firstfree],ecx                     ;update first FREE block pointer
  3916                                  ;
  3917                                  ;       Update the next block's previous-block pointer if there is a next block. Otherwise, update the last free block
  3918                                  ;       pointer.
  3919                                  ;
  3920                                  .140                    jecxz   .150                                            ;branch if no next block
  3921                                                          mov     [ecx+MEMBLOCK.previousblock],ebx                ;update next contig's next blk prev-ptr
  3922                                                          jmp     .160                                            ;continue
  3923                                  .150                    mov     [esi+MEMROOT.lastfree],ebx                      ;update last FREE block pointer
  3924                                  ;
  3925                                  ;       Clear next contiguous block's header
  3926                                  ;
  3927                                  .160                    pop     edi                                             ;next congiguous block pointer
  3928                                                          mov     ecx,EMEMBLOCKLEN                                ;memory block header length
  3929                                                          mov     al,EMEMWIPEBYTE                                 ;memory wipe byte
  3930                                                          rep     stosb                                           ;clear memory header
  3931                                                          pop     edi                                             ;this block's pointer
  3932                                  ;-----------------------------------------------------------------------------------------------------------------------
  3933                                  ;
  3934                                  ;       Insert the final FREE block back into the block chain.
  3935                                  ;
  3936                                  ;-----------------------------------------------------------------------------------------------------------------------
  3937                                  ;
  3938                                  ;       Walk the FREE memory chain until a block is found that is larger than or equal in size to the block being
  3939                                  ;       inserted. The block being inserted will be inserted before that block or after the last block found if none
  3940                                  ;       all are smaller in size.
  3941                                  ;
  3942                                  .170                    mov     ebx,[edi+MEMBLOCK.bytes]                        ;size of block
  3943                                                          mov     ecx,[esi+MEMROOT.firstfree]                     ;first free block ptr
  3944                                  .180                    jecxz   .190                                            ;exit if no ptr
  3945                                                          cmp     ebx,[ecx+MEMBLOCK.bytes]                        ;next block bigger?
  3946                                                          jb      .190                                            ;yes, branch
  3947                                                          mov     ecx,[ecx+MEMBLOCK.nextblock]                    ;next free memory block
  3948                                                          jmp     .180                                            ;continue
  3949                                  ;
  3950                                  ;       Set the next-block pointer. Determine the previous-block, which may be the last FREE block if we found no
  3951                                  ;       larger free block. Update the next block's previous block pointer.
  3952                                  ;
  3953                                  .190                    mov     [edi+MEMBLOCK.nextblock],ecx                    ;set the next block ptr
  3954                                                          mov     ebx,[esi+MEMROOT.lastfree]                      ;assume all blocks smaller
  3955                                                          jecxz   .200                                            ;branch if no block found
  3956                                                          mov     ebx,[ecx+MEMBLOCK.previousblock]                ;next block's previous block ptr
  3957                                                          mov     [ecx+MEMBLOCK.previousblock],edi                ;update next block's previous ptr
  3958                                                          jmp     .210                                            ;continue
  3959                                  .200                    mov     [esi+MEMROOT.lastfree],edi                      ;this block is now the last free
  3960                                  ;
  3961                                  ;       Set our previous block pointer to either the previous pointer of the found block or the last free block.
  3962                                  ;       If there is no previous block pointer, then this block now the first FREE block. Otherwise update that block's
  3963                                  ;       next pointer.
  3964                                  ;
  3965                                  .210                    mov     [edi+MEMBLOCK.previousblock],ebx                ;set the previous block ptr
  3966                                                          test    ebx,ebx                                         ;do we have a previous block?
  3967                                                          jz      .220                                            ;no, branch
  3968                                                          mov     [ebx+MEMBLOCK.nextblock],edi                    ;update previous block's next block ptr
  3969                                                          jmp     .230                                            ;continue
  3970                                  .220                    mov     [esi+MEMROOT.firstfree],edi                     ;update first free ptr
  3971                                  ;
  3972                                  ;       The memory free has completed.
  3973                                  ;
  3974                                  .230                    xor     eax,eax                                         ;indicate success
  3975                                  ;
  3976                                  ;       Restore and return.
  3977                                  ;
  3978                                  .240                    pop     es                                              ;restore non-volatile regs
  3979                                                          pop     ds                                              ;
  3980                                                          pop     edi                                             ;
  3981                                                          pop     esi                                             ;
  3982                                                          pop     ecx                                             ;
  3983                                                          pop     ebx                                             ;
  3984                                                          ret                                                     ;return
