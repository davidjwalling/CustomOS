  6147                                  ;       Routine:        ConPCIProbe
  6148                                  ;
  6149                                  ;       Description:    This routine handles the PCIProbe command.
  6150                                  ;
  6151                                  ;-----------------------------------------------------------------------------------------------------------------------
  6152                                  ConPCIProbe             push    ebx                                             ;save non-volatile regs
  6153                                  ;
  6154                                  ;       Initialize variables.
  6155                                  ;       Construct PCI selector.
  6156                                  ;       Read PCI configuration data.
  6157                                  ;
  6158                                                          mov     ebx,wsConsolePCI                                ;PCI structure address
  6159                                                          call    ConInitPCIContext                               ;initialize PCI struct
  6160                                  .10                     call    ConBuildPCISelector                             ;build the PCI selector
  6161                                                          call    ConReadPCIConfigData                            ;read the configuration data
  6162                                  ;
  6163                                  ;       Interpret PCI data value and display finding.
  6164                                  ;
  6165                                                          cmp     eax,-1                                          ;function defined?
  6166                                                          jne     .20                                             ;yes, branch
  6167                                                          cmp     byte [ebx+PCI.function],0                       ;function zero?
  6168                                                          je      .40                                             ;yes, skip to next device
  6169                                                          jmp     short .30                                       ;no, skip to next function
  6170                                  ;
  6171                                  ;       Build PCI identifying string.
  6172                                  ;       Write identifying string to console.
  6173                                  ;       Determine the vendor and chip.
  6174                                  ;       Write vendor and chip to console.
  6175                                  ;
  6176                                  .20                     mov     edx,wzConsolePCIIdent                           ;output buffer
  6177                                                          call    ConBuildPCIIdent                                ;build PCI bus, device, function ident
  6178                                                          call    ConInterpretPCIData                             ;update flags based on data
  6179                                                          putConsoleString wzConsolePCIIdent                      ;display bus as decimal
  6180                                                          putConsoleString czSpace                                ;space delimiter
  6181                                                          putConsoleString [ebx+PCI.vendorstr]                    ;display vendor string
  6182                                                          putConsoleString czSpace                                ;space delimiter
  6183                                                          putConsoleString [ebx+PCI.chipstr]                      ;display chip string
  6184                                                          call    ConPutNewLine                                   ;display new line
  6185                                  ;
  6186                                  ;       Next function.
  6187                                  ;
  6188                                  .30                     call    ConNextPCIFunction                              ;next function
  6189                                                          jb      .10                                             ;continue if no overflow
  6190                                  ;
  6191                                  ;       Next device, bus.
  6192                                  ;
  6193                                  .40                     call    ConNextPCIDevice                                ;next device, bus
  6194                                                          jb      .10                                             ;continue if no overflow
  6195                                  ;
  6196                                  ;       Restore and return.
  6197                                  ;
  6198                                                          pop     ebx                                             ;restore non-volatile regs
  6199                                                          ret                                                     ;return
  6200                                  ;-----------------------------------------------------------------------------------------------------------------------
  6201                                  ;
  6202                                  ;       Routine:        ConBuildPCIIdent
  6203                                  ;
  6204                                  ;       Description:    This routine constructs a PCI identification string from the current PCI Bus, Device, and
  6205                                  ;                       Function code values.
  6206                                  ;
  6207                                  ;       In:             DS:EBX  PCI structure address
  6208                                  ;                       DS:EDX  output buffer address 999.99.9\0
  6209                                  ;
  6210                                  ;-----------------------------------------------------------------------------------------------------------------------
  6211                                  ConBuildPCIIdent        push    edi                                             ;save non-volatile regs
  6212                                                          push    es                                              ;
  6213                                  ;
  6214                                  ;       Establish addressability
  6215                                  ;
  6216                                                          push    ds                                              ;load data segment...
  6217                                                          pop     es                                              ;...into extra segment reg
  6218                                  ;
  6219                                  ;       Build identifier string (bus.device.function)
  6220                                  ;
  6221                                                          mov     edi,edx                                         ;output buffer address
  6222                                                          mov     al,[ebx+PCI.bus]                                ;current PCI bus (0-255)
  6223                                                          xor     ah,ah                                           ;zero high-order dividend
  6224                                                          mov     cl,100                                          ;divisor (10^2)
  6225                                                          div     cl                                              ;AL=100's, AH=bus MOD 100
  6226                                                          or      al,30h                                          ;apply ASCII zone
  6227                                                          cld                                                     ;forward strings
  6228                                                          stosb                                                   ;store 100's digit
  6229                                                          mov     al,ah                                           ;bus MOD 100
  6230                                                          xor     ah,ah                                           ;zero high-order dividend
  6231                                                          mov     cl,10                                           ;divisor (10^1)
  6232                                                          div     cl                                              ;AL=10's, AH=1's
  6233                                                          or      ax,3030h                                        ;apply ASCII zone
  6234                                                          stosw                                                   ;store 10's and 1's
  6235                                                          mov     al,EASCIIPERIOD                                 ;ASCII period delimiter
  6236                                                          stosb                                                   ;store delimiter
  6237                                                          mov     al,[ebx+PCI.device]                             ;current PCI device (0-15)
  6238                                                          xor     ah,ah                                           ;zero high order dividend
  6239                                                          mov     cl,10                                           ;divisor (10^1)
  6240                                                          div     cl                                              ;AL=10's, AH=1's
  6241                                                          or      ax,3030h                                        ;apply ASCII zone
  6242                                                          stosw                                                   ;store 10's and 1's
  6243                                                          mov     al,EASCIIPERIOD                                 ;ASCII period delimiter
  6244                                                          stosb                                                   ;store delimiter
  6245                                                          mov     al,[ebx+PCI.function]                           ;current PCI function (0-7)
  6246                                                          or      al,30h                                          ;apply ASCII zone
  6247                                                          stosb                                                   ;store 1's
  6248                                                          xor     al,al                                           ;null terminator
  6249                                                          stosb                                                   ;store terminator
  6250                                  ;
  6251                                  ;       Restore and return.
  6252                                  ;
  6253                                                          pop     es                                              ;restore non-volatile regs
  6254                                                          pop     edi                                             ;
  6255                                                          ret                                                     ;return
  6256                                  ;-----------------------------------------------------------------------------------------------------------------------
  6257                                  ;
  6258                                  ;       Routine:        ConInterpretPCIData
  6259                                  ;
  6260                                  ;       Description:    This routine interprets the PCI vendor and device IDs.
  6261                                  ;
  6262                                  ;       In:             DS:EBX  PCI structure address
  6263                                  ;
  6264                                  ;-----------------------------------------------------------------------------------------------------------------------
  6265                                  ConInterpretPCIData     mov     eax,czApple
  6266                                                          cmp     word [ebx+PCI.configdata_lo],EPCIVENDORAPPLE    ;Apple?
  6267                                                          jne     .10                                             ;no, branch
  6268                                                          mov     edx,czUSBController
  6269                                                          cmp     word [ebx+PCI.configdata_hi],EPCIAPPLEUSB       ;USB?
  6270                                                          je      .40                                             ;yes, branch
  6271                                                          mov     edx,czOther                                     ;other
  6272                                                          jmp     .40                                             ;continue
  6273                                  .10                     mov     eax,czIntel                                     ;Intel
  6274                                                          cmp     word [ebx+PCI.configdata_lo],EPCIVENDORINTEL    ;Intel?
  6275                                                          jne     .20                                             ;no, branch
  6276                                                          mov     edx,czPro1000MT                                 ;Pro/1000 MT
  6277                                                          cmp     word [ebx+PCI.configdata_hi],EPCIINTELPRO1000MT ;Pro/1000 MT?
  6278                                                          je      .40                                             ;yes, branch
  6279                                                          mov     edx,czPCIAndMem                                 ;PCI and Memory
  6280                                                          cmp     word [ebx+PCI.configdata_hi],EPCIINTELPCIMEM    ;PCI and Memory?
  6281                                                          je      .40                                             ;yes, branch
  6282                                                          mov     edx,czAurealAD1881                              ;Aureal 1881 SOUNDMAX
  6283                                                          cmp     word [ebx+PCI.configdata_hi],EPCIINTELAD1881    ;Aureal 1881 SOUNDMAX?
  6284                                                          je      .40                                             ;yes, branch
  6285                                                          mov     edx,czPIIX3PCItoIDEBridge                       ;PIIX3 PCI-to-IDE Bridge
  6286                                                          cmp     word [ebx+PCI.configdata_hi],EPCIINTELPIIX3     ;PIIX3 PCI-to-IDE Bridge?
  6287                                                          je      .40                                             ;yes, branch
  6288                                                          mov     edx,cz82371ABBusMaster                          ;82371AB Bus Master
  6289                                                          cmp     word [ebx+PCI.configdata_hi],EPCIINTEL82371AB   ;82371AB Bua Master?
  6290                                                          je      .40                                             ;yes, branch
  6291                                                          mov     edx,czPIIX4PowerMgmt                            ;PIIX4/4E/4M Power Mgmt Controller
  6292                                                          cmp     word [ebx+PCI.configdata_hi],EPCIINTELPIIX4     ;PIIX4/4E/4M Power Mgmt Controller?
  6293                                                          je      .40                                             ;yes, branch
  6294                                                          mov     edx,czOther                                     ;other
  6295                                                          jmp     .40                                             ;continue
  6296                                  .20                     mov     eax,czOracle                                    ;Oracle
  6297                                                          cmp     word [ebx+PCI.configdata_lo],EPCIVENDORORACLE   ;Oracle?
  6298                                                          jne     .30                                             ;no, branch
  6299                                                          mov     edx,czVirtualBoxGA                              ;VirtulaBox Graphics Adapter
  6300                                                          cmp     word [ebx+PCI.configdata_hi],EPCIORACLEVBOXGA   ;VirtualBox Graphics Adapter?
  6301                                                          je      .40                                             ;yes, branch
  6302                                                          mov     edx,czVirtualBoxDevice                          ;VirtualBox Device
  6303                                                          cmp     word [ebx+PCI.configdata_hi],EPCIORACLEVBOXDEVICE       ;VirtualBox Device?
  6304                                                          je      .40                                             ;yes, branch
  6305                                                          mov     edx,czOther                                     ;other
  6306                                                          jmp     .40                                             ;continue
  6307                                  .30                     mov     eax,czOther                                     ;other
  6308                                                          mov     edx,czOther                                     ;other
  6309                                  .40                     mov     [ebx+PCI.vendorstr],eax                         ;save vendor string
  6310                                                          mov     [ebx+PCI.chipstr],edx                           ;save chip string
  6311                                                          ret                                                     ;return
