  4172                                  ;
  4173                                  ;       Routine:        NextConsoleColumn
  4174                                  ;
  4175                                  ;       Description:    This routine advances the console position one column. The columnn is reset to zero and the row
  4176                                  ;                       incremented if the end of the current row is reached.
  4177                                  ;
  4178                                  ;       In:             DS      OS data selector
  4179                                  ;
  4180                                  ;-----------------------------------------------------------------------------------------------------------------------
  4181                                  NextConsoleColumn       mov     al,[wbConsoleColumn]                            ;current column
  4182                                                          inc     al                                              ;increment column
  4183                                                          mov     [wbConsoleColumn],al                            ;save column
  4184                                                          cmp     al,ECONCOLS                                     ;end of row?
  4185                                                          jb      .10                                             ;no, skip ahead
  4186                                                          call    FirstConsoleColumn                              ;reset column to start of row
  4187                                                          call    NextConsoleRow                                  ;line feed to next row
  4188                                  .10                     ret                                                     ;return
  4189                                  ;-----------------------------------------------------------------------------------------------------------------------
  4190                                  ;
  4191                                  ;       Routine:        NextConsoleRow
  4192                                  ;
  4193                                  ;       Description:    This routine advances the console position one line. Scroll the screen one row if needed.
  4194                                  ;
  4195                                  ;       In:             DS      OS data selector
  4196                                  ;
  4197                                  ;-----------------------------------------------------------------------------------------------------------------------
  4198                                  NextConsoleRow          mov     al,[wbConsoleRow]                               ;current row
  4199                                                          inc     al                                              ;increment row
  4200                                                          mov     [wbConsoleRow],al                               ;save row
  4201                                                          cmp     al,ECONROWS                                     ;end of screen?
  4202                                                          jb      .10                                             ;no, skip ahead
  4203                                                          call    ScrollConsoleRow                                ;scroll up one row
  4204                                                          mov     al,[wbConsoleRow]                               ;row
  4205                                                          dec     al                                              ;decrement row
  4206                                                          mov     [wbConsoleRow],al                               ;save row
  4207                                  .10                     ret                                                     ;return
  4208                                  ;-----------------------------------------------------------------------------------------------------------------------
  4209                                  ;
  4210                                  ;       Routine:        PreviousConsoleColumn
  4211                                  ;
  4212                                  ;       Description:    This routine retreats the cursor one logical column. If the cursor was at the start of a row,
  4213                                  ;                       the column is set to the last position in the row and the row is decremented.
  4214                                  ;
  4215                                  ;       In:             DS      OS data selector
  4216                                  ;
  4217                                  ;-----------------------------------------------------------------------------------------------------------------------
  4218                                  PreviousConsoleColumn   mov     al,[wbConsoleColumn]                            ;current column
  4219                                                          or      al,al                                           ;start of row?
  4220                                                          jnz     .10                                             ;no, skip ahead
  4221                                                          mov     ah,[wbConsoleRow]                               ;current row
  4222                                                          or      ah,ah                                           ;top of screen?
  4223                                                          jz      .20                                             ;yes, exit with no change
  4224                                                          dec     ah                                              ;decrement row
  4225                                                          mov     [wbConsoleRow],ah                               ;save row
  4226                                                          mov     al,ECONCOLS                                     ;set maximum column
  4227                                  .10                     dec     al                                              ;decrement column
  4228                                                          mov     [wbConsoleColumn],al                            ;save column
  4229                                  .20                     ret                                                     ;return
