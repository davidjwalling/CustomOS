  5775                                  ;
  5776                                  ;       Routine:        ConTakeToken
  5777                                  ;
  5778                                  ;       Description:    This routine extracts the next token from the given source buffer.
  5779                                  ;
  5780                                  ;       In:             DS:EDX  source buffer address
  5781                                  ;                       DS:EBX  target buffer address
  5782                                  ;
  5783                                  ;       Out:            DS:EDX  source buffer address
  5784                                  ;                       DS:EBX  target buffer address
  5785                                  ;
  5786                                  ;       Command Form:   Line    = *3( *SP 1*ALNUM )
  5787                                  ;
  5788                                  ;-----------------------------------------------------------------------------------------------------------------------
  5789                                  ConTakeToken            push    esi                                             ;save non-volatile regs
  5790                                                          push    edi                                             ;
  5791                                                          push    es                                              ;
  5792                                                          push    ds                                              ;load data segment selector ...
  5793                                                          pop     es                                              ;... into extra segment reg
  5794                                                          mov     esi,edx                                         ;source buffer address
  5795                                                          mov     edi,ebx                                         ;target buffer address
  5796                                                          mov     byte [edi],0                                    ;null-terminate target buffer
  5797                                                          cld                                                     ;forward strings
  5798                                  .10                     lodsb                                                   ;load byte
  5799                                                          cmp     al,EASCIISPACE                                  ;space?
  5800                                                          je      .10                                             ;yes, continue
  5801                                                          test    al,al                                           ;end of line?
  5802                                                          jz      .40                                             ;yes, branch
  5803                                  .20                     stosb                                                   ;store byte
  5804                                                          lodsb                                                   ;load byte
  5805                                                          test    al,al                                           ;end of line?
  5806                                                          jz      .40                                             ;no, continue
  5807                                                          cmp     al,EASCIISPACE                                  ;space?
  5808                                                          jne     .20                                             ;no, continue
  5809                                  .30                     lodsb                                                   ;load byte
  5810                                                          cmp     al,EASCIISPACE                                  ;space?
  5811                                                          je      .30                                             ;yes, continue
  5812                                                          dec     esi                                             ;pre-position
  5813                                  .40                     mov     byte [edi],0                                    ;terminate buffer
  5814                                                          mov     edi,edx                                         ;source buffer address
  5815                                  .50                     lodsb                                                   ;remaining byte
  5816                                                          stosb                                                   ;move to front of buffer
  5817                                                          test    al,al                                           ;end of line?
  5818                                                          jnz     .50                                             ;no, continue
  5819                                                          pop     es                                              ;restore non-volatile regs
  5820                                                          pop     edi                                             ;
  5821                                                          pop     esi                                             ;
  5822                                                          ret                                                     ;return
