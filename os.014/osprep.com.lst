  6015                                  ;
  6016                                  ;       Routine:        ConMem
  6017                                  ;
  6018                                  ;       Description:    This routine handles the MEMORY command and its MEM alias.
  6019                                  ;
  6020                                  ;       Input:          wzConsoleInBuffer contains parameter(s)
  6021                                  ;
  6022                                  ;-----------------------------------------------------------------------------------------------------------------------
  6023                                  ConMem                  push    ebx                                             ;save non-volatile regs
  6024                                                          push    esi                                             ;
  6025                                                          push    edi                                             ;
  6026                                  ;
  6027                                  ;       Update the source address if a parameter is given.
  6028                                  ;
  6029                                                          mov     edx,wzConsoleInBuffer                           ;console input buffer address (params)
  6030                                                          mov     ebx,wzConsoleToken                              ;console command token address
  6031                                                          call    ConTakeToken                                    ;take first param as token
  6032                                                          cmp     byte [wzConsoleToken],0                         ;token found?
  6033                                                          je      .10                                             ;no, branch
  6034                                                          mov     edx,wzConsoleToken                              ;first param as token address
  6035                                                          hexadecimalToUnsigned                                   ;convert string token to unsigned
  6036                                                          mov     [wdConsoleMemBase],eax                          ;save console memory address
  6037                                  ;
  6038                                  ;       Setup source address and row count.
  6039                                  ;
  6040                                  .10                     mov     esi,[wdConsoleMemBase]                          ;source memory address
  6041                                                          xor     ecx,ecx                                         ;zero register
  6042                                                          mov     cl,16                                           ;row count
  6043                                  ;
  6044                                  ;       Start the row with the source address in hexadecimal.
  6045                                  ;
  6046                                  .20                     push    ecx                                             ;save remaining rows
  6047                                                          mov     edi,wzConsoleOutBuffer                          ;output buffer address
  6048                                                          mov     edx,edi                                         ;output buffer address
  6049                                                          mov     ecx,esi                                         ;console memory address
  6050                                                          unsignedToHexadecimal                                   ;convert unsigned address to hex string
  6051                                                          add     edi,8                                           ;end of memory addr hexnum
  6052                                                          mov     al,' '                                          ;ascii space
  6053                                                          stosb                                                   ;store delimiter
  6054                                  ;
  6055                                  ;       Output 16 ASCII hexadecimal byte values for the row.
  6056                                  ;
  6057                                                          xor     ecx,ecx                                         ;zero register
  6058                                                          mov     cl,16                                           ;loop count
  6059                                  .30                     push    ecx                                             ;save loop count
  6060                                                          lodsb                                                   ;memory byte
  6061                                                          mov     ah,al                                           ;memory byte
  6062                                                          shr     al,4                                            ;high-order in bits 3-0
  6063                                                          or      al,30h                                          ;apply ascii numeric zone
  6064                                                          cmp     al,3ah                                          ;numeric range?
  6065                                                          jb      .40                                             ;yes, skip ahead
  6066                                                          add     al,7                                            ;adjust ascii for 'A'-'F'
  6067                                  .40                     stosb                                                   ;store ascii hexadecimal of high-order
  6068                                                          mov     al,ah                                           ;low-order in bits 3-0
  6069                                                          and     al,0fh                                          ;mask out high-order bits
  6070                                                          or      al,30h                                          ;apply ascii numeric zone
  6071                                                          cmp     al,3ah                                          ;numeric range?
  6072                                                          jb      .50                                             ;yes, skip ahead
  6073                                                          add     al,7                                            ;adjust ascii for 'A'-'F'
  6074                                  .50                     stosb                                                   ;store ascii hexadecimal of low-order
  6075                                                          mov     al,' '                                          ;ascii space
  6076                                                          stosb                                                   ;store ascii space delimiter
  6077                                                          pop     ecx                                             ;loop count
  6078                                                          loop    .30                                             ;next
  6079                                  ;
  6080                                  ;       Output printable ASCII character section for the row.
  6081                                  ;
  6082                                                          sub     esi,16                                          ;reset source pointer
  6083                                                          mov     cl,16                                           ;loop count
  6084                                  .60                     lodsb                                                   ;source byte
  6085                                                          cmp     al,32                                           ;printable? (low-range test)
  6086                                                          jb      .70                                             ;no, skip ahead
  6087                                                          cmp     al,128                                          ;printable? (high-range test)
  6088                                                          jb      .80                                             ;yes, skip ahead
  6089                                  .70                     mov     al,' '                                          ;display space instead of printable
  6090                                  .80                     stosb                                                   ;store printable ascii byte
  6091                                                          loop    .60                                             ;next source byte
  6092                                                          xor     al,al                                           ;nul-terminator
  6093                                                          stosb                                                   ;terminate output line
  6094                                  ;
  6095                                  ;       Display constructed output buffer and newline.
  6096                                  ;
  6097                                                          putConsoleString wzConsoleOutBuffer                     ;display constructed output
  6098                                                          call    ConPutNewLine                                   ;display new line
  6099                                  ;
  6100                                  ;       Repeat until all lines displayed and preserve source address.
  6101                                  ;
  6102                                                          pop     ecx                                             ;remaining rows
  6103                                                          loop    .20                                             ;next row
  6104                                                          mov     [wdConsoleMemBase],esi                          ;update console memory address
  6105                                                          pop     edi                                             ;restore regs
  6106                                                          pop     esi                                             ;
  6107                                                          pop     ebx                                             ;
  6108                                                          ret                                                     ;return
