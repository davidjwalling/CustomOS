  4687                                                          xor     ecx,ecx                                         ;zero register
  4688                                                          mov     cl,8                                            ;nybble count
  4689                                  .10                     rol     edx,4                                           ;next hi-order nybble in bits 0-3
  4690                                                          mov     al,dl                                           ;????bbbb
  4691                                                          and     al,00Fh                                         ;mask out bits 4-7
