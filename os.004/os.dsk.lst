  1094 0000000C 00<rept>                                        times   10 db 0                                         ;unused
  1095 00000016 0000                                            dw      0h                                              ;time
  1096 00000018 41                                              db      01000001b                                       ;mmm = 10 MOD 8 = 2; ddddd = 1
  1097 00000019 49                                              db      01001001b                                       ;yyyyyyy = 2016-1980 = 36 = 24h; m/8 = 1
  1098 0000001A 0200                                            dw      2                                               ;first cluster
  1099 0000001C 00540000                                        dd      05400h                                          ;file size
  1100 00000020 00<rept>                                        times   (EBOOTDIRENTRIES*32)-($-$$) db 0h               ;zero fill to end of section
