  1310 000000B4 7406                                            jz      .20                                             ;... yes, exit our loop
  1311 000000B6 B40E                                            mov     ah,EBIOSFNTTYOUTPUT                             ;BIOS teletype function
  1312 000000B8 CD10                                            int     EBIOSINTVIDEO                                   ;call BIOS display interrupt
  1313 000000BA EBF5                                            jmp     .10                                             ;repeat until done
  1314 000000BC C3                      .20                     ret                                                     ;return
