   571 00000030 8EDB                                            mov     ds,bx                                           ;DS = 07b0 = psp
   572 00000032 8EC3                                            mov     es,bx                                           ;ES = 07b0 = psp
   573 00000034 8ED3                                            mov     ss,bx                                           ;SS = 07b0 = psp (ints disabled)
   574 00000036 BC0001                                          mov     sp,EBOOTSTACKTOP                                ;SP = 0100       (ints enabled)
