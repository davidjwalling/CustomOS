  1252 0000008C FB                      .50                     sti                                                     ;enable maskable interrupts
  1253 0000008D F4                                              hlt                                                     ;stop until reset, int, nmi
  1254 0000008E EBFC                                            jmp     .50                                             ;loop until restart kicks in
