  2475                                  ;
  2476                                  ;       Routine:        PlaceCursor
  2477                                  ;
  2478                                  ;       Description:    This routine positions the cursor on the console.
  2479                                  ;
  2480                                  ;       In:             DS      OS data selector
  2481                                  ;
  2482                                  ;-----------------------------------------------------------------------------------------------------------------------
  2483                                  PlaceCursor             push    ecx                                             ;save non-volatile regs
  2484                                                          mov     al,[wbConsoleRow]                               ;AL = row
  2485                                                          mov     ah,ECONCOLS                                     ;AH = cols/row
  2486                                                          mul     ah                                              ;row offset
  2487                                                          add     al,[wbConsoleColumn]                            ;add column
  2488                                                          adc     ah,0                                            ;add overflow
  2489                                                          mov     ecx,eax                                         ;screen offset
  2490                                                          mov     dl,ECRTPORTLO                                   ;crt controller port lo
  2491                                                          mov     dh,ECRTPORTHI                                   ;crt controller port hi
  2492                                                          mov     al,ECRTCURLOCHI                                 ;crt cursor loc reg hi
  2493                                                          out     dx,al                                           ;select register
  2494                                                          inc     edx                                             ;data port
  2495                                                          mov     al,ch                                           ;hi-order cursor loc
  2496                                                          out     dx,al                                           ;store hi-order loc
  2497                                                          dec     edx                                             ;register select port
  2498                                                          mov     al,ECRTCURLOCLO                                 ;crt cursor loc reg lo
  2499                                                          out     dx,al                                           ;select register
  2500                                                          inc     edx                                             ;data port
  2501                                                          mov     al,cl                                           ;lo-order cursor loc
  2502                                                          out     dx,al                                           ;store lo-order loc
  2503                                                          pop     ecx                                             ;restore non-volatile regs
  2504                                                          ret                                                     ;return
