  2038                                                          cmp     cl,EASCIIUPPERZ                                 ;greater than 'Z'?
  2039                                                          ja      irq1.100                                        ;yes, skip ahead
  2040                                                          xor     al,EASCIICASE                                   ;switch case
  2041                                  irq1.100                mov     [wbConsoleChar],al                              ;save ASCII code
  2042                                  irq1.130                jmp     irq1.150                                        ;finish keyboard handling
  2043                                  irq1.140                mov     al,EKEYFTIMEOUT                                 ;controller timeout flag
  2044                                                          or      [wbConsoleStatus],al                            ;set controller timeout flag
