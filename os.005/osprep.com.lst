  2435                                  ;-----------------------------------------------------------------------------------------------------------------------
  2436                                  ;
  2437                                  ;       Routine:        SetConsoleChar
  2438                                  ;
  2439                                  ;       Description:    This routine outputs an ASCII character at the given row and column.
  2440                                  ;
  2441                                  ;       In:             AL      ASCII character
  2442                                  ;                       CL      column
  2443                                  ;                       CH      row
  2444                                  ;                       ES      CGA selector
  2445                                  ;
  2446                                  ;       Out:            EAX     last target address written (ES:)
  2447                                  ;                       CL      column + 1
  2448                                  ;
  2449                                  ;-----------------------------------------------------------------------------------------------------------------------
  2450                                  SetConsoleChar          mov     dl,al                                           ;ASCII character
  2451                                                          movzx   eax,ch                                          ;row
  2452                                                          mov     ah,ECONCOLS                                     ;cols/row
  2453                                                          mul     ah                                              ;row * cols/row
  2454                                                          add     al,cl                                           ;add column
  2455                                                          adc     ah,0                                            ;handle carry
  2456                                                          shl     eax,1                                           ;screen offset
  2457                                                          mov     [es:eax],dl                                     ;store character
  2458                                                          inc     cl                                              ;next column
  2459                                                          ret                                                     ;return
