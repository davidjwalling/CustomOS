  2280                                  ;
  2281                                  ;       Routine:        PutConsoleHexByte
  2282                                  ;
  2283                                  ;       Description:    This routine writes two ASCII characters to the console representing a byte value.
  2284                                  ;
  2285                                  ;       In:             AL      byte value
  2286                                  ;                       CL      column
  2287                                  ;                       CH      row
  2288                                  ;                       DS      OS data selector
  2289                                  ;                       ES      CGA selector
  2290                                  ;
  2291                                  ;-----------------------------------------------------------------------------------------------------------------------
  2292                                  PutConsoleHexByte       push    eax                                             ;save non-volatile regs
  2293                                                          shr     al,4                                            ;hi-order nybble
  2294                                                          call    .10                                             ;make ASCII and store
  2295                                                          pop     eax                                             ;byte value
  2296                                                          and     al,0Fh                                          ;lo-order nybble
  2297                                  .10                     or      al,030h                                         ;apply ASCII zone
  2298                                                          cmp     al,03Ah                                         ;numeric?
  2299                                                          jb      .20                                             ;yes, skip ahead
  2300                                                          add     al,7                                            ;add ASCII offset for alpha
  2301                                  .20                     call    SetConsoleChar                                  ;display ASCII character
  2302                                                          ret                                                     ;return
  2303                                  ;-----------------------------------------------------------------------------------------------------------------------
  2304                                  ;
  2305                                  ;       Routine:        PutConsoleOIAChar
  2306                                  ;
  2307                                  ;       Description:    This routine updates the Operator Information Area (OIA).
  2308                                  ;
  2309                                  ;       In:             DS      OS data selector
  2310                                  ;
  2311                                  ;-----------------------------------------------------------------------------------------------------------------------
  2312                                  PutConsoleOIAChar       push    ebx                                             ;save non-volatile regs
  2313                                                          push    ecx                                             ;
  2314                                                          push    esi                                             ;
  2315                                                          push    ds                                              ;
  2316                                                          push    es                                              ;
  2317                                                          push    EGDTOSDATA                                      ;load OS data selector ...
  2318                                                          pop     ds                                              ;... into data segment register
  2319                                                          push    EGDTCGA                                         ;load CGA selector ...
  2320                                                          pop     es                                              ;... into extra segment register
  2321                                                          mov     esi,wbConsoleScan0                              ;scan codes address
  2322                                                          mov     bh,ECONOIAROW                                   ;OIA row
  2323                                                          mov     bl,0                                            ;starting column
  2324                                                          mov     ecx,6                                           ;maximum scan codes
  2325                                  .10                     push    ecx                                             ;save remaining count
  2326                                                          mov     ecx,ebx                                         ;row, column
  2327                                                          lodsb                                                   ;read scan code
  2328                                                          or      al,al                                           ;scan code present?
  2329                                                          jz      .20                                             ;no, skip ahead
  2330                                                          call    PutConsoleHexByte                               ;display scan code
  2331                                                          jmp     .30                                             ;continue
  2332                                  .20                     mov     al,' '                                          ;ASCII space
  2333                                                          call    SetConsoleChar                                  ;display space
  2334                                                          mov     al,' '                                          ;ASCII space
  2335                                                          call    SetConsoleChar                                  ;display space
  2336                                  .30                     add     bl,2                                            ;next column (+2)
  2337                                                          pop     ecx                                             ;restore remaining
  2338                                                          loop    .10                                             ;next code
  2339                                                          mov     al,[wbConsoleChar]                              ;console ASCII character
  2340                                                          cmp     al,32                                           ;printable? (lower-bounds)
  2341                                                          jb      .40                                             ;no, skip ahead
  2342                                                          cmp     al,126                                          ;printable? (upper-bounds)
  2343                                                          ja      .40                                             ;no, skip ahead
  2344                                                          mov     ch,bh                                           ;OIA row
  2345                                                          mov     cl,40                                           ;character display column
  2346                                                          call    SetConsoleChar                                  ;display ASCII character
  2347                                  .40                     pop     es                                              ;restore non-volatile regs
  2348                                                          pop     ds                                              ;
  2349                                                          pop     esi                                             ;
  2350                                                          pop     ecx                                             ;
  2351                                                          pop     ebx                                             ;
  2352                                                          ret                                                     ;return
  2353                                  ;-----------------------------------------------------------------------------------------------------------------------
  2354                                  ;
  2355                                  ;       Routine:        PutConsoleOIAShift
  2356                                  ;
  2357                                  ;       Description:    This routine updates the shift/ctrl/alt/lock indicators in the operator information area (OIA).
  2358                                  ;
  2359                                  ;       In:             BL      shift flags
  2360                                  ;                       BH      lock flags
  2361                                  ;                       DS      OS data selector
  2362                                  ;
  2363                                  ;-----------------------------------------------------------------------------------------------------------------------
  2364                                  PutConsoleOIAShift      push    ecx                                             ;save non-volatile regs
  2365                                                          push    es                                              ;
  2366                                                          push    EGDTCGA                                         ;load CGA selector ...
  2367                                                          pop     es                                              ;... into ES register
  2368                                                          mov     ch,ECONOIAROW                                   ;OIA row
  2369                                                          mov     al,EASCIISPACE                                  ;space is default character
  2370                                                          test    bl,EKEYFSHIFTLEFT                               ;left-shift indicated?
  2371                                                          jz      .10                                             ;no, skip ahead
  2372                                                          mov     al,'S'                                          ;yes, indicate with 'S'
  2373                                  .10                     mov     cl,14                                           ;indicator column
  2374                                                          call    SetConsoleChar                                  ;display ASCII character
  2375                                                          mov     al,EASCIISPACE                                  ;ASCII space
  2376                                                          test    bl,EKEYFSHIFTRIGHT                              ;right-shift indicated?
  2377                                                          jz      .20                                             ;no, skip ahead
  2378                                                          mov     al,'S'                                          ;yes, indicate with 'S'
  2379                                  .20                     mov     cl,64                                           ;indicator column
  2380                                                          call    SetConsoleChar                                  ;display ASCII character
  2381                                                          mov     al,EASCIISPACE                                  ;ASCII space
  2382                                                          test    bl,EKEYFCTRLLEFT                                ;left-ctrl indicated?
  2383                                                          jz      .30                                             ;no, skip ahead
  2384                                                          mov     al,'C'                                          ;yes, indicate with 'C'
  2385                                  .30                     mov     cl,15                                           ;indicator column
  2386                                                          call    SetConsoleChar                                  ;display ASCII character
  2387                                                          mov     al,EASCIISPACE                                  ;ASCII space
  2388                                                          test    bl,EKEYFCTRLRIGHT                               ;right-ctrl indicated?
  2389                                                          jz      .40                                             ;no, skip ahead
  2390                                                          mov     al,'C'                                          ;yes, indicate with 'C'
  2391                                  .40                     mov     cl,63                                           ;indicator column
  2392                                                          call    SetConsoleChar                                  ;display ASCII character
  2393                                                          mov     al,EASCIISPACE                                  ;ASCII space
  2394                                                          test    bl,EKEYFALTLEFT                                 ;left-alt indicated?
  2395                                                          jz      .50                                             ;no, skip ahead
  2396                                                          mov     al,'A'                                          ;yes, indicate with 'A'
  2397                                  .50                     mov     cl,16                                           ;indicator column
  2398                                                          call    SetConsoleChar                                  ;display ASCII character
  2399                                                          mov     al,EASCIISPACE                                  ;ASCII space
  2400                                                          test    bl,EKEYFALTRIGHT                                ;right-alt indicated?
  2401                                                          jz      .60                                             ;no, skip ahead
  2402                                                          mov     al,'A'                                          ;yes, indicate with 'A'
  2403                                  .60                     mov     cl,62                                           ;indicator column
  2404                                                          call    SetConsoleChar                                  ;display ASCII character
  2405                                                          mov     al,EASCIISPACE                                  ;ASCII space
  2406                                                          test    bh,EKEYFLOCKCAPS                                ;caps-lock indicated?
  2407                                                          jz      .70                                             ;no, skip ahead
  2408                                                          mov     al,'C'                                          ;yes, indicate with 'C'
  2409                                  .70                     mov     cl,78                                           ;indicator column
  2410                                                          call    SetConsoleChar                                  ;display ASCII character
  2411                                                          mov     al,EASCIISPACE                                  ;ASCII space
  2412                                                          test    bh,EKEYFLOCKNUM                                 ;num-lock indicated?
  2413                                                          jz      .80                                             ;no, skip ahead
  2414                                                          mov     al,'N'                                          ;yes, indicate with 'N'
  2415                                  .80                     mov     cl,77                                           ;indicator column
  2416                                                          call    SetConsoleChar                                  ;display ASCII character
  2417                                                          mov     al,EASCIISPACE                                  ;ASCII space
  2418                                                          test    bh,EKEYFLOCKSCROLL                              ;scroll-lock indicated?
  2419                                                          jz      .90                                             ;no, skip ahead
  2420                                                          mov     al,'S'                                          ;yes, indicate with 'S'
  2421                                  .90                     mov     cl,76                                           ;indicator column
  2422                                                          call    SetConsoleChar                                  ;display ASCII character
  2423                                                          pop     es                                              ;restore non-volatile regs
  2424                                                          pop     ecx                                             ;
  2425                                                          ret                                                     ;return
