  2510                                  ;
  2511                                  ;-----------------------------------------------------------------------------------------------------------------------
  2512                                  PutPrimaryEndOfInt      mov     al,EPICEOI                                      ;non-specific end-of-interrupt
  2513                                                          out     EPICPORTPRI,al                                  ;send EOI to primary PIC
  2514                                                          ret                                                     ;return
