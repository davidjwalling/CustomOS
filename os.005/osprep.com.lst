  2027                                                          test    bl,EKEYFSHIFT                                   ;either shift key down?
  2028                                                          jz      irq1.90                                         ;no, skip ahead
  2029                                                          mov     esi,tscan2shift                                 ;scan-to-shifted table address
  2030                                  irq1.90                 movzx   ecx,ah                                          ;scan code offset
  2031                                                          mov     al,[cs:ecx+esi]                                 ;al = ASCII code
  2032                                                          test    bh,EKEYFLOCKCAPS                                ;caps-lock on?
  2033                                                          jz      irq1.100                                        ;no skip ahead
