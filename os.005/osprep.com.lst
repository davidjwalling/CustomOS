  1903                                                          pop     ds                                              ;... into data segment register
  1904                                                          xor     al,al                                           ;zero
  1905                                                          mov     [wbConsoleScan0],al                             ;clear scan code 0
  1906                                                          mov     [wbConsoleScan1],al                             ;clear scan code 1
  1907                                                          mov     [wbConsoleScan2],al                             ;clear scan code 2
  1908                                                          mov     [wbConsoleScan3],al                             ;clear scan code 3
  1909                                                          mov     [wbConsoleScan4],al                             ;clear scan code 4
  1910                                                          mov     [wbConsoleScan5],al                             ;clear scan code 5
  1911                                                          mov     al,' '                                          ;space
  1912                                                          mov     [wbConsoleChar],al                              ;set character to space
  1913                                                          mov     al,EKEYFTIMEOUT                                 ;controller timeout flag
  1914                                                          not     al                                              ;controller timeout mask
  1915                                                          and     [wbConsoleStatus],al                            ;clear controller timeout flag
  1916                                                          mov     bl,[wbConsoleShift]                             ;shift flags
  1917                                                          mov     bh,[wbConsoleLock]                              ;lock flags
  1918                                                          call    WaitForKeyOutBuffer                             ;controller timeout?
  1919                                                          jz      irq1.140                                        ;yes, skip ahead
  1920                                                          in      al,EKEYBPORTDATA                                ;read scan code 0
  1921                                                          mov     [wbConsoleScan0],al                             ;save scan code 0
  1922                                                          mov     ah,al                                           ;copy scan code 0
  1923                                                          mov     al,EKEYFSHIFTLEFT                               ;left shift flag
  1924                                                          cmp     ah,EKEYBSHIFTLDOWN                              ;left shift key down code?
  1925                                                          je      irq1.30                                         ;yes, set flag
  1926                                                          cmp     ah,EKEYBSHIFTLUP                                ;left shift key up code?
  1927                                                          je      irq1.40                                         ;yes, reset flag
  1928                                                          mov     al,EKEYFSHIFTRIGHT                              ;right shift flag
  1929                                                          cmp     ah,EKEYBSHIFTRDOWN                              ;right shift key down code?
  1930                                                          je      irq1.30                                         ;yes, set flag
  1931                                                          cmp     ah,EKEYBSHIFTRUP                                ;right shift key up code?
  1932                                                          je      irq1.40                                         ;yes, reset flag
  1933                                                          mov     al,EKEYFCTRLLEFT                                ;left control flag
  1934                                                          cmp     ah,EKEYBCTRLDOWN                                ;control key down code?
  1935                                                          je      irq1.30                                         ;yes, set flag
  1936                                                          cmp     ah,EKEYBCTRLUP                                  ;control key up code?
  1937                                                          je      irq1.40                                         ;yes, reset flag
  1938                                                          mov     al,EKEYFALTLEFT                                 ;left alt flag
  1939                                                          cmp     ah,EKEYBALTDOWN                                 ;alt key down code?
  1940                                                          je      irq1.30                                         ;yes, set flag
  1941                                                          cmp     ah,EKEYBALTUP                                   ;alt key up code?
  1942                                                          je      irq1.40                                         ;yes, reset flag
  1943                                                          mov     al,EKEYFLOCKCAPS                                ;caps-lock flag
  1944                                                          cmp     ah,EKEYBCAPSDOWN                                ;caps-lock key down code?
  1945                                                          je      irq1.50                                         ;yes, toggle lamps and flags
  1946                                                          mov     al,EKEYFLOCKNUM                                 ;num-lock flag
  1947                                                          cmp     ah,EKEYBNUMDOWN                                 ;num-lock key down code?
  1948                                                          je      irq1.50                                         ;yes, toggle lamps and flags
  1949                                                          mov     al,EKEYFLOCKSCROLL                              ;scroll-lock flag
  1950                                                          cmp     ah,EKEYBSCROLLDOWN                              ;scroll-lock key down code?
  1951                                                          je      irq1.50                                         ;yes, toggle lamps and flags
  1952                                                          cmp     ah,EKEYBCODEEXT0                                ;extended scan code 0?
  1953                                                          jne     irq1.70                                         ;no, skip ahead
  1954                                                          call    WaitForKeyOutBuffer                             ;controller timeout?
  1955                                                          jz      irq1.140                                        ;yes, skip ahead
  1956                                                          in      al,EKEYBPORTDATA                                ;read scan code 1
  1957                                                          mov     [wbConsoleScan1],al                             ;save scan code 1
  1958                                                          mov     ah,al                                           ;copy scan code 1
  1959                                                          mov     al,EKEYFCTRLRIGHT                               ;right control flag
  1960                                                          cmp     ah,EKEYBCTRLDOWN                                ;control key down code?
  1961                                                          je      irq1.30                                         ;yes, set flag
  1962                                                          cmp     ah,EKEYBCTRLUP                                  ;control key up code?
  1963                                                          je      irq1.40                                         ;yes, reset flag
  1964                                                          mov     al,EKEYFALTRIGHT                                ;right alt flag
  1965                                                          cmp     ah,EKEYBALTDOWN                                 ;alt key down code?
  1966                                                          je      irq1.30                                         ;yes, set flag
  1967                                                          cmp     ah,EKEYBALTUP                                   ;alt key up code?
  1968                                                          je      irq1.40                                         ;yes, reset flag
  1969                                                          cmp     ah,EKEYBSLASH                                   ;slash down code?
  1970                                                          je      irq1.80                                         ;yes, skip ahead
  1971                                                          cmp     ah,EKEYBSLASHUP                                 ;slash up code?
  1972                                                          je      irq1.80                                         ;yes, skip ahead
  1973                                                          cmp     ah,EKEYBPRTSCRDOWN                              ;print screen down code?
  1974                                                          je      irq1.10                                         ;yes, continue
  1975                                                          cmp     ah,EKEYBPRTSCRUP                                ;print screen up code?
  1976                                                          jne     irq1.20                                         ;no, skip ahead
  1977                                  irq1.10                 call    WaitForKeyOutBuffer                             ;controller timeout?
  1978                                                          jz      irq1.140                                        ;yes, skip ahead
  1979                                                          in      al,EKEYBPORTDATA                                ;read scan code 2
  1980                                                          mov     [wbConsoleScan2],al                             ;save scan code 2
  1981                                                          call    WaitForKeyOutBuffer                             ;controller timeout?
  1982                                                          jz      irq1.140                                        ;yes, skip ahead
  1983                                                          in      al,EKEYBPORTDATA                                ;read scan code 3
  1984                                                          mov     [wbConsoleScan3],al                             ;read scan code 3
  1985                                  irq1.20                 jmp     irq1.150                                        ;finish keyboard handling
  1986                                  irq1.30                 or      bl,al                                           ;set shift flag
  1987                                                          jmp     irq1.60                                         ;skip ahead
  1988                                  irq1.40                 not     al                                              ;convert flag to mask
  1989                                                          and     bl,al                                           ;reset shift flag
  1990                                                          jmp     irq1.60                                         ;skip ahead
  1991                                  irq1.50                 xor     bh,al                                           ;toggle lock flag
  1992                                                          call    SetKeyboardLamps                                ;update keyboard lamps
  1993                                  irq1.60                 mov     [wbConsoleShift],bl                             ;save shift flags
