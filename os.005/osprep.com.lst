  1997                                  irq1.70                 cmp     ah,EKEYBCODEEXT1                                ;extended scan code 1?
  1998                                                          jne     irq1.80                                         ;no continue
  1999                                                          call    WaitForKeyOutBuffer                             ;controller timeout?
  2000                                                          jz      irq1.140                                        ;yes, skip ahead
  2001                                                          in      al,EKEYBPORTDATA                                ;read scan code 1
  2002                                                          mov     [wbConsoleScan1],al                             ;save scan code 1
  2003                                                          mov     ah,al                                           ;copy scan code 1
  2004                                                          cmp     ah,EKEYBPAUSEDOWN                               ;pause key down code?
  2005                                                          jne     irq1.150                                        ;no, finish keyboard handling
  2006                                                          call    WaitForKeyOutBuffer                             ;controller timeout?
  2007                                                          jz      irq1.140                                        ;yes, skip ahead
  2008                                                          in      al,EKEYBPORTDATA                                ;read scan code 2
  2009                                                          mov     [wbConsoleScan2],al                             ;save scan code 2
  2010                                                          call    WaitForKeyOutBuffer                             ;controller timeout?
  2011                                                          jz      irq1.140                                        ;yes, skip ahead
  2012                                                          in      al,EKEYBPORTDATA                                ;read scan code 3
  2013                                                          mov     [wbConsoleScan3],al                             ;save scan code 3
  2014                                                          call    WaitForKeyOutBuffer                             ;controller timeout?
  2015                                                          jz      irq1.140                                        ;yes, skip ahead
  2016                                                          in      al,EKEYBPORTDATA                                ;read scan code 4
  2017                                                          mov     [wbConsoleScan4],al                             ;save scan code 4
  2018                                                          call    WaitForKeyOutBuffer                             ;controller timeout?
  2019                                                          jz      irq1.140                                        ;yes, skip ahead
