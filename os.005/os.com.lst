  2442 00000028 00000000                			dd	0						;28 eax
  2443 0000002C 00000000                			dd	0						;2C ecx
  2444 00000030 00000000                			dd	0						;30 edx
  2445 00000034 00000000                			dd	0						;34 ebx
  2446 00000038 00070000                			dd	0700h						;38 esp ring 3
  2447 0000003C 00000000                			dd	0						;3C ebp
  2448 00000040 00000000                			dd	0						;40 esi
  2449 00000044 00000000                			dd	0						;44 edi
  2450 00000048 1C000000                			dd	001Ch						;48 es
  2451 0000004C 24000000                			dd	0024h						;4C cs
