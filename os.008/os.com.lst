  2786 0000095A 89DF                                            mov     edi,ebx                                         ;second source address
  2787 0000095C FC                                              cld                                                     ;forward strings
  2788 0000095D F3A6                                            rep     cmpsb                                           ;compare bytes
  2789 0000095F B000                                            mov     al,0                                            ;default result
