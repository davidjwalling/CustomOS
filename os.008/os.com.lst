  1285 00000038 07                                              pop     es                                              ;... as target segment
  1286 00000039 31FF                                            xor     di,di                                           ;ES:DI = target address
  1287 0000003B BE0005                                          mov     si,EKRNCODESRCADR                               ;DS:SI = source address
  1288 0000003E B90050                                          mov     cx,EKRNCODELEN                                  ;CX = kernel size
  1289 00000041 FC                                              cld                                                     ;forward strings
  1290 00000042 F3A4                                            rep     movsb                                           ;copy kernel image
  1291 00000044 BE[4B01]                                        mov     si,czKernelLoadedMsg                            ;kernel moved message
  1292 00000047 E86600                                          call    PutTTYString                                    ;display message
  1293                                  ;
  1294                                  ;       Switch to protected mode.
  1295                                  ;
