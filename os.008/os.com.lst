  1272 0000001D 8CC8                                            mov     ax,cs                                           ;AX:SI = gdt source
  1273 0000001F C1C004                                          rol     ax,4                                            ;AX = phys addr bits 11-0,15-12
  1274 00000022 88C1                                            mov     cl,al                                           ;CL = phys addr bits 3-0,15-12
  1275 00000024 24F0                                            and     al,0F0h                                         ;AL = phys addr bits 11-0
  1276 00000026 80E10F                                          and     cl,00Fh                                         ;CL = phys addr bits 15-12
