  1278 0000002C 884C34                                          mov     byte [si+EGDTLOADERCODE+4],cl                   ;lo-order loader code (16-23)
  1279 0000002F BE[3C01]                                        mov     si,czGDTOKMsg                                   ;GDT prepared message
  1280 00000032 E87B00                                          call    PutTTYString                                    ;display message
  1281                                  ;
  1282                                  ;       Move the 32-bit kernel to its appropriate memory location.
  1283                                  ;
