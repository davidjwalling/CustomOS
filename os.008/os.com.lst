  1311 00000065 E621                                            out     EPICPORTPRI1,al                                 ;enable all primary 8259A ints
  1312 00000067 E6A1                                            out     EPICPORTSEC1,al                                 ;enable all secondary 8259A ints
  1313 00000069 FB                                              sti                                                     ;enable maskable interrupts
