  1297 0000004C 8ED6                                            mov     ss,si                                           ;protected mode ss
  1298 0000004E BC0010                                          mov     sp,EKRNCODEBASE                                 ;initial stack immediate before code
  1299 00000051 B489                                            mov     ah,EBIOSFNINITPROTMODE                          ;initialize protected mode fn.
  1300 00000053 BB2820                                          mov     bx,02028h                                       ;BH,BL = IRQ int bases
  1301 00000056 BA1F00                                          mov     dx,001Fh                                        ;outer delay loop count
  1302 00000059 B9FFFF                  .10                     mov     cx,0FFFFh                                       ;inner delay loop count
  1303 0000005C E2FE                                            loop    $                                               ;wait out pending interrupts
  1304 0000005E 4A                                              dec     dx                                              ;restore outer loop count
  1305 0000005F 75F8                                            jnz     .10                                             ;continue outer loop
