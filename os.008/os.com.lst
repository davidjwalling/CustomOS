  1262 0000000D BE[C800]                                        mov     si,czCPUErrorMsg                                ;loader error message
  1263 00000010 3C03                                            cmp     al,3                                            ;80386+?
  1264 00000012 7267                                            jb      LoaderExit                                      ;no, exit with error message
  1265                                                          cpu     386                                             ;allow 80386 instructions
  1266 00000014 BE[3301]                                        mov     si,czCPUOKMsg                                   ;cpu ok message
  1267 00000017 E89600                                          call    PutTTYString                                    ;display message
  1268                                  ;
  1269                                  ;       Fixup the GDT descriptor for the current (loader) code segment.
  1270                                  ;
