  3334 00000C38 6A2C                                            push    ELDTMQ                                          ;load task message queue selector ...
  3335 00000C3A 1F                                              pop     ds                                              ;... into data segment register
  3336 00000C3B A104000000                                      mov     eax,[MQTail]                                    ;tail ptr
  3337 00000C40 833800                                          cmp     dword [eax],0                                   ;is queue full?
  3338 00000C43 F9                                              stc                                                     ;assume failure
  3339 00000C44 7517                                            jne     .20                                             ;yes, cannot store
  3340 00000C46 8910                                            mov     [eax],edx                                       ;store lo-order data
  3341 00000C48 894804                                          mov     [eax+4],ecx                                     ;store hi-order data
  3342 00000C4B 83C008                                          add     eax,8                                           ;next queue element adr
  3343 00000C4E 25FC030000                                      and     eax,03fch                                       ;at end of queue?
  3344 00000C53 7502                                            jnz     .10                                             ;no, skip ahead
  3345 00000C55 B008                                            mov     al,8                                            ;reset to top of queue
  3346 00000C57 A304000000              .10                     mov     [MQTail],eax                                    ;save new tail ptr
  3347 00000C5C F8                                              clc                                                     ;indicate success
  3348 00000C5D 1F                      .20                     pop     ds                                              ;restore non-volatile regs
  3349 00000C5E C3                                              ret                                                     ;return
