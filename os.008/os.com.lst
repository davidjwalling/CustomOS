  3300 00000C07 1F                                              pop     ds                                              ;... into data segment register
  3301 00000C08 8B1D00000000                                    mov     ebx,[MQHead]                                    ;head ptr
  3302 00000C0E 8B03                                            mov     eax,[ebx]                                       ;lo-order 32 bits
  3303 00000C10 8B5304                                          mov     edx,[ebx+4]                                     ;hi-order 32 bits
  3304 00000C13 09D0                                            or      eax,edx                                         ;is queue empty?
  3305 00000C15 F9                                              stc                                                     ;assume queue is emtpy
  3306 00000C16 741B                                            jz      .20                                             ;yes, skip ahead
  3307 00000C18 31C9                                            xor     ecx,ecx                                         ;store zero
  3308 00000C1A 890B                                            mov     [ebx],ecx                                       ;... in lo-order dword
  3309 00000C1C 894B04                                          mov     [ebx+4],ecx                                     ;... in hi-order dword
  3310 00000C1F 83C308                                          add     ebx,8                                           ;next queue element
  3311 00000C22 81E3FC030000                                    and     ebx,03fch                                       ;at end of queue?
  3312 00000C28 7502                                            jnz     .10                                             ;no, skip ahead
  3313 00000C2A B308                                            mov     bl,8                                            ;reset to 1st entry
  3314 00000C2C 891D00000000            .10                     mov     [MQHead],ebx                                    ;save new head ptr
  3315 00000C32 F8                                              clc                                                     ;indicate message read
  3316 00000C33 1F                      .20                     pop     ds                                              ;restore non-volatile regs
  3317 00000C34 59                                              pop     ecx                                             ;
  3318 00000C35 5B                                              pop     ebx                                             ;
  3319 00000C36 C3                                              ret                                                     ;return
