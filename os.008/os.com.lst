  2795 0000096C 07                                              pop     es                                              ;restore non-volatile regs
  2796 0000096D 5F                                              pop     edi                                             ;
  2797 0000096E 5E                                              pop     esi                                             ;
  2798 0000096F C3                                              ret                                                     ;return
