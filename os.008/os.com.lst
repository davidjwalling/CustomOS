  2808 00000957 07                                              pop     es                                              ;... to ES
  2809 00000958 89D6                                            mov     esi,edx                                         ;first source address
  2810 0000095A 89DF                                            mov     edi,ebx                                         ;second source address
  2811 0000095C FC                                              cld                                                     ;forward strings
  2812 0000095D F3A6                                            rep     cmpsb                                           ;compare bytes
  2813 0000095F B000                                            mov     al,0                                            ;default result
