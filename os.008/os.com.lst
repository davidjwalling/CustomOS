  3621 00000018 FFFF00000093CF00                                dq      00CF93000000FFFFh                               ;1c data
  3622 00000020 FF0F0050009B4000                                dq      00409B0050000FFFh                               ;24 code
  3623 00000028 FF07004800934000                                dq      00409300480007FFh                               ;2c message queue
  3624 00000030 00<rept>                                        times   128-($-$$) db 0h                                ;zero fill to end of section
