  1247                                  section                 loader  vstart=0100h                                    ;use .COM compatible addressing
  1248                                                          bits    16                                              ;this is 16-bit code
  1249 00000000 0E                      Loader                  push    cs                                              ;use the code segment
  1250 00000001 1F                                              pop     ds                                              ;...as our data segment
  1251 00000002 0E                                              push    cs                                              ;use the code segment
  1252 00000003 07                                              pop     es                                              ;...as our extra segment
  1253                                  ;
  1254                                  ;       Write a message to the console so we know we have our addressability established.
  1255                                  ;
