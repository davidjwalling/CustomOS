  2748                                  ;
  2749                                  ;	Routine:	ClearConsoleScreen
  2750                                  ;
  2751                                  ;	Description:	This routine clears the console (CGA) screen.
  2752                                  ;
  2753                                  ;-----------------------------------------------------------------------------------------------------------------------
  2754                                  ClearConsoleScreen	push	ecx						;save non-volatile regs
  2755                                  			push	edi						;
  2756                                  			push	ds						;
  2757                                  			push	es						;
  2758                                  			push	ESELDAT						;load OS Data selector ...
  2759                                  			pop	ds						;... into DS register
  2760                                  			push	ESELCGA						;load CGA selector ...
  2761                                  			pop	es						;... into ES register
  2762                                  			mov	eax,ECONCLEARDWORD				;initializtion value
  2763                                  			mov	ecx,ECONROWDWORDS*(ECONROWS)			;double-words to clear
  2764                                  			xor	edi,edi						;target offset
  2765                                  			cld							;forward strings
  2766                                  			rep	stosd						;reset screen body
  2767                                  			mov	eax,ECONOIADWORD				;OIA attribute and space
  2768                                  			mov	ecx,ECONROWDWORDS				;double-words per row
  2769                                  			rep	stosd						;reset OIA line
  2770                                  			xor	al,al						;zero register
  2771                                  			mov	[wbConsoleRow],al				;reset console row
  2772                                  			mov	[wbConsoleColumn],al				;reset console column
  2773                                  			call	PlaceCursor					;place cursor at current position
  2774                                  			pop	es						;restore non-volatile regs
  2775                                  			pop	ds						;
  2776                                  			pop	edi						;
  2777                                  			pop	ecx						;
  2778                                  			ret							;return
  2779                                  ;-----------------------------------------------------------------------------------------------------------------------
  2780                                  ;
  2781                                  ;	Routine:	ScrollConsoleRow
  2782                                  ;
  2783                                  ;	Description:	This routine scrolls the console (text) screen up one row.
  2784                                  ;
  2785                                  ;-----------------------------------------------------------------------------------------------------------------------
  2786                                  ScrollConsoleRow	push	ecx						;save non-volatile regs
  2787                                  			push	esi						;
  2788                                  			push	edi						;
  2789                                  			push	ds						;
  2790                                  			push	es						;
  2791                                  			push	ESELCGA						;load CGA video selector ...
  2792                                  			pop	ds						;... into DS
  2793                                  			push	ESELCGA						;load CGA video selector ...
  2794                                  			pop	es						;... into ES
  2795                                  			mov	ecx,ECONROWDWORDS*(ECONROWS-1)			;double-words to move
  2796                                  			mov	esi,ECONROWBYTES				;esi = source (line 2)
  2797                                  			xor	edi,edi						;edi = target (line 1)
  2798                                  			cld							;forward strings
  2799                                  			rep	movsd						;move 24 lines up
  2800                                  			mov	eax,ECONCLEARDWORD				;attribute and ASCII space
  2801                                  			mov	ecx,ECONROWDWORDS				;double-words per row
  2802                                  			rep	stosd						;clear bottom row
  2803                                  			pop	es						;restore non-volatile regs
  2804                                  			pop	ds						;
  2805                                  			pop	edi						;
  2806                                  			pop	esi						;
  2807                                  			pop	ecx						;
  2808                                  			ret							;return
