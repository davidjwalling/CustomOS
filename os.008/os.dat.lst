  2580                                  ;
  2581                                  ;	Routine:	PutConsoleOIAChar
  2582                                  ;
  2583                                  ;	Description:	This routine updates the Operator Information Area (OIA).
  2584                                  ;
  2585                                  ;	In:		DS	OS data selector
  2586                                  ;
  2587                                  ;-----------------------------------------------------------------------------------------------------------------------
  2588                                  PutConsoleOIAChar	push	ebx						;save non-volatile regs
  2589                                  			push	ecx						;
  2590                                  			push	esi						;
  2591                                  			push	ds						;
  2592                                  			push	es						;
  2593                                  			push	ESELDAT						;load OS data selector ...
  2594                                  			pop	ds						;... into data segment register
  2595                                  			push	ESELCGA						;load CGA selector ...
  2596                                  			pop	es						;... into extra segment register
  2597                                  			mov	esi,wbConsoleScan0				;scan codes address
  2598                                  			mov	bh,ECONOIAROW					;OIA row
  2599                                  			mov	bl,0						;starting column
  2600                                  			mov	ecx,6						;maximum scan codes
  2601                                  .10			push	ecx						;save remaining count
  2602                                  			mov	ecx,ebx						;row, column
  2603                                  			lodsb							;read scan code
  2604                                  			or	al,al						;scan code present?
  2605                                  			jz	.20						;no, skip ahead
  2606                                  			call	PutConsoleHexByte				;display scan code
  2607                                  			jmp	.30						;continue
  2608                                  .20			mov	al,' '						;ASCII space
  2609                                  			call	SetConsoleChar					;display space
  2610                                  			inc	cl
  2611                                  			mov	al,' '						;ASCII space
  2612                                  			call	SetConsoleChar					;display space
  2613                                  .30			add	bl,2						;next column (+2)
  2614                                  			pop	ecx						;restore remaining
  2615                                  			loop	.10						;next code
  2616                                  			mov	al,[wbConsoleChar]				;console ASCII character
  2617                                  			cmp	al,32						;printable? (lower-bounds)
  2618                                  			jb	.40						;no, skip ahead
  2619                                  			cmp	al,126						;printable? (upper-bounds)
  2620                                  			ja	.40						;no, skip ahead
  2621                                  			mov	ch,bh						;OIA row
  2622                                  			mov	cl,40						;character display column
  2623                                  			call	SetConsoleChar					;display ASCII character
  2624                                  .40			pop	es						;restore non-volatile regs
  2625                                  			pop	ds						;
  2626                                  			pop	esi						;
  2627                                  			pop	ecx						;
  2628                                  			pop	ebx						;
  2629                                  			ret							;return
