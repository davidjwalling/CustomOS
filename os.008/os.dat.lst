  2293                                  ;	Routine:	PutConsoleString
  2294                                  ;
  2295                                  ;	Description:	This routine writes a sequence of ASCII characters to the console until null and updates the
  2296                                  ;			console position as needed.
  2297                                  ;
  2298                                  ;	In:		EDX	source address
  2299                                  ;			DS	OS data selector
  2300                                  ;
  2301                                  ;-----------------------------------------------------------------------------------------------------------------------
  2302                                  PutConsoleString	push	esi						;save non-volatile regs
  2303                                  			mov	esi,edx						;source address
  2304                                  			cld							;forward strings
  2305                                  .10			lodsb							;ASCII character
  2306                                  			or	al,al						;end of string?
  2307                                  			jz	.40						;yes, skip ahead
  2308                                  			cmp	al,EASCIIRETURN					;carriage return?
  2309                                  			jne	.20						;no, skip ahead
  2310                                  			call	FirstConsoleColumn				;move to start of row
  2311                                  			jmp	.10						;next character
  2312                                  .20			cmp	al,EASCIILINEFEED				;line feed?
  2313                                  			jne	.30						;no, skip ahead
  2314                                  			call	NextConsoleRow					;move to next row
  2315                                  			jmp	.10						;next character
  2316                                  .30			call	PutConsoleChar					;output character to console
  2317                                  			call	NextConsoleColumn				;advance to next column
  2318                                  			jmp	.10						;next character
  2319                                  .40			pop	esi						;restore non-volatile regs
  2320                                  			ret							;return
