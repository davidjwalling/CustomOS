  2416                                  ;
  2417                                  ;	Routine:	PreviousConsoleColumn
  2418                                  ;
  2419                                  ;	Description:	This routine retreats the cursor one logical column. If the cursor was at the start of a row,
  2420                                  ;			the column is set to the last position in the row and the row is decremented.
  2421                                  ;
  2422                                  ;	Input:		DS	OS data selector
  2423                                  ;
  2424                                  ;-----------------------------------------------------------------------------------------------------------------------
  2425                                  PreviousConsoleColumn	mov	al,[wbConsoleColumn]				;current column
  2426                                  			or	al,al						;start of row?
  2427                                  			jnz	.10						;no, skip ahead
  2428                                  			mov	ah,[wbConsoleRow]				;current row
  2429                                  			or	ah,ah						;top of screen?
  2430                                  			jz	.20						;yes, exit with no change
  2431                                  			dec	ah						;decrement row
  2432                                  			mov	[wbConsoleRow],ah				;save row
  2433                                  			mov	al,ECONCOLS					;set maximum column
  2434                                  .10			dec	al						;decrement column
  2435                                  			mov	[wbConsoleColumn],al				;save column
  2436                                  .20			ret							;return
  2437                                  ;-----------------------------------------------------------------------------------------------------------------------
  2438                                  ;
  2439                                  ;	Routine:	NextConsoleColumn
  2440                                  ;
  2441                                  ;	Description:	This routine advances the console position one column. The columnn is reset to zero and the row
  2442                                  ;			incremented if the end of the current row is reached.
  2443                                  ;
  2444                                  ;	In:		DS	OS data selector
  2445                                  ;
  2446                                  ;-----------------------------------------------------------------------------------------------------------------------
  2447                                  NextConsoleColumn	mov	al,[wbConsoleColumn]				;current column
  2448                                  			inc	al						;increment column
  2449                                  			mov	[wbConsoleColumn],al				;save column
  2450                                  			cmp	al,ECONCOLS					;end of row?
  2451                                  			jb	.10						;no, skip ahead
  2452                                  			call	FirstConsoleColumn				;reset column to start of row
  2453                                  			call	NextConsoleRow					;line feed to next row
  2454                                  .10			ret							;return
  2455                                  ;-----------------------------------------------------------------------------------------------------------------------
  2456                                  ;
  2457                                  ;	Routine:	FirstConsoleColumn
  2458                                  ;
  2459                                  ;	Description:	This routine resets the console column to start of the row.
  2460                                  ;
  2461                                  ;	In:		DS	OS data selector
  2462                                  ;
  2463                                  ;-----------------------------------------------------------------------------------------------------------------------
  2464                                  FirstConsoleColumn	xor	al,al						;zero column
  2465                                  			mov	[wbConsoleColumn],al				;save column
  2466                                  			ret							;return
  2467                                  ;-----------------------------------------------------------------------------------------------------------------------
  2468                                  ;
  2469                                  ;	Routine:	NextConsoleRow
  2470                                  ;
  2471                                  ;	Description:	This routine advances the console position one line. Scroll the screen one row if needed.
  2472                                  ;
  2473                                  ;	In:		DS	OS data selector
  2474                                  ;
  2475                                  ;-----------------------------------------------------------------------------------------------------------------------
  2476                                  NextConsoleRow		mov	al,[wbConsoleRow]				;current row
  2477                                  			inc	al						;increment row
  2478                                  			mov	[wbConsoleRow],al				;save row
  2479                                  			cmp	al,ECONROWS					;end of screen?
  2480                                  			jb	.10						;no, skip ahead
  2481                                  			call	ScrollConsoleRow				;scroll up one row
  2482                                  			mov	al,[wbConsoleRow]				;row
  2483                                  			dec	al						;decrement row
  2484                                  			mov	[wbConsoleRow],al				;save row
  2485                                  .10			ret							;return
