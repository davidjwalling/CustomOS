  2862                                  			mov	dl,ECRTPORTLO					;crt controller port lo
  2863                                  			mov	dh,ECRTPORTHI					;crt controller port hi
  2864                                  			mov	al,ECRTCURLOCHI					;crt cursor loc reg hi
  2865                                  			out	dx,al						;select register
  2866                                  			inc	edx						;data port
  2867                                  			mov	al,ch						;hi-order cursor loc
  2868                                  			out	dx,al						;store hi-order loc
  2869                                  			dec	edx						;register select port
  2870                                  			mov	al,ECRTCURLOCLO					;crt cursor loc reg lo
  2871                                  			out	dx,al						;select register
  2872                                  			inc	edx						;data port
  2873                                  			mov	al,cl						;lo-order cursor loc
  2874                                  			out	dx,al						;store lo-order loc
  2875                                  			pop	ecx						;restore non-volatile regs
  2876                                  			ret							;return
