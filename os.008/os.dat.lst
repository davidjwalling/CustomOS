  2669                                  ;-----------------------------------------------------------------------------------------------------------------------
  2670                                  ;
  2671                                  ;	Routine:	PutMessage
  2672                                  ;
  2673                                  ;	Description:	This routine adda a message to the message queue.
  2674                                  ;
  2675                                  ;	Input:		ECX	hi-order data word
  2676                                  ;			EDX	lo-order data word
  2677                                  ;
  2678                                  ;	Output:		CY	0 = success
  2679                                  ;				1 = fail: queue is full
  2680                                  ;
  2681                                  ;-----------------------------------------------------------------------------------------------------------------------
  2682                                  PutMessage		push	ds						;save non-volatile regs
  2683                                  			push	ESELMQ						;load task message queue selector ...
  2684                                  			pop	ds						;... into data segment register
  2685                                  			mov	eax,[MQTail]					;tail ptr
  2686                                  			cmp	dword [eax],0					;is queue full?
  2687                                  			stc							;assume failure
  2688                                  			jne	.20						;yes, cannot store
  2689                                  			mov	[eax],edx					;store lo-order data
  2690                                  			mov	[eax+4],ecx					;store hi-order data
  2691                                  			add	eax,8						;next queue element adr
  2692                                  			and	eax,03fch					;at end of queue?
  2693                                  			jnz	.10						;no, skip ahead
  2694                                  			mov	al,8						;reset to top of queue
  2695                                  .10			mov	[MQTail],eax					;save new tail ptr
  2696                                  			clc							;indicate success
  2697                                  .20			pop	ds						;restore non-volatile regs
  2698                                  			ret							;return
  2699                                  ;-----------------------------------------------------------------------------------------------------------------------
  2700                                  ;
  2701                                  ;	Routine:	GetMessage
  2702                                  ;
  2703                                  ;	Description:	This routine reads and removes a message from the message queue.
  2704                                  ;
  2705                                  ;	Output:		EAX	lo-order message data
  2706                                  ;			EDX	hi-order message data
  2707                                  ;
  2708                                  ;			CY	0 = message read
  2709                                  ;				1 = no message to read
  2710                                  ;
  2711                                  ;-----------------------------------------------------------------------------------------------------------------------
  2712                                  GetMessage		push	ebx						;save non-volatile regs
  2713                                  			push	ecx						;
  2714                                  			push	ds						;
  2715                                  			push	ESELMQ						;load message queue selector ...
  2716                                  			pop	ds						;... into data segment register
  2717                                  			mov	ebx,[MQHead]					;head ptr
  2718                                  			mov	eax,[ebx]					;lo-order 32 bits
  2719                                  			mov	edx,[ebx+4]					;hi-order 32 bits
  2720                                  			or	eax,edx						;is queue empty?
  2721                                  			stc							;assume queue is emtpy
  2722                                  			jz	.20						;yes, skip ahead
  2723                                  			xor	ecx,ecx						;store zero
  2724                                  			mov	[ebx],ecx					;... in lo-order dword
  2725                                  			mov	[ebx+4],ecx					;... in hi-order dword
  2726                                  			add	ebx,8						;next queue element
  2727                                  			and	ebx,03fch					;at end of queue?
  2728                                  			jnz	.10						;no, skip ahead
  2729                                  			mov	bl,8						;reset to 1st entry
  2730                                  .10			mov	[MQHead],ebx					;save new head ptr
  2731                                  			clc							;indicate message read
  2732                                  .20			pop	ds						;restore non-volatile regs
  2733                                  			pop	ecx						;
  2734                                  			pop	ebx						;
  2735                                  			ret							;return
