  2385                                  ;
  2386                                  ;	Routine:	GetConsoleChar
  2387                                  ;
  2388                                  ;	Description:	This routine waits for EMSGKEYCHAR message and return character code.
  2389                                  ;
  2390                                  ;	Output:		AL	ASCII character code
  2391                                  ;			AH	keyboard scan code
  2392                                  ;
  2393                                  ;-----------------------------------------------------------------------------------------------------------------------
  2394                                  GetConsoleChar.10	call	Yield						;pass control or halt
  2395                                  GetConsoleChar		call	GetMessage					;get the next message
  2396                                  			or	eax,eax						;do we have a message?
  2397                                  			jz	GetConsoleChar.10				;no, skip ahead
  2398                                  			push	eax						;save key codes
  2399                                  			and	eax,0FFFF0000h					;mask for message type
  2400                                  			cmp	eax,EMSGKEYCHAR					;key-char message?
  2401                                  			pop	eax						;restore key codes
  2402                                  			jne	GetConsoleChar					;no, try again
  2403                                  			and	eax,0000ffffh					;mask for key codes
  2404                                  			ret							;return
