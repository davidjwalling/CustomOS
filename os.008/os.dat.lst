  2810                                  ;
  2811                                  ;	Routine:	SetConsoleChar
  2812                                  ;
  2813                                  ;	Description:	This routine outputs an ASCII character at the given row and column.
  2814                                  ;
  2815                                  ;	In:		AL	ASCII character
  2816                                  ;			CL	column
  2817                                  ;			CH	row
  2818                                  ;			ES	CGA selector
  2819                                  ;
  2820                                  ;-----------------------------------------------------------------------------------------------------------------------
  2821                                  SetConsoleChar		mov	dl,al						;ASCII character
  2822                                  			movzx	eax,ch						;row
  2823                                  			mov	ah,ECONCOLS					;cols/row
  2824                                  			mul	ah						;row * cols/row
  2825                                  			add	al,cl						;add column
  2826                                  			adc	ah,0						;handle carry
  2827                                  			shl	eax,1						;screen offset
  2828                                  			mov	[es:eax],dl					;store character
  2829                                  			ret							;return
