  2322                                  ;
  2323                                  ;	Routine:	GetConsoleString
  2324                                  ;
  2325                                  ;	Description:	This routine accepts keyboard input into a buffer.
  2326                                  ;
  2327                                  ;	Input:		DS:EDX	target buffer address
  2328                                  ;			ECX	maximum number of characters to accept
  2329                                  ;			BH	echo to terminal
  2330                                  ;			BL	terminating character
  2331                                  ;
  2332                                  ;-----------------------------------------------------------------------------------------------------------------------
  2333                                  GetConsoleString	push	ecx						;save non-volatile regs
  2334                                  			push	esi						;
  2335                                  			push	edi						;
  2336                                  			push	es						;
  2337                                  			push	ds						;load data segment selector ...
  2338                                  			pop	es						;... into extra segment register
  2339                                  			mov	edi,edx						;edi = target buffer
  2340                                  			push	ecx						;save maximum characters
  2341                                  			xor	al,al						;zero register
  2342                                  			cld							;forward strings
  2343                                  			rep	stosb						;zero fill buffer
  2344                                  			pop	ecx						;maximum characters
  2345                                  			mov	edi,edx						;edi = target buffer
  2346                                  			mov	esi,edx						;esi = target buffer
  2347                                  .10			jecxz	.50						;exit if max-length is zero
  2348                                  .20			call	GetConsoleChar					;al = next input char
  2349                                  			cmp	al,bl						;is this the terminator?
  2350                                  			je	.50						;yes, exit
  2351                                  			cmp	al,EASCIIBACKSPACE				;is this a backspace?
  2352                                  			jne	.30						;no, skip ahead
  2353                                  			cmp	esi,edi						;at start of buffer?
  2354                                  			je	.20						;yes, get next character
  2355                                  			dec	edi						;backup target pointer
  2356                                  			mov	byte [edi],0					;zero previous character
  2357                                  			inc	ecx						;increment remaining chars
  2358                                  			test	bh,1						;echo to console?
  2359                                  			jz	.20						;no, get next character
  2360                                  			call	PreviousConsoleColumn				;backup console position
  2361                                  			mov	al,EASCIISPACE					;ASCII space
  2362                                  			call	PutConsoleChar					;write space to console
  2363                                  			call	PlaceCursor					;position the cursor
  2364                                  			jmp	.20						;get next character
  2365                                  .30			cmp	al,EASCIISPACE					;printable? (lower bounds)
  2366                                  			jb	.20						;no, get another character
  2367                                  			cmp	al,EASCIITILDE					;printable? (upper bounds)
  2368                                  			ja	.20						;no, get another character
  2369                                  			stosb							;store character in buffer
  2370                                  			test	bh,1						;echo to console?
  2371                                  			jz	.40						;no, skip ahead
  2372                                  			call	PutConsoleChar					;write character to console
  2373                                  			call	NextConsoleColumn				;advance console position
  2374                                  			call	PlaceCursor					;position the cursor
  2375                                  .40			dec	ecx						;decrement remaining chars
  2376                                  			jmp	.10						;next
  2377                                  .50			xor	al,al						;null
  2378                                  			stosb							;terminate buffer
  2379                                  			pop	es						;restore non-volatile regs
  2380                                  			pop	edi						;
  2381                                  			pop	esi						;
