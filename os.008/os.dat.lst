  2115                                  ;	Routine:	PutDateString
  2116                                  ;
  2117                                  ;	Description:	This routine returns an ASCIIZ mm/dd/yyyy string at ds:edx from the date in the DATETIME
  2118                                  ;			structure at ds:ebx.
  2119                                  ;
  2120                                  ;	In:		DS:EBX	DATETIME address
  2121                                  ;			DS:EDX	output buffer address
  2122                                  ;
  2123                                  ;-----------------------------------------------------------------------------------------------------------------------
  2124                                  PutDateString		push	ecx						;save non-volatile regs
  2125                                  			push	edi						;
  2126                                  			push	es						;
  2127                                  			push	ds						;store data selector ...
  2128                                  			pop	es						;... in extra segment reg
  2129                                  			mov	edi,edx						;output buffer address
  2130                                  			mov	cl,10						;divisor
  2131                                  			mov	edx,002f3030h					;ASCIIZ "00/" (reversed)
  2132                                  			movzx	eax,byte [ebx+DATETIME.month]			;month
  2133                                  			div	cl						;ah = rem; al = quotient
  2134                                  			or	eax,edx						;apply ASCII zones and delimiter
  2135                                  			cld							;forward strings
  2136                                  			stosd							;store "mm/"nul
  2137                                  			dec	edi						;address of terminator
  2138                                  			movzx	eax,byte [ebx+DATETIME.day]			;day
  2139                                  			div	cl						;ah = rem; al = quotient
  2140                                  			or	eax,edx						;apply ASCII zones and delimiter
  2141                                  			stosd							;store "dd/"nul
  2142                                  			dec	edi						;address of terminator
  2143                                  			movzx	eax,byte [ebx+DATETIME.century]			;century
  2144                                  			div	cl						;ah = rem; al = quotient
  2145                                  			or	eax,edx						;apply ASCII zones and delimiter
  2146                                  			stosd							;store "cc/"null
  2147                                  			dec	edi						;address of terminator
  2148                                  			dec	edi						;address of delimiter
  2149                                  			movzx	eax,byte [ebx+DATETIME.year]			;year (yy)
  2150                                  			div	cl						;ah = rem; al = quotient
  2151                                  			or	eax,edx						;apply ASCII zones and delimiter
  2152                                  			stosb							;store quotient
  2153                                  			mov	al,ah						;remainder
  2154                                  			stosb							;store remainder
  2155                                  			xor	al,al						;null terminator
  2156                                  			stosb							;store terminator
  2157                                  			pop	es						;restore non-volatile regs
  2158                                  			pop	edi						;
  2159                                  			pop	ecx						;
  2160                                  			ret							;return
