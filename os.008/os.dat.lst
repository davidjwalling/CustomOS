  2901                                  ;	Routine:	ReadRealTimeClock
  2902                                  ;
  2903                                  ;	Description:	This routine gets current date time from the real-time clock.
  2904                                  ;
  2905                                  ;	In:		DS:EBX	DATETIME structure
  2906                                  ;
  2907                                  ;-----------------------------------------------------------------------------------------------------------------------
  2908                                  ReadRealTimeClock	push	esi						;save non-volatile regs
  2909                                  			push	edi						;
  2910                                  			push	es						;
  2911                                  			push	ds						;store data selector ...
  2912                                  			pop	es						;... in es register
  2913                                  			mov	edi,ebx						;date-time structure
  2914                                  			mov	al,ERTCSECONDREG				;second register
  2915                                  			out	ERTCREGPORT,al					;select second register
  2916                                  			in	al,ERTCDATAPORT					;read second register
  2917                                  			cld							;forward strings
  2918                                  			stosb							;store second value
  2919                                  			mov	al,ERTCMINUTEREG				;minute register
  2920                                  			out	ERTCREGPORT,al					;select minute register
  2921                                  			in	al,ERTCDATAPORT					;read minute register
  2922                                  			stosb							;store minute value
  2923                                  			mov	al,ERTCHOURREG					;hour register
  2924                                  			out	ERTCREGPORT,al					;select hour register
  2925                                  			in	al,ERTCDATAPORT					;read hour register
  2926                                  			stosb							;store hour value
  2927                                  			mov	al,ERTCWEEKDAYREG				;weekday register
  2928                                  			out	ERTCREGPORT,al					;select weekday register
  2929                                  			in	al,ERTCDATAPORT					;read weekday register
  2930                                  			stosb							;store weekday value
  2931                                  			mov	al,ERTCDAYREG					;day register
  2932                                  			out	ERTCREGPORT,al					;select day register
  2933                                  			in	al,ERTCDATAPORT					;read day register
  2934                                  			stosb							;store day value
  2935                                  			mov	al,ERTCMONTHREG					;month register
  2936                                  			out	ERTCREGPORT,al					;select month register
  2937                                  			in	al,ERTCDATAPORT					;read month register
  2938                                  			stosb							;store month value
  2939                                  			mov	al,ERTCYEARREG					;year register
  2940                                  			out	ERTCREGPORT,al					;select year register
  2941                                  			in	al,ERTCDATAPORT					;read year register
  2942                                  			stosb							;store year value
  2943                                  			mov	al,ERTCCENTURYREG				;century register
  2944                                  			out	ERTCREGPORT,al					;select century register
  2945                                  			in	al,ERTCDATAPORT					;read century register
  2946                                  			stosb							;store century value
  2947                                  			mov	al,ERTCSTATUSREG				;status register
  2948                                  			out	ERTCREGPORT,al					;select status register
  2949                                  			in	al,ERTCDATAPORT					;read status register
  2950                                  			test	al,ERTCBINARYVALS				;test if values are binary
  2951                                  			jnz	.20						;skip ahead if binary values
  2952                                  			mov	esi,ebx						;date-time structure address
  2953                                  			mov	edi,ebx						;date-time structure address
  2954                                  			mov	ecx,8						;loop counter
  2955                                  .10			lodsb							;BCD value
  2956                                  			mov	ah,al						;BCD value
  2957                                  			and	al,00001111b					;low-order decimal zone
  2958                                  			and	ah,11110000b					;hi-order decimal zone
  2959                                  			shr	ah,1						;hi-order decimal * 8
  2960                                  			add	al,ah						;low-order + hi-order * 8
  2961                                  			shr	ah,2						;hi-order decimal * 2
  2962                                  			add	al,ah						;low-order + hi-order * 10
  2963                                  			stosb							;replace BCD with binary
  2964                                  			loop	.10						;next value
  2965                                  .20			pop	es						;restore non-volatile regs
  2966                                  			pop	edi						;
  2967                                  			pop	esi						;
  2968                                  			ret							;return
