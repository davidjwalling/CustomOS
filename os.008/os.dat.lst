  2213                                  ;	Routine:	UpperCaseString
  2214                                  ;
  2215                                  ;	Description:	This routine places all characters in the given string to upper case.
  2216                                  ;
  2217                                  ;	In:		DS:EDX	string address
  2218                                  ;
  2219                                  ;	Out:		EDX	string address
  2220                                  ;
  2221                                  ;-----------------------------------------------------------------------------------------------------------------------
  2222                                  UpperCaseString		push	esi						;save non-volatile regs
  2223                                  			mov	esi,edx						;string address
  2224                                  			cld							;forward strings
  2225                                  .10			lodsb							;string character
  2226                                  			test	al,al						;null?
  2227                                  			jz	.20						;yes, skip ahead
  2228                                  			cmp	al,EASCIILOWERA					;lower-case? (lower bounds)
  2229                                  			jb	.10						;no, continue
  2230                                  			cmp	al,EASCIILOWERZ					;lower-case? (upper bounds)
  2231                                  			ja	.10						;no, continue
  2232                                  			and	al,EASCIICASEMASK				;mask for upper case
  2233                                  			mov	[esi-1],al					;upper character
  2234                                  			jmp	.10						;continue
  2235                                  .20			pop	esi						;restore non-volatile regs
  2236                                  			ret							;return
