  2984                                  ;
  2985                                  ;	Routine:	SetKeyboardLamps
  2986                                  ;
  2987                                  ;	Description:	This routine sends the set/reset mode indicators command to the keyboard device.
  2988                                  ;
  2989                                  ;	In:		BH	00000CNS (C:Caps Lock,N:Num Lock,S:Scroll Lock)
  2990                                  ;
  2991                                  ;-----------------------------------------------------------------------------------------------------------------------
  2992                                  SetKeyboardLamps	call	WaitForKeyInBuffer				;wait for input buffer ready
  2993                                  			mov	al,EKEYCMDLAMPS					;set/reset lamps command
  2994                                  			out	EKEYPORTDATA,al					;send command to 8042
  2995                                  			call	WaitForKeyOutBuffer				;wait for 8042 result
  2996                                  			in	al,EKEYPORTDATA					;read 8042 'ACK' (0fah)
  2997                                  			call	WaitForKeyInBuffer				;wait for input buffer ready
  2998                                  			mov	al,bh						;set/reset lamps value
  2999                                  			out	EKEYPORTDATA,al					;send lamps value
  3000                                  			call	WaitForKeyOutBuffer				;wait for 8042 result
  3001                                  			in	al,EKEYPORTDATA					;read 8042 'ACK' (0fah)
  3002                                  			ret							;return
