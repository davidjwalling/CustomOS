  2517                                  PutConsoleOIAShift	push	ecx						;save non-volatile regs
  2518                                  			push	es						;
  2519                                  			push	ESELCGA						;load CGA selector ...
  2520                                  			pop	es						;... into ES register
  2521                                  			mov	ch,ECONOIAROW					;OIA row
  2522                                  			mov	al,EASCIISPACE					;space is default character
  2523                                  			test	bl,EKEYSHIFTLEFT				;left-shift indicated?
  2524                                  			jz	.10						;no, skip ahead
  2525                                  			mov	al,'S'						;yes, indicate with 'S'
  2526                                  .10			mov	cl,14						;indicator column
  2527                                  			call	SetConsoleChar					;display ASCII character
  2528                                  			mov	al,EASCIISPACE					;ASCII space
  2529                                  			test	bl,EKEYSHIFTRIGHT				;right-shift indicated?
  2530                                  			jz	.20						;no, skip ahead
  2531                                  			mov	al,'S'						;yes, indicate with 'S'
  2532                                  .20			mov	cl,64						;indicator column
  2533                                  			call	SetConsoleChar					;display ASCII character
  2534                                  			mov	al,EASCIISPACE					;ASCII space
  2535                                  			test	bl,EKEYCTRLLEFT					;left-ctrl indicated?
  2536                                  			jz	.30						;no, skip ahead
  2537                                  			mov	al,'C'						;yes, indicate with 'C'
  2538                                  .30			mov	cl,15						;indicator column
  2539                                  			call	SetConsoleChar					;display ASCII character
  2540                                  			mov	al,EASCIISPACE					;ASCII space
  2541                                  			test	bl,EKEYCTRLRIGHT				;right-ctrl indicated?
  2542                                  			jz	.40						;no, skip ahead
  2543                                  			mov	al,'C'						;yes, indicate with 'C'
  2544                                  .40			mov	cl,63						;indicator column
  2545                                  			call	SetConsoleChar					;display ASCII character
  2546                                  			mov	al,EASCIISPACE					;ASCII space
  2547                                  			test	bl,EKEYALTLEFT					;left-alt indicated?
  2548                                  			jz	.50						;no, skip ahead
  2549                                  			mov	al,'A'						;yes, indicate with 'A'
  2550                                  .50			mov	cl,16						;indicator column
  2551                                  			call	SetConsoleChar					;display ASCII character
  2552                                  			mov	al,EASCIISPACE					;ASCII space
  2553                                  			test	bl,EKEYALTRIGHT					;right-alt indicated?
  2554                                  			jz	.60						;no, skip ahead
  2555                                  			mov	al,'A'						;yes, indicate with 'A'
  2556                                  .60			mov	cl,62						;indicator column
  2557                                  			call	SetConsoleChar					;display ASCII character
  2558                                  			mov	al,EASCIISPACE					;ASCII space
  2559                                  			test	bh,EKEYLOCKCAPS					;caps-lock indicated?
  2560                                  			jz	.70						;no, skip ahead
  2561                                  			mov	al,'C'						;yes, indicate with 'C'
  2562                                  .70			mov	cl,78						;indicator column
  2563                                  			call	SetConsoleChar					;display ASCII character
  2564                                  			mov	al,EASCIISPACE					;ASCII space
  2565                                  			test	bh,EKEYLOCKNUM					;num-lock indicated?
  2566                                  			jz	.80						;no, skip ahead
  2567                                  			mov	al,'N'						;yes, indicate with 'N'
  2568                                  .80			mov	cl,77						;indicator column
  2569                                  			call	SetConsoleChar					;display ASCII character
  2570                                  			mov	al,EASCIISPACE					;ASCII space
  2571                                  			test	bh,EKEYLOCKSCROLL				;scroll-lock indicated?
  2572                                  			jz	.90						;no, skip ahead
  2573                                  			mov	al,'S'						;yes, indicate with 'S'
  2574                                  .90			mov	cl,76						;indicator column
  2575                                  			call	SetConsoleChar					;display ASCII character
  2576                                  			pop	es						;restore non-volatile regs
  2577                                  			pop	ecx						;
  2578                                  			ret							;return
