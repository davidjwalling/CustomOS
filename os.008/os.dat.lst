  1592                                  ;
  1593                                  ;	Hardware Device Interupts
  1594                                  ;
  1595                                  ;	The next 16 interrupts are defined as our hardware interrupts. These interrupts vectors (20h-2Fh) are mapped to
  1596                                  ;	the hardware interrupts IRQ0-IRQF by the BIOS when the call to the BIOS is made invoking BIOS function 89h
  1597                                  ;	(BX=2028h).
  1598                                  ;
  1599                                  ;-----------------------------------------------------------------------------------------------------------------------
  1600                                  ;-----------------------------------------------------------------------------------------------------------------------
  1601                                  ;
  1602                                  ;	IRQ0	Clock Tick Interrupt
  1603                                  ;
  1604                                  ;	PC compatible systems contain or emulate the function of the Intel 8253 Programmable Interval Timer (PIT).
  1605                                  ;	Channel 0 of this chip decrements an internal counter to zero and then issues a hardware interrupt. The default
  1606                                  ;	rate at which IRQ0 occurs is approximately 18.2 times per second or, more accurately, 1,573,040 times per day.
  1607                                  ;
  1608                                  ;	Every time IRQ0 occurs, a counter at 40:6c is incremented. When the number of ticks reaches the maximum for one
  1609                                  ;	day, the counter is set to zero and the number of days counter at 40:70 is incremented.
  1610                                  ;
  1611                                  ;	This handler also decrements the floppy drive motor count at 40:40 if it is not zero. When this count reaches
  1612                                  ;	zero, the floppy disk motors are turned off.
  1613                                  ;
  1614                                  ;-----------------------------------------------------------------------------------------------------------------------
  1615                                  			menter	clocktick					;clock tick interrupt
  1616                                  			push	eax						;save modified regs
  1617                                  			push	edx						;
  1618                                  			push	ds						;
  1619                                  			push	ESELDAT						;load OS data selector ...
  1620                                  			pop	ds						;... into data segment register
  1621                                  			mov	eax,[wfClockTicks]				;eax = clock ticks
  1622                                  			inc	eax						;increment clock ticks
  1623                                  			cmp	eax,EPITDAYTICKS				;clock ticks per day?
  1624                                  			jb	irq0.10						;no, skip ahead
  1625                                  			inc	byte [wbClockDays]				;increment clock days
  1626                                  			xor	eax,eax						;reset clock ticks
  1627                                  irq0.10			mov	dword [wfClockTicks],eax			;save clock ticks
  1628                                  			cmp	byte [wbFDCMotor],0				;floppy motor timeout?
  1629                                  			je	irq0.20						;yes, skip ahead
  1630                                  			dec	byte [wbFDCMotor]				;decrement motor timeout
  1631                                  			jnz	irq0.20						;skip ahead if non-zero
  1632                                  			sti							;enable maskable interrupts
  1633                                  irq0.15 		mov	dh,EFDCPORTHI					;FDC controller port hi
  1634                                  			mov	dl,EFDCPORTLOSTAT				;FDC main status register
  1635                                  			in	al,dx						;FDC main status byte
  1636                                  			test	al,EFDCSTATBUSY					;test FDC main status for busy
  1637                                  			jnz	irq0.15						;wait while busy
  1638                                  			mov	al,EFDCMOTOROFF					;motor-off / enable/ DMA setting
  1639                                  			mov	byte [wbFDCControl],al				;save motor-off setting
  1640                                  			mov	dh,EFDCPORTHI					;fdc port hi
  1641                                  			mov	dl,EFDCPORTLOOUT				;fdc digital output register
  1642                                  			out	dx,al						;turn motor off
  1643                                  irq0.20			call	PutPrimaryEndOfInt				;send end-of-interrupt to PIC
  1644                                  			pop	ds						;restore modified regs
  1645                                  			pop	edx						;
  1646                                  			pop	eax						;
  1647                                  			iretd							;return
  1648                                  ;-----------------------------------------------------------------------------------------------------------------------
  1649                                  ;
  1650                                  ;	IRQ1	Keyboard Interrupt
  1651                                  ;
  1652                                  ;	This handler is called when an IRQ1 hardware interrupt occurs, caused by a keyboard event. The scan-code(s)
  1653                                  ;	corresponding to the keyboard event are read and message events are appended to the message queue. Since this
  1654                                  ;	code is called in response to a hardware interrupt, no task switch occurs. We need to preseve the state of
  1655                                  ;	ALL modified registers upon return.
  1656                                  ;
  1657                                  ;-----------------------------------------------------------------------------------------------------------------------
  1658                                  			menter	keyboard					;keyboard interrrupt
  1659                                  			push	eax						;save non-volatile regs
  1660                                  			push	ebx						;
  1661                                  			push	ecx						;
  1662                                  			push	esi						;
  1663                                  			push	ds						;
  1664                                  			push	ESELDAT						;load OS data selector ...
  1665                                  			pop	ds						;... into data segment register
  1666                                  			xor	al,al						;zero
  1667                                  			mov	[wbConsoleScan0],al				;clear scan code 0
  1668                                  			mov	[wbConsoleScan1],al				;clear scan code 1
  1669                                  			mov	[wbConsoleScan2],al				;clear scan code 2
  1670                                  			mov	[wbConsoleScan3],al				;clear scan code 3
  1671                                  			mov	[wbConsoleScan4],al				;clear scan code 4
  1672                                  			mov	[wbConsoleScan5],al				;clear scan code 5
  1673                                  			mov	al,' '						;space
  1674                                  			mov	[wbConsoleChar],al				;set character to space
  1675                                  			mov	al,EKEYTIMEOUT					;controller timeout flag
  1676                                  			not	al						;controller timeout mask
  1677                                  			and	[wbConsoleStatus],al				;clear controller timeout flag
  1678                                  			mov	bl,[wbConsoleShift]				;shift flags
  1679                                  			mov	bh,[wbConsoleLock]				;lock flags
  1680                                  			call	WaitForKeyOutBuffer				;controller timeout?
  1681                                  			jz	irq1.140					;yes, skip ahead
  1682                                  			in	al,EKEYPORTDATA					;read scan code 0
  1683                                  			mov	[wbConsoleScan0],al				;save scan code 0
  1684                                  			mov	ah,al						;copy scan code 0
  1685                                  			mov	al,EKEYSHIFTLEFT				;left shift flag
  1686                                  			cmp	ah,EKEYSHIFTLDOWN				;left shift key down code?
  1687                                  			je	irq1.30						;yes, set flag
  1688                                  			cmp	ah,EKEYSHIFTLUP					;left shift key up code?
  1689                                  			je	irq1.40						;yes, reset flag
  1690                                  			mov	al,EKEYSHIFTRIGHT				;right shift flag
  1691                                  			cmp	ah,EKEYSHIFTRDOWN				;right shift key down code?
  1692                                  			je	irq1.30						;yes, set flag
  1693                                  			cmp	ah,EKEYSHIFTRUP					;right shift key up code?
  1694                                  			je	irq1.40						;yes, reset flag
  1695                                  			mov	al,EKEYCTRLLEFT					;left control flag
  1696                                  			cmp	ah,EKEYCTRLDOWN					;control key down code?
  1697                                  			je	irq1.30						;yes, set flag
  1698                                  			cmp	ah,EKEYCTRLUP					;control key up code?
  1699                                  			je	irq1.40						;yes, reset flag
  1700                                  			mov	al,EKEYALTLEFT					;left alt flag
  1701                                  			cmp	ah,EKEYALTDOWN					;alt key down code?
  1702                                  			je	irq1.30						;yes, set flag
  1703                                  			cmp	ah,EKEYALTUP					;alt key up code?
  1704                                  			je	irq1.40						;yes, reset flag
  1705                                  			mov	al,EKEYLOCKCAPS					;caps-lock flag
  1706                                  			cmp	ah,EKEYCAPSDOWN					;caps-lock key down code?
  1707                                  			je	irq1.50						;yes, toggle lamps and flags
