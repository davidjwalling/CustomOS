  2239                                  ;	Routine:	CompareMemory
  2240                                  ;
  2241                                  ;	Description:	This routine compares two byte arrays.
  2242                                  ;
  2243                                  ;	In:		DS:EDX	first source address
  2244                                  ;			DS:EBX	second source address
  2245                                  ;			ECX	comparison length
  2246                                  ;
  2247                                  ;	Out:		EDX	first source address
  2248                                  ;			EBX	second source address
  2249                                  ;			ECX	0	array 1 = array 2
  2250                                  ;				<0	array 1 < array 2
  2251                                  ;				>0	array 1 > array 2
  2252                                  ;
  2253                                  ;-----------------------------------------------------------------------------------------------------------------------
  2254                                  CompareMemory		push	esi						;save non-volatile regs
  2255                                  			push	edi						;
  2256                                  			push	es						;
  2257                                  			push	ds						;copy DS
  2258                                  			pop	es						;... to ES
  2259                                  			mov	esi,edx						;first source address
  2260                                  			mov	edi,ebx						;second source address
  2261                                  			cld							;forward strings
  2262                                  			rep	cmpsb						;compare bytes
  2263                                  			mov	al,0						;default result
  2264                                  			jz	.10						;branch if arrays equal
  2265                                  			mov	al,1						;positive result
  2266                                  			jnc	.10						;branch if target > source
  2267                                  			mov	al,-1						;negative result
  2268                                  .10			movsx	ecx,al						;extend sign
  2269                                  			pop	es						;restore regs
  2270                                  			pop	edi						;
  2271                                  			pop	esi						;
  2272                                  			ret							;return
