  2848                                  ;	Routine:	PlaceCursor
  2849                                  ;
  2850                                  ;	Description:	This routine positions the cursor on the console.
  2851                                  ;
  2852                                  ;	In:		DS	OS data selector
  2853                                  ;
  2854                                  ;-----------------------------------------------------------------------------------------------------------------------
  2855                                  PlaceCursor		push	ecx						;save non-volatile regs
  2856                                  			mov	al,[wbConsoleRow]				;al = row
  2857                                  			mov	ah,ECONCOLS					;ah = cols/row
  2858                                  			mul	ah						;row offset
  2859                                  			add	al,[wbConsoleColumn]				;add column
  2860                                  			adc	ah,0						;add overflow
