  2631                                  ;
  2632                                  ;	Routine:	PutConsoleHexByte
  2633                                  ;
  2634                                  ;	Description:	This routine writes two ASCII characters to the console screen representing the value of a byte.
  2635                                  ;
  2636                                  ;	In:		AL	byte value
  2637                                  ;			CH	row
  2638                                  ;			CL	column
  2639                                  ;			DS	OS data selector
  2640                                  ;			ES	CGA selector
  2641                                  ;
  2642                                  ;-----------------------------------------------------------------------------------------------------------------------
  2643                                  PutConsoleHexByte	push	ebx						;save non-volatile regs
  2644                                  			mov	bl,al						;save byte value
  2645                                  			shr	al,4						;hi-order nybble
  2646                                  			or	al,030h						;apply ASCII zone
  2647                                  			cmp	al,03ah						;numeric?
  2648                                  			jb	.10						;yes, skip ahead
  2649                                  			add	al,7						;add ASCII offset for alpha
  2650                                  .10			call	SetConsoleChar					;display ASCII character
  2651                                  			inc	cl						;increment column
  2652                                  			mov	al,bl						;byte value
  2653                                  			and	al,0fh						;lo-order nybble
  2654                                  			or	al,30h						;apply ASCII zone
  2655                                  			cmp	al,03ah						;numeric?
  2656                                  			jb	.20						;yes, skip ahead
  2657                                  			add	al,7						;add ASCII offset for alpha
  2658                                  .20			call	SetConsoleChar					;display ASCII character
  2659                                  			pop	ebx						;restore non-volatile regs
  2660                                  			ret							;return
