  2970                                  ;
  2971                                  ;	Routine:	ResetSystem
  2972                                  ;
  2973                                  ;	Description:	This routine restarts the system using the 8042 controller.
  2974                                  ;
  2975                                  ;-----------------------------------------------------------------------------------------------------------------------
  2976                                  ResetSystem		mov	ecx,001fffffh					;delay to clear ints
  2977                                  			loop	$						;clear interrupts
  2978                                  			mov	al,EKEYCMDRESET					;mask out bit zero
  2979                                  			out	EKEYPORTSTAT,al					;drive bit zero low
  2980                                  .10			sti							;enable maskable interrupts
  2981                                  			hlt							;halt until interrupt
  2982                                  			jmp	.10						;repeat until reset kicks in
