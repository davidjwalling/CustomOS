  3004                                  ;
  3005                                  ;	Routine:	WaitForKeyInBuffer
  3006                                  ;
  3007                                  ;	Description:	This routine waits for keyboard input buffer to be ready for input.
  3008                                  ;
  3009                                  ;	Out:		ZF	1 = Input buffer ready
  3010                                  ;				0 = Input buffer not ready after timeout
  3011                                  ;
  3012                                  ;-----------------------------------------------------------------------------------------------------------------------
  3013                                  WaitForKeyInBuffer	push	ecx						;save non-volatile regs
  3014                                  			mov	ecx,EKEYWAITLOOP				;keyboard controller timeout
  3015                                  .10			in	al,EKEYPORTSTAT					;keyboard status byte
  3016                                  			test	al,EKEYBITIN					;is input buffer still full?
  3017                                  			loopnz	.10						;yes, repeat till timeout
  3018                                  			pop	ecx						;restore non-volatile regs
  3019                                  			ret							;return
  3020                                  ;-----------------------------------------------------------------------------------------------------------------------
  3021                                  ;
  3022                                  ;	Routine:	WaitForKeyOutBuffer
  3023                                  ;
  3024                                  ;	Description:	This routine waits for keyboard output buffer to have data to read.
  3025                                  ;
  3026                                  ;	Out:		ZF	0 = Output buffer has data from controller
  3027                                  ;				1 = Output buffer empty after timeout
  3028                                  ;
  3029                                  ;-----------------------------------------------------------------------------------------------------------------------
  3030                                  WaitForKeyOutBuffer	push	ecx						;save non-volatile regs
  3031                                  			mov	ecx,EKEYWAITLOOP				;keyboard controller timeout
  3032                                  .10			in	al,EKEYPORTSTAT					;keyboard status byte
  3033                                  			test	al,EKEYBITOUT					;output buffer status bit
  3034                                  			loopz	.10						;loop until output buffer bit
  3035                                  			pop	ecx						;restore non-volatile regs
  3036                                  			ret							;return
