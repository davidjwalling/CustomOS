  3039                                  ;	End of the Kernel Function Library
  3040                                  ;
  3041                                  ;-----------------------------------------------------------------------------------------------------------------------
  3042                                  			times	8190-($-$$) db 0h				;zero fill to end of section
  3043                                  			db	055h,0AAh					;end of segment
