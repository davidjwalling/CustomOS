  3119 000009C4 750D                                            jnz     .no                                             ;no, branch
  3120 000009C6 B264                                            mov     dl,100                                          ;divisor
  3121 000009C8 F6F2                                            div     dl                                              ;divide by 100
  3122 000009CA 84E4                                            test    ah,ah                                           ;multiple of 100?
  3123 000009CC 7504                                            jnz     .yes                                            ;yes, branch
  3124 000009CE A803                                            test    al,00000011b                                    ;multiple of 400?
  3125 000009D0 7501                                            jnz     .no                                             ;no, branch
  3126 000009D2 41                      .yes                    inc     ecx                                             ;indicate leap
  3127 000009D3 C3                      .no                     ret                                                     ;return
