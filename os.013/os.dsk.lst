  1335                                  ;
  1336                                  ;       Diskette Directory                                                      @disk: 002600   @mem: n/a
  1337                                  ;
  1338                                  ;       The disk contains one copy of the diskette directory. Each directory entry is 32 bytes long. Our directory
  1339                                  ;       contains only one entry. Unused entries are set to all nulls. The directory immediately follows the second FAT
  1340                                  ;       copy.
  1341                                  ;
  1342                                  ;-----------------------------------------------------------------------------------------------------------------------
  1343                                  section                 dir                                                     ;diskette directory
  1344 00000000 4F5320202020202043-                             db      "OS      COM"                                   ;file name (must contain spaces)
  1344 00000009 4F4D               
  1345 0000000B 20                                              db      020h                                            ;attribute (archive bit set)
  1346 0000000C 00<rept>                                        times   10 db 0                                         ;unused
  1347 00000016 0000                                            dw      0h                                              ;time
  1348 00000018 41                                              db      01000001b                                       ;mmm = 10 MOD 8 = 2; ddddd = 1
  1349 00000019 49                                              db      01001001b                                       ;yyyyyyy = 2016-1980 = 36 = 24h; m/8 = 1
  1350 0000001A 0200                                            dw      2                                               ;first cluster
  1351 0000001C 00540000                                        dd      05400h                                          ;file size
  1352 00000020 00<rept>                                        times   (EBOOTDIRENTRIES*32)-($-$$) db 0h               ;zero fill to end of section
  1353                                  %endif
