  3104 000009AF 6BC964                                          imul    ecx,100                                         ;century * 100
  3105 000009B2 0FB64306                                        movzx   eax,byte [ebx+DATETIME.year]                    ;year of century
  3106 000009B6 01C1                                            add     ecx,eax                                         ;year (YYYY)
