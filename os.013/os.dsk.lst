  3121 000009BE 89C8                                            mov     eax,ecx                                         ;EAX = YYYY
  3122 000009C0 31C9                                            xor     ecx,ecx                                         ;assume not leap year
  3123 000009C2 A803                                            test    al,00000011b                                    ;multiple of four?
  3124 000009C4 750D                                            jnz     .no                                             ;no, branch
  3125 000009C6 B264                                            mov     dl,100                                          ;divisor
  3126 000009C8 F6F2                                            div     dl                                              ;divide by 100
  3127 000009CA 84E4                                            test    ah,ah                                           ;multiple of 100?
  3128 000009CC 7504                                            jnz     .yes                                            ;yes, branch
  3129 000009CE A803                                            test    al,00000011b                                    ;multiple of 400?
  3130 000009D0 7501                                            jnz     .no                                             ;no, branch
  3131 000009D2 41                      .yes                    inc     ecx                                             ;indicate leap
  3132 000009D3 C3                      .no                     ret                                                     ;return
