  1560 000000B1 AC                      .10                     lodsb                                                   ;load next byte at DS:SI in AL
  1561 000000B2 84C0                                            test    al,al                                           ;end of string?
  1562 000000B4 7406                                            jz      .20                                             ;... yes, exit our loop
  1563 000000B6 B40E                                            mov     ah,EBIOSFNTTYOUTPUT                             ;BIOS teletype function
  1564 000000B8 CD10                                            int     EBIOSINTVIDEO                                   ;call BIOS display interrupt
  1565 000000BA EBF5                                            jmp     .10                                             ;repeat until done
  1566 000000BC C3                      .20                     ret                                                     ;return
